{
  AbstractInsnNode next=getNextInstruction(node);
  if (next != null && next.getOpcode() == Opcodes.INVOKEVIRTUAL && CLASS_CONSTRUCTOR.equals(method.name) && ORDINAL_METHOD.equals(((MethodInsnNode)next).name) && classNode.outerClass != null && isEnumSwitchInitializer(classNode)) {
    LintDriver driver=context.getDriver();
    ClassNode outer=driver.getOuterClassNode(classNode);
    if (outer != null) {
      MethodNode switchUser=findEnumSwitchUsage(outer,owner);
      if (switchUser != null) {
        if (driver.isSuppressed(UNSUPPORTED,outer,switchUser,null)) {
          return true;
        }
        if (getLocalMinSdk(switchUser.invisibleAnnotations) >= api || getLocalMinSdk(outer.invisibleAnnotations) >= api) {
          return true;
        }
      }
    }
  }
  return false;
}
