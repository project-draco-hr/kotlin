{
  String tag=element.getTagName();
  if (tag.equals(SdkConstants.VIEW_TAG)) {
    tag=element.getAttribute(ATTR_CLASS);
    if (tag == null || tag.isEmpty()) {
      return false;
    }
  }
  if (tag.indexOf('.') != -1) {
    return true;
  }
  return tag.contains("Text") || tag.contains(BUTTON) || tag.equals("DigitalClock")|| tag.equals("Chronometer")|| tag.equals(CHECK_BOX)|| tag.equals(SWITCH);
}
