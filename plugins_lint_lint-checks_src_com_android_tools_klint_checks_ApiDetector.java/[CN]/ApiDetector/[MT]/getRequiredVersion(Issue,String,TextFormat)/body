{
  errorMessage=format.toText(errorMessage);
  if (issue == UNSUPPORTED || issue == INLINED) {
    Pattern pattern=Pattern.compile("\\s(\\d+)\\s");
    Matcher matcher=pattern.matcher(errorMessage);
    if (matcher.find()) {
      return Integer.parseInt(matcher.group(1));
    }
  }
  return -1;
}
