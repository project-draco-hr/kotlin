{
  if (context.getResourceFolderType() == ResourceFolderType.DRAWABLE && attributeApiLevel == 21) {
    String root=attribute.getOwnerDocument().getDocumentElement().getTagName();
    if (TAG_RIPPLE.equals(root) || TAG_VECTOR.equals(root) || TAG_ANIMATED_VECTOR.equals(root)|| TAG_ANIMATED_SELECTOR.equals(root)) {
      return true;
    }
  }
  return false;
}
