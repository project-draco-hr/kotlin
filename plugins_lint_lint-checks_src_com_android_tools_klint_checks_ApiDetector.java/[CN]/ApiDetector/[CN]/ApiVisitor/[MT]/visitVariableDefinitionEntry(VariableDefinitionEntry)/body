{
  if (mCurrentMethod != null) {
    if (mLocalVars == null) {
      mLocalVars=Sets.newHashSet();
    }
    mLocalVars.add(node.astName().astValue());
  }
 else {
    if (mFields == null) {
      mFields=Sets.newHashSet();
    }
    mFields.add(node.astName().astValue());
  }
  return super.visitVariableDefinitionEntry(node);
}
