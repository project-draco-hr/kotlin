{
  JetScriptInfo scriptInfo=(JetScriptInfo)declarationProvider.getOwnerInfo();
  ScriptDescriptor scriptDescriptor=resolveSession.getScriptDescriptor(scriptInfo.getScript());
  ConstructorDescriptorImpl constructor=ScriptDescriptorImpl.createConstructor(scriptDescriptor,scriptDescriptor.getScriptCodeDescriptor().getValueParameters());
  setDeferredReturnType(constructor);
  return constructor;
}
