{
  String mainFunFqName=packageFqName.child(Name.identifier("main")).asString();
  Collection<JetNamedFunction> mainFunctions=JetTopLevelFunctionsFqnNameIndex.getInstance().get(mainFunFqName,module.getProject(),module.getModuleRuntimeScope(true));
  for (  JetNamedFunction function : mainFunctions) {
    if (JetMainDetector.isMain(function)) {
      return function;
    }
  }
  return null;
}
