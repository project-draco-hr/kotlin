{
  JetType leftType=getType(scope,binaryExpression.getLeft(),false,context);
  FunctionDescriptor functionDescriptor=context.services.callResolver.resolveCallWithGivenName(scope,CallMaker.makeCall(binaryExpression),binaryExpression.getOperationReference(),name,leftType,context.expectedType);
  if (functionDescriptor != null) {
    return functionDescriptor.getReturnType();
  }
  return null;
}
