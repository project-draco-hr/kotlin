{
  if (!(position.getFile() instanceof JetFile)) {
    throw new NoDataException();
  }
  try {
    int line=position.getLine() + 1;
    List<Location> locations=myDebugProcess.getVirtualMachineProxy().versionHigher("1.4") ? type.locationsOfLine(DebugProcess.JAVA_STRATUM,null,line) : type.locationsOfLine(line);
    if (locations == null || locations.isEmpty())     throw new NoDataException();
    return locations;
  }
 catch (  AbsentInformationException e) {
    throw new NoDataException();
  }
}
