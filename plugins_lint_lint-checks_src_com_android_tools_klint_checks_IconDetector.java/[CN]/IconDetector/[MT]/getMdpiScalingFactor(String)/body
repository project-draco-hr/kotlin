{
  if (folderName.contains("-mdpi")) {
    return 1.0f;
  }
 else   if (folderName.contains("-hdpi")) {
    return 1.5f;
  }
 else   if (folderName.contains("-xhdpi")) {
    return 2.0f;
  }
 else   if (folderName.contains("-xxhdpi")) {
    return 3.0f;
  }
 else   if (folderName.contains("-xxxhdpi")) {
    return 4.0f;
  }
 else   if (folderName.contains("-ldpi")) {
    return 0.75f;
  }
 else {
    return 0f;
  }
}
