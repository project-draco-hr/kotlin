{
  SignaturesPropagationData data=new SignaturesPropagationData(owner,returnType,receiverType,valueParameters,typeParameters,method);
  return new AlternativeMethodSignature(data.getModifiedReturnType(),data.getModifiedReceiverType(),data.getModifiedValueParameters(),data.getModifiedTypeParameters(),data.getSignatureErrors(),data.getModifiedHasStableParameterNames());
}
