{
  int c=in.read();
  if (!Character.isJavaIdentifierStart((char)c)) {
    in.unread();
    reportSyntaxError("msg.jsni.expected.identifier",null);
    return false;
  }
  addToString(c);
  for (; ; ) {
    c=in.read();
    if (Character.isJavaIdentifierPart((char)c)) {
      addToString(c);
    }
 else     if (c == '(') {
      addToString(c);
      if (jsniMatchParamListSignature()) {
        return true;
      }
 else {
        return false;
      }
    }
 else {
      in.unread();
      return true;
    }
  }
}
