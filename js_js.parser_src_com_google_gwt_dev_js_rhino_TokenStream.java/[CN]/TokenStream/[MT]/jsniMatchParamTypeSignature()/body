{
  int c=in.read();
switch (c) {
case 'Z':
case 'B':
case 'C':
case 'S':
case 'I':
case 'J':
case 'F':
case 'D':
    addToString(c);
  return true;
case 'L':
addToString(c);
return jsniMatchQualifiedTypeName('/',';');
case '[':
addToString(c);
return jsniMatchParamArrayTypeSignature();
default :
in.unread();
reportSyntaxError("msg.jsni.expected.param.type",null);
return false;
}
}
