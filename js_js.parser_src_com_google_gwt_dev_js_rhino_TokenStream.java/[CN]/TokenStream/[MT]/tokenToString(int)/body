{
  if (Context.printTrees) {
    String name=tokenToName(token);
switch (token) {
case UNARYOP:
case ASSIGN:
case PRIMARY:
case EQOP:
case SHOP:
case RELOP:
      return name + " " + tokenToName(this.op);
case STRING:
case REGEXP:
case NAME:
    return name + " `" + this.string+ "'";
case NUMBER:
  return "NUMBER " + this.number;
}
return name;
}
return "";
}
