{
  new InjectionLogicGenerator(){
    @Override public String prefixForPostConstructorCall(    Field field){
      return "";
    }
    @Override public String prefixForSetterCall(    Field field){
      return field.isPublic() ? "this." : "";
    }
    @Override public String prefixForInitialization(    Field field){
      return "this.";
    }
  }
.generate(p,fields);
}
