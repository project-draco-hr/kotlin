{
  for (int i=0; i < type.getArrayDimension(); i++) {
    sb.append('[');
  }
  if (type.hasPrimitiveType()) {
    sb.append(PRIMITIVE_TYPES[type.getPrimitiveType().ordinal()]);
  }
 else {
    sb.append("L");
    sb.append(fqNameToInternalName(nameResolver.getFqName(type.getClassFqName())));
    sb.append(";");
  }
  return sb;
}
