{
  if (element == null) {
    return null;
  }
  if (strict) {
    element=element.getParent();
  }
  while (element != null && !clz.isInstance(element)) {
    for (    Class<?> terminator : terminators) {
      if (terminator.isInstance(element)) {
        return null;
      }
    }
    element=element.getParent();
  }
  return (T)element;
}
