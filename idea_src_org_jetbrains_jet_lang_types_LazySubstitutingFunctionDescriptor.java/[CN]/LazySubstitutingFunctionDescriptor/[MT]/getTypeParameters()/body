{
  List<TypeParameterDescriptor> result=new ArrayList<TypeParameterDescriptor>();
  for (  TypeParameterDescriptor parameterDescriptor : functionDescriptor.getTypeParameters()) {
    result.add(new TypeParameterDescriptor(ResolveUtil.getJetTypeParameter(parameterDescriptor),parameterDescriptor.getAttributes(),parameterDescriptor.getVariance(),parameterDescriptor.getName(),TypeSubstitutor.INSTANCE.substituteInSet(substitutionContext,parameterDescriptor.getUpperBounds(),Variance.INVARIANT)));
  }
  return result;
}
