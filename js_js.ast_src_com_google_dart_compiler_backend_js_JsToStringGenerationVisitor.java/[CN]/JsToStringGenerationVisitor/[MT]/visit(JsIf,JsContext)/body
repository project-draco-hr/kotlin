{
  _if();
  _spaceOpt();
  _lparen();
  accept(x.getIfExpr());
  _rparen();
  JsStatement thenStmt=x.getThenStmt();
  _nestedPush(thenStmt,false);
  accept(thenStmt);
  _nestedPop(thenStmt);
  JsStatement elseStmt=x.getElseStmt();
  if (elseStmt != null) {
    if (needSemi) {
      _semi();
      _newlineOpt();
    }
 else {
      _spaceOpt();
      needSemi=true;
    }
    _else();
    boolean elseIf=elseStmt instanceof JsIf;
    if (!elseIf) {
      _nestedPush(elseStmt,true);
    }
 else {
      _space();
    }
    accept(elseStmt);
    if (!elseIf) {
      _nestedPop(elseStmt);
    }
  }
  return false;
}
