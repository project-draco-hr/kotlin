{
  for (  File file : myClasspath) {
    final PsiClass psiClass=findClassInClasspathEntry(qName,file);
    if (psiClass != null) {
      return psiClass;
    }
  }
  for (  String str : myVirtualClasspath) {
    final PsiClass psiClass=findInVirtualJar(str,qName);
    if (psiClass != null) {
      return psiClass;
    }
  }
  return null;
}
