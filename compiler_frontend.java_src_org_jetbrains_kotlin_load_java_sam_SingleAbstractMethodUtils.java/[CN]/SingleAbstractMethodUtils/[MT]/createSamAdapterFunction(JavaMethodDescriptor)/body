{
  final SamAdapterFunctionDescriptor result=new SamAdapterFunctionDescriptor(original);
  return initSamAdapter(original,result,new FunctionInitializer(){
    @Override public void initialize(    @NotNull List<TypeParameterDescriptor> typeParameters,    @NotNull List<ValueParameterDescriptor> valueParameters,    @NotNull JetType returnType){
      result.initialize(null,original.getDispatchReceiverParameter(),typeParameters,valueParameters,returnType,Modality.FINAL,original.getVisibility(),false);
    }
  }
);
}
