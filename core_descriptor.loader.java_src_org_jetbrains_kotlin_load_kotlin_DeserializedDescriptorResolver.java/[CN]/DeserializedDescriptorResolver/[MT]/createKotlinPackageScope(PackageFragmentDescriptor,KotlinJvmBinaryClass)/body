{
  String[] data=readData(kotlinClass,KotlinClassHeader.Kind.FILE_FACADE);
  if (data != null) {
    PackageData packageData=JvmProtoBufUtil.readPackageDataFrom(data);
    return new DeserializedPackageMemberScope(descriptor,packageData.getPackageProto(),packageData.getNameResolver(),components,new Function0<Collection<Name>>(){
      @Override public Collection<Name> invoke(){
        return Collections.emptyList();
      }
    }
);
  }
  return null;
}
