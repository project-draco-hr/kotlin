{
  if (isDeclaration()) {
    return JsFunction.getAnonymousFunctionWithScope(context().getScopeForElement((JetDeclaration)functionDeclaration));
  }
  if (isLiteral()) {
    return new JsFunction(context().enclosingScope());
  }
  throw new AssertionError("Unsupported type of functionDeclaration.");
}
