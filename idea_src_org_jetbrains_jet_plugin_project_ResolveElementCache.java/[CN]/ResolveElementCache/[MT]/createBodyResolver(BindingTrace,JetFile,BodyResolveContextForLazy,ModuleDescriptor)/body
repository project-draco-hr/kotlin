{
  TopDownAnalysisParameters parameters=new TopDownAnalysisParameters(bodyResolveContext.getStorageManager(),bodyResolveContext.getExceptionTracker(),Predicates.<PsiFile>alwaysTrue(),false,true,Collections.<AnalyzerScriptParameter>emptyList());
  InjectorForBodyResolve bodyResolve=new InjectorForBodyResolve(file.getProject(),parameters,trace,module);
  return bodyResolve.getBodyResolver();
}
