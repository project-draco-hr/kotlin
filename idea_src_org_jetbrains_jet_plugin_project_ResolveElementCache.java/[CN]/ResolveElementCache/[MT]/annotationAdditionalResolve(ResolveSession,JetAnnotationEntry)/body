{
  JetDeclaration declaration=PsiTreeUtil.getParentOfType(jetAnnotationEntry,JetDeclaration.class);
  if (declaration != null) {
    Annotated descriptor=resolveSession.resolveToDescriptor(declaration);
    resolveSession.getAnnotationResolver().resolveAnnotationsArguments(descriptor,resolveSession.getTrace(),resolveSession.getScopeProvider().getResolutionScopeForDeclaration(declaration));
    ForceResolveUtil.forceResolveAllContents(descriptor.getAnnotations());
  }
}
