{
  return new JetSingleIntentionActionFactory(){
    @Override public KotlinQuickFixAction<PsiElement> createAction(    Diagnostic diagnostic){
      KtTypeArgumentList element=QuickFixUtil.getParentElementOfType(diagnostic,KtTypeArgumentList.class);
      if (element == null)       return null;
      return new RemovePsiElementSimpleFix(element,JetBundle.message("remove.type.arguments"));
    }
  }
;
}
