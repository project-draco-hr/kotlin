{
  return new KotlinSingleIntentionActionFactory(){
    @Override public KotlinQuickFixAction<PsiElement> createAction(    @NotNull Diagnostic diagnostic){
      KtTypeArgumentList element=QuickFixUtil.getParentElementOfType(diagnostic,KtTypeArgumentList.class);
      if (element == null)       return null;
      return new RemovePsiElementSimpleFix(element,KotlinBundle.message("remove.type.arguments"));
    }
  }
;
}
