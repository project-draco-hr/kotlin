{
  super(contextType,contextKind,parentContext,closure);
  final Type type=enclosingClassType(typeMapper);
  outerExpression=type != null ? StackValue.field(type,closure.state.getInjector().getJetTypeMapper().mapType(contextType.getDefaultType(),OwnerKind.IMPLEMENTATION).getInternalName(),"this$0",false) : null;
}
