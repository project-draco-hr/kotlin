{
  KotlinType thisType=makeType(contextType);
  ReceiverParameterDescriptorImpl receiverParameterDescriptor=new ReceiverParameterDescriptorImpl(scopeWithImports.getOwnerDescriptor(),new ExpressionReceiver(KtPsiFactoryKt.KtPsiFactory(getProject()).createExpression(expression),thisType));
  LexicalScope scope=new LexicalScopeImpl(scopeWithImports,scopeWithImports.getOwnerDescriptor(),false,receiverParameterDescriptor,"Scope with receiver: " + thisType);
  assertType(scope,expression,expectedType);
}
