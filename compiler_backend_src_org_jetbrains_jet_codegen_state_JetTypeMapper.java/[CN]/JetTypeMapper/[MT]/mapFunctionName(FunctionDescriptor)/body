{
  if (descriptor instanceof PropertyAccessorDescriptor) {
    return getPropertyAccessorName(((PropertyAccessorDescriptor)descriptor).getCorrespondingProperty(),descriptor instanceof PropertyGetterDescriptor);
  }
 else   if (isLocalNamedFun(descriptor) || descriptor instanceof AnonymousFunctionDescriptor || descriptor instanceof ExpressionAsFunctionDescriptor) {
    return "invoke";
  }
 else {
    return descriptor.getName().asString();
  }
}
