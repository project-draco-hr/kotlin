{
  CallCandidateResolutionContext<FunctionDescriptor> callCandidateResolutionContext;
  if (call != null && context != null && resolvedCall instanceof ResolvedCallImpl) {
    BasicCallResolutionContext basicCallResolutionContext=BasicCallResolutionContext.create(TRACE_STUB,context.scope,call,TypeUtils.NO_EXPECTED_TYPE,typeInfo.getDataFlowInfo(),context.namespacesAllowed);
    callCandidateResolutionContext=CallCandidateResolutionContext.createForCallBeingAnalyzed((ResolvedCallImpl<FunctionDescriptor>)resolvedCall,basicCallResolutionContext,TracingStrategy.EMPTY);
  }
 else {
    callCandidateResolutionContext=null;
  }
  return new TypeInfoForCall(typeInfo,callCandidateResolutionContext);
}
