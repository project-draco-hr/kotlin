{
  enterTemp();
  if (descriptor != null) {
    if (CodegenUtil.hasThis0(classDescriptor)) {
      myOuterThisIndex=enterTemp();
    }
  }
  List<Type> explicitArgTypes=callableMethod.getValueParameterTypes();
  List<ValueParameterDescriptor> paramDescrs=descriptor != null ? descriptor.getValueParameters() : Collections.<ValueParameterDescriptor>emptyList();
  for (int i=0; i < paramDescrs.size(); i++) {
    ValueParameterDescriptor parameter=paramDescrs.get(i);
    enter(parameter,explicitArgTypes.get(i).getSize());
  }
}
