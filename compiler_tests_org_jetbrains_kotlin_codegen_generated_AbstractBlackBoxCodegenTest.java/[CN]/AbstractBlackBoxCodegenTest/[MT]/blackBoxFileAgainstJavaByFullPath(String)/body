{
  String ktFile=relativePath(new File(ktFileFullPath));
  File javaClassesTempDirectory=compileJava(ktFile.replaceFirst("\\.kt$",".java"));
  myEnvironment=KotlinCoreEnvironment.createForTests(getTestRootDisposable(),JetTestUtils.compilerConfigurationForTests(ConfigurationKind.ALL,getTestJdkKind(ktFileFullPath),JetTestUtils.getAnnotationsJar(),javaClassesTempDirectory),EnvironmentConfigFiles.JVM_CONFIG_FILES);
  loadFile(ktFile);
  blackBox();
}
