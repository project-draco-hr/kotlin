{
  AbstractInsnNode next=node;
  while (true) {
    next=next.getNext();
    if (next == null) {
      return null;
    }
 else {
      int type=next.getType();
      if (type != AbstractInsnNode.LINE && type != AbstractInsnNode.LABEL && type != AbstractInsnNode.FRAME) {
        return next;
      }
    }
  }
}
