{
  JetTypeReference typeReference=psi.getTypeReference();
  JetExpression defaultValue=psi.getDefaultValue();
  return new PsiJetParameterStubImpl(parentStub,psi.getFqName(),psi.getName(),psi.isMutable(),psi.isVarArg(),typeReference != null ? typeReference.getText() : null,defaultValue != null ? defaultValue.getText() : null);
}
