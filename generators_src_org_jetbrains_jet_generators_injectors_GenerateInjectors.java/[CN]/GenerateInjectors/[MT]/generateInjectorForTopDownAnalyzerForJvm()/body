  generator.addField(false,VirtualFileFinder.class,"virtualFileFinder",new GivenExpression("com.intellij.openapi.components.ServiceManager.getService(project, VirtualFileFinder.class)"));
  generator.configure("compiler/frontend.java/src","org.jetbrains.jet.di","InjectorForTopDownAnalyzerForJvm",GenerateInjectors.class);
