{
  if (InlineUtil.isInlineLambdaParameter(parameter)) {
    if (parameter.getType().isMarkedNullable()) {
      if (trace != null) {
        trace.report(Errors.NULLABLE_INLINE_PARAMETER.on(expression,expression,functionDescriptor));
      }
    }
 else {
      return true;
    }
  }
  return false;
}
