{
  if (!(element instanceof PsiMethod))   return null;
  PsiMethod annotationOwner=element.getOriginalElement() instanceof PsiMethod ? (PsiMethod)element.getOriginalElement() : (PsiMethod)element;
  PsiAnnotation annotation=JavaDescriptorResolver.findAnnotation(annotationOwner,JvmStdlibNames.KOTLIN_SIGNATURE.getFqName().getFqName());
  if (annotation == null)   return null;
  if (annotation.getParameterList().getAttributes().length == 0)   return null;
  return annotation;
}
