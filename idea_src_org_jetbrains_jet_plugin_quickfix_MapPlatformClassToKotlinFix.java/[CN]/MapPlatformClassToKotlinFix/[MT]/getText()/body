{
  String platformClassQualifiedName=DescriptorRenderer.FQNAMES_IN_TYPES.renderType(platformClass.getDefaultType());
  return possibleClasses.size() == 1 ? JetBundle.message("map.platform.class.to.kotlin",platformClassQualifiedName,DescriptorRenderer.FQNAMES_IN_TYPES.renderType(possibleClasses.iterator().next().getDefaultType())) : JetBundle.message("map.platform.class.to.kotlin.multiple",platformClassQualifiedName);
}
