{
  Node result=cloneNode();
  for (Node n2=getFirstChild(); n2 != null; n2=n2.getNext()) {
    Node n2clone=n2.cloneTree();
    if (result.last != null) {
      result.last.next=n2clone;
    }
    if (result.first == null) {
      result.first=n2clone;
    }
    result.last=n2clone;
  }
  return result;
}
