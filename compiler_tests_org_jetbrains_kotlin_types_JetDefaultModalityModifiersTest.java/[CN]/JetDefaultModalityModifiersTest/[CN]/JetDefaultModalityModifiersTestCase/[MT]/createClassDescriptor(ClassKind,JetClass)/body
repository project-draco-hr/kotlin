{
  ModuleContext moduleContext=ContextPackage.ModuleContext(root,getProject());
  ResolveSession resolveSession=createLazyResolveSession(moduleContext,new FileBasedDeclarationProviderFactory(moduleContext.getStorageManager(),Collections.singleton(aClass.getContainingJetFile())),new BindingTraceContext(),TargetPlatform.Default.INSTANCE$);
  return (ClassDescriptorWithResolutionScopes)resolveSession.getClassDescriptor(aClass,NoLookupLocation.FROM_TEST);
}
