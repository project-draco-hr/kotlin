{
  ClassDescriptorImpl classDescriptor=classesForFunctions.get(scriptDescriptor);
  if (classDescriptor == null) {
    classDescriptor=new ClassDescriptorImpl(scriptDescriptor,Collections.<AnnotationDescriptor>emptyList(),Name.special("<script>"));
    recordName(classDescriptor,JvmClassName.byInternalName("Script"));
    classDescriptor.initialize(false,Collections.<TypeParameterDescriptor>emptyList(),Collections.singletonList(JetStandardClasses.getAnyType()),JetScope.EMPTY,Collections.<ConstructorDescriptor>emptySet(),null);
    classesForFunctions.put(scriptDescriptor,classDescriptor);
  }
  return classDescriptor;
}
