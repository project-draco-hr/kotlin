{
  VirtualFile virtualFile=kotlinPackagePsiClass.getContainingFile().getVirtualFile();
  if (virtualFile == null) {
    return null;
  }
  PackageData packageData=readPackageDataFromClassFile(virtualFile,reporter);
  if (packageData == null) {
    return null;
  }
  return new DeserializedPackageMemberScope(storageManager,packageDescriptor,annotationDeserializer,javaDescriptorFinder,packageData);
}
