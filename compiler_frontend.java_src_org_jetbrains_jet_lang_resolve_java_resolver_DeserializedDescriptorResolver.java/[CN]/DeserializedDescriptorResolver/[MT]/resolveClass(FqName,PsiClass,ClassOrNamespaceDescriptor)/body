{
  VirtualFile virtualFile=getVirtualFile(psiClass,fqName,containingDeclaration);
  if (virtualFile == null) {
    return null;
  }
  ClassData classData=readClassDataFromClassFile(virtualFile);
  if (classData == null) {
    return null;
  }
  return deserializeClass(classData,fqName,containingDeclaration);
}
