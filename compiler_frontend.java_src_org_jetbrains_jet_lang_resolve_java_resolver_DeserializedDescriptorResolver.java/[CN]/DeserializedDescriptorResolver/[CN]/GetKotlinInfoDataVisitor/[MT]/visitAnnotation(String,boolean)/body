{
  if (!desc.equals(KOTLIN_INFO_TYPE)) {
    return null;
  }
  return new AnnotationVisitor(Opcodes.ASM4){
    @Override public void visit(    String name,    Object value){
      if (name.equals("data")) {
        data=(byte[])value;
      }
 else {
        LOG.error("Unexpected property " + name + " for annotation "+ KOTLIN_INFO_TYPE);
      }
    }
  }
;
}
