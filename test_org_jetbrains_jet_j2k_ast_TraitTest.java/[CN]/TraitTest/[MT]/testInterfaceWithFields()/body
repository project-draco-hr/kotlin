{
  Assert.assertEquals(classToKotlin("interface INode {" + "String IN = \"in\";" + "String AT = \"@\";"+ "String COMMA_WITH_SPACE = COMMA + SPACE;"+ "}"),"trait INode {\n" + "public val IN : String? = \"in\"\n" + "public val AT : String? = \"@\"\n"+ "public val COMMA_WITH_SPACE : String? = (COMMA + SPACE)\n"+ "}");
}
