{
  components.localClassifierAnalyzer.processClassOrObject(components.globalContext,scope,context.replaceScope(scope).replaceContextDependency(INDEPENDENT),scope.getContainingDeclaration(),klass,components.additionalCheckerProvider,components.dynamicTypesSettings);
  return TypeInfoFactoryPackage.createTypeInfo(DataFlowUtils.checkStatementType(klass,context),context);
}
