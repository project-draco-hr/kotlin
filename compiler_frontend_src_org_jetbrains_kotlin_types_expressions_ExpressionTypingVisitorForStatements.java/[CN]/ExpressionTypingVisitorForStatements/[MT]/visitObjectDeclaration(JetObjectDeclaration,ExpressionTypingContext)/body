{
  components.localClassifierAnalyzer.processClassOrObject(components.globalContext,scope,context.replaceScope(scope).replaceContextDependency(INDEPENDENT),scope.getContainingDeclaration(),declaration,components.additionalCheckerProvider,components.dynamicTypesSettings);
  return TypeInfoFactoryPackage.createTypeInfo(DataFlowUtils.checkStatementType(declaration,context),context);
}
