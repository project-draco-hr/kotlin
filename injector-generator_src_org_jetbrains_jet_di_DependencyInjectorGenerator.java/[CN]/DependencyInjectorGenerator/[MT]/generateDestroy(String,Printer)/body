{
  out.println("@PreDestroy");
  out.println("public void destroy() {");
  out.pushIndent();
  for (  Field field : fields) {
    List<Method> preDestroyMethods=InjectorGeneratorUtil.getPreDestroyMethods(InjectorGeneratorUtil.getEffectiveFieldType(field).getClazz());
    for (    Method preDestroy : preDestroyMethods) {
      out.println(field.getName() + "." + preDestroy.getName()+ "();");
    }
    if (preDestroyMethods.size() > 0) {
      out.println();
    }
  }
  out.popIndent();
  out.println("}");
}
