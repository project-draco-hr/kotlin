{
  for (  Field field : fields) {
    if (!field.isPublic())     continue;
    String visibility="public";
    out.println(visibility + " " + type(field.getType())+ " "+ field.getGetterName()+ "() {");
    out.pushIndent();
    out.println("return this." + field.getName() + ";");
    out.popIndent();
    out.println("}");
    out.println();
  }
}
