{
  List<Field> fields=Lists.newArrayList();
  for (  Map.Entry<Class<?>,Field> entry : typeToField.entries()) {
    if (parameterType.isAssignableFrom(entry.getKey())) {
      fields.add(entry.getValue());
    }
  }
  Field dependency;
  if (fields.isEmpty()) {
    if (parameterType.isPrimitive() || parameterType.getPackage().getName().equals("java.lang")) {
      throw new IllegalArgumentException("cannot declare magic field of type " + parameterType + ": "+ errorMessage);
    }
    dependency=addField(parameterType);
    satisfyDependenciesFor(dependency,neededFor);
  }
 else   if (fields.size() == 1) {
    dependency=fields.iterator().next();
  }
 else {
    throw new IllegalArgumentException("Ambiguous dependency: " + errorMessage);
  }
  return dependency;
}
