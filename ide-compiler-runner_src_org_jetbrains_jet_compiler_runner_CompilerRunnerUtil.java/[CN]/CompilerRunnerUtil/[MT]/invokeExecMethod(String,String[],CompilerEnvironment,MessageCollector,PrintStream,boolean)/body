{
  ClassLoader loader=usePreloader ? getOrCreatePreloader(environment.getKotlinPaths(),environment.getParentClassLoader(),environment.getClassesToLoadByParent(),messageCollector) : getOrCreateClassLoader(environment.getKotlinPaths(),messageCollector);
  Class<?> kompiler=Class.forName(compilerClassName,true,loader);
  Method exec=kompiler.getMethod("execAndOutputHtml",PrintStream.class,Class.forName("org.jetbrains.jet.config.CompilerServices",true,loader),String[].class);
  return exec.invoke(kompiler.newInstance(),out,environment.getServices(),arguments);
}
