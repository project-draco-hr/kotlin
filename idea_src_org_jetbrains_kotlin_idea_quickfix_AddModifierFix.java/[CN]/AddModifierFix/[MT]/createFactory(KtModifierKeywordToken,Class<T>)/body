{
  return new JetSingleIntentionActionFactory(){
    @Override public IntentionAction createAction(    Diagnostic diagnostic){
      KtModifierListOwner modifierListOwner=QuickFixUtil.getParentElementOfType(diagnostic,modifierOwnerClass);
      if (modifierListOwner == null)       return null;
      if (modifier == KtTokens.ABSTRACT_KEYWORD && modifierListOwner instanceof KtObjectDeclaration && ((KtObjectDeclaration)modifierListOwner).isObjectLiteral())       return null;
      return new AddModifierFix(modifierListOwner,modifier);
    }
  }
;
}
