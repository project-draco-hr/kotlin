{
  initializeDataMap(pseudocode,initialDataValue);
  dataMap.put(straightDirection ? pseudocode.getEnterInstruction() : pseudocode.getSinkInstruction(),Pair.create(initialDataValueForEnterInstruction,initialDataValueForEnterInstruction));
  boolean[] changed=new boolean[1];
  changed[0]=true;
  while (changed[0]) {
    changed[0]=false;
    traverseSubGraph(pseudocode,instructionDataMergeStrategy,Collections.<Instruction>emptyList(),straightDirection,changed,false);
  }
}
