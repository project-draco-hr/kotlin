{
switch (descriptor.getKind()) {
case INTERFACE:
    return KotlinHighlightingColors.TRAIT;
case ANNOTATION_CLASS:
  return KotlinHighlightingColors.ANNOTATION;
case OBJECT:
return KotlinHighlightingColors.OBJECT;
case ENUM_ENTRY:
return KotlinHighlightingColors.ENUM_ENTRY;
default :
return descriptor.getModality() == Modality.ABSTRACT ? KotlinHighlightingColors.ABSTRACT_CLASS : KotlinHighlightingColors.CLASS;
}
}
