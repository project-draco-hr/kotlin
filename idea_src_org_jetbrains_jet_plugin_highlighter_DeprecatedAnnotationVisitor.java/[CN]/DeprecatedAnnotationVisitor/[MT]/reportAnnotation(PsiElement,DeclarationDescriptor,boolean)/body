{
  if (isWarning) {
    holder.createInfoAnnotation(element,"'" + renderName(descriptor) + "' is deprecated").setTextAttributes(CodeInsightColors.WARNINGS_ATTRIBUTES);
  }
 else {
    holder.createInfoAnnotation(element,"'" + renderName(descriptor) + "' is deprecated").setTextAttributes(CodeInsightColors.DEPRECATED_ATTRIBUTES);
  }
}
