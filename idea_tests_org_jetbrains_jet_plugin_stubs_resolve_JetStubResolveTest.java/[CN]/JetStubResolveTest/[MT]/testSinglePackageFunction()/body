{
  myFixture.configureByText(JetFileType.INSTANCE,"package test.testing\n" + "fun some() {}");
  StubPackageMemberDeclarationProvider provider=new StubPackageMemberDeclarationProvider(new FqName("test.testing"),getProject(),GlobalSearchScope.projectScope(getProject()));
  List<JetNamedFunction> some=Lists.newArrayList(provider.getFunctionDeclarations(Name.identifier("some")));
  assertSize(1,some);
  assertEquals("fun some() {}",some.get(0).getText());
}
