{
  StringBuilder exclude=new StringBuilder();
  for (  String dir : excludeDirs) {
    exclude.append(", \"");
    exclude.append(StringUtil.escapeStringCharacters(dir));
    exclude.append("\"");
  }
  String assertTestsPresentStr=String.format("JetTestUtils.assertAllTestsPresentByMetadata(this.getClass(), new File(\"%s\"), Pattern.compile(\"%s\"), %s%s);",JetTestUtils.getFilePath(rootFile),StringUtil.escapeStringCharacters(filenamePattern.pattern()),recursive,exclude);
  p.println(assertTestsPresentStr);
}
