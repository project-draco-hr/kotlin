{
  PsiBuilder.Marker expression=mark();
  priority.parseHigherPriority(this);
  while (!myBuilder.eolInLastWhitespace() && atSet(priority.getOperations())) {
    advance();
    priority.parseHigherPriority(this);
    expression.done(BINARY_EXPRESSION);
    expression=expression.precede();
  }
  expression.drop();
}
