{
  while (at(SEMICOLON))   advance();
  while (!eof() && !at(RBRACE)) {
    parseExpression();
    if (at(SEMICOLON)) {
      while (at(SEMICOLON))       advance();
    }
 else     if (at(RBRACE)) {
      break;
    }
 else     if (!myBuilder.eolInLastWhitespace()) {
      errorUntil("Unexpected tokens (use ';' to separate expressions on the same line",TokenSet.create(EOL_OR_SEMICOLON));
    }
  }
}
