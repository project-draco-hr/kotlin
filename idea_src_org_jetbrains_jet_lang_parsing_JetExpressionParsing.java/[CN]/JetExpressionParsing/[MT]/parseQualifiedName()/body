{
  PsiBuilder.Marker mark=mark();
  if (at(NAMESPACE_KEYWORD)) {
    advance();
    mark.done(REFERENCE_EXPRESSION);
    mark=mark.precede();
    expect(DOT,"Expecting '.'");
  }
  while (true) {
    expect(IDENTIFIER,"Expecting an indetifier",TokenSet.create(DOT));
    mark.done(REFERENCE_EXPRESSION);
    mark=mark.precede();
    if (!at(DOT))     break;
    advance();
  }
  mark.drop();
}
