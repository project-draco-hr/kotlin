{
  PsiBuilder.Marker pattern=mark();
  if (at(IDENTIFIER)) {
    myJetParsing.parseUserType();
    if (at(LPAR)) {
      parseTuplePattern();
    }
  }
 else   if (at(LPAR)) {
    parseTuplePattern();
  }
 else   if (at(QUEST)) {
    parseBindingPattern();
  }
 else   if (!parseLiteralConstant()) {
    errorUntil("Pattern expected",TokenSet.create(RBRACE,DOUBLE_ARROW));
  }
  pattern.done(PATTERN);
}
