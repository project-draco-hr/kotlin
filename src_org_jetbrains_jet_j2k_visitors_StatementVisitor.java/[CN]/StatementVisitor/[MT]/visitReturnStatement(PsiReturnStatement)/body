{
  super.visitReturnStatement(statement);
  PsiExpression returnValue=statement.getReturnValue();
  PsiType methodReturnType=getConverter().getMethodReturnType();
  Expression expression=returnValue != null && methodReturnType != null ? this.getConverter().expressionToExpression(returnValue,methodReturnType) : getConverter().expressionToExpression(returnValue);
  myResult=new ReturnStatement(expression);
}
