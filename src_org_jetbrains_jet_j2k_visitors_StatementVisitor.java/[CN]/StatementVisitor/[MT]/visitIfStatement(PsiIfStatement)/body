{
  super.visitIfStatement(statement);
  PsiExpression condition=statement.getCondition();
  @SuppressWarnings("ConstantConditions") Expression expression=condition != null && condition.getType() != null ? this.getConverter().expressionToExpression(condition,condition.getType()) : getConverter().expressionToExpression(condition);
  myResult=new IfStatement(expression,getConverter().statementToStatement(statement.getThenBranch()),getConverter().statementToStatement(statement.getElseBranch()));
}
