{
  JetCoreEnvironment jetCoreEnvironment=new JetCoreEnvironment(myTestRootDisposable,CompileCompilerDependenciesTest.compilerConfigurationForTests(ConfigurationKind.ALL,TestJdkKind.MOCK_JDK,JetTestUtils.getAnnotationsJar(),aDir));
  String text=FileUtil.loadFile(ktBFile);
  LightVirtualFile virtualFile=new LightVirtualFile(ktAFile.getName(),JetLanguage.INSTANCE,text);
  virtualFile.setCharset(CharsetToolkit.UTF8_CHARSET);
  JetFile psiFile=(JetFile)((PsiFileFactoryImpl)PsiFileFactory.getInstance(jetCoreEnvironment.getProject())).trySetupPsiForFile(virtualFile,JetLanguage.INSTANCE,true,false);
  ClassFileFactory classFileFactory=GenerationUtils.compileFileGetClassFileFactoryForTest(psiFile);
  CompileEnvironmentUtil.writeToOutputDirectory(classFileFactory,bDir);
  Disposer.dispose(myTestRootDisposable);
}
