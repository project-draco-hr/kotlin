{
  CompletionSorter sorter=CompletionSorter.defaultSorter(parameters,result.getPrefixMatcher());
  sorter=sorter.weighAfter("stats",new JetLocalPreferableWeigher(),new JetExplicitlyImportedWeigher((JetFile)parameters.getOriginalFile()),new JetAccessibleWeigher());
  return result.withRelevanceSorter(sorter);
}
