{
  PropertyDescriptorImpl propertyDescriptor=PropertyDescriptorImpl.create(scriptDescriptor.getClassDescriptor(),Annotations.EMPTY,Modality.FINAL,Visibilities.PUBLIC,false,parameter.getName(),CallableMemberDescriptor.Kind.DECLARATION);
  propertyDescriptor.setType(parameter.getType(),Collections.<TypeParameterDescriptor>emptyList(),scriptDescriptor.getThisAsReceiverParameter(),ReceiverParameterDescriptor.NO_RECEIVER_PARAMETER);
  propertyDescriptor.initialize(null,null);
  return propertyDescriptor;
}
