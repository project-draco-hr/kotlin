{
  Annotations annotations=DescriptorDeserializer.getAnnotations(containingDeclaration,proto,proto.getFlags(),Deserializers.AnnotatedCallableKind.FUNCTION,deserializers.getAnnotationDeserializer(),nameResolver);
  return new DeserializedSimpleFunctionDescriptor(containingDeclaration,null,annotations,nameResolver.getName(proto.getName()),DescriptorDeserializer.memberKind(Flags.MEMBER_KIND.get(proto.getFlags())),proto,nameResolver);
}
