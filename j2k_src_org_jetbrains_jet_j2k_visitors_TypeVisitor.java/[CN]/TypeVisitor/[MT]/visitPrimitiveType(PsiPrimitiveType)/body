{
  final String name=primitiveType.getCanonicalText();
  final IdentifierImpl identifier=new IdentifierImpl(name);
  if (name.equals("void"))   myResult=new PrimitiveType(new IdentifierImpl("Unit"));
 else   if (Node.PRIMITIVE_TYPES.contains(name))   myResult=new PrimitiveType(new IdentifierImpl(AstUtil.upperFirstCharacter(name)));
 else   myResult=new PrimitiveType(identifier);
  return super.visitPrimitiveType(primitiveType);
}
