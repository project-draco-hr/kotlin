{
  final IdentifierImpl identifier=constructClassTypeIdentifier(classType);
  final List<Type> resolvedClassTypeParams=createRawTypesForResolvedReference(classType);
  if (classType.getParameterCount() == 0 && resolvedClassTypeParams.size() > 0)   myResult=new ClassType(identifier,resolvedClassTypeParams);
 else   myResult=new ClassType(identifier,typesToTypeList(classType.getParameters()));
  return super.visitClassType(classType);
}
