{
  Disposable compileDisposable=createDisposable("compileA");
  KotlinCoreEnvironment environment=KotlinTestUtils.createEnvironmentWithJdkAndNullabilityAnnotationsFromIdea(compileDisposable,ConfigurationKind.ALL,getJdkKind(files));
