{
  parameterInfos=CollectionsKt.mapTo(parameters,receiver != null ? CollectionsKt.arrayListOf(new ParameterInfo(receiver,true)) : new ArrayList<ParameterInfo>(),new Function1<Parameter,ParameterInfo>(){
    @Override public ParameterInfo invoke(    Parameter parameter){
      return new ParameterInfo(parameter,false);
    }
  }
);
  super.init();
}
