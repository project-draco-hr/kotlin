{
  loadText("fun box() : String? { val s = Array<String>(1,{ \"\" }); s [1, -1] = \"5\"; s[2, -2] += \"7\"; return s[-3,3] }\n" + "fun Array<String>.get(index1: Int, index2 : Int) = this[index1+index2]\n" + "fun Array<String>.set(index1: Int, index2 : Int, elem: String) { this[index1+index2] = elem\n }");
  System.out.println(generateToText());
  Method foo=generateFunction("box");
  assertTrue(foo.invoke(null).equals("57"));
}
