{
  loadText("fun box() : Int { var l = IntArray(1); l[0.lng] = 4; l[0.lng] += 6; return l[0.lng];}\n" + "fun IntArray.set(index: Long, elem: Int) { this[index.int] = elem }\n" + "fun IntArray.get(index: Long) = this[index.int]");
  System.out.println(generateToText());
  Method foo=generateFunction("box");
  assertTrue((Integer)foo.invoke(null) == 10);
}
