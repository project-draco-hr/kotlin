{
  List<JetNamespace> allNamespaces=new ArrayList<JetNamespace>(mySourceFileNamespaces);
  allNamespaces.addAll(myLibrarySourceFileNamespaces);
  return AnalyzerFacade.analyzeNamespacesWithJavaIntegration(myEnvironment.getProject(),allNamespaces,Predicates.<PsiFile>alwaysTrue(),JetControlFlowDataTraceFactory.EMPTY);
}
