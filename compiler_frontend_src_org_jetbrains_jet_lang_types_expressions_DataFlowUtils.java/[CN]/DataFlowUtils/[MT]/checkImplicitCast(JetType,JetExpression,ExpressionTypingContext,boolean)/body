{
  if (expressionType != null && context.expectedType == NO_EXPECTED_TYPE && context.contextDependency == INDEPENDENT && !isStatement && (KotlinBuiltIns.getInstance().isUnit(expressionType) || KotlinBuiltIns.getInstance().isAnyOrNullableAny(expressionType))) {
    context.trace.report(IMPLICIT_CAST_TO_UNIT_OR_ANY.on(expression,expressionType));
  }
  return expressionType;
}
