{
  if (expressionType != null && context.expectedType == TypeUtils.NO_EXPECTED_TYPE && !isStatement && (KotlinBuiltIns.getInstance().isUnit(expressionType) || KotlinBuiltIns.getInstance().isAny(expressionType))) {
    context.trace.report(IMPLICIT_CAST_TO_UNIT_OR_ANY.on(expression,expressionType));
  }
  return expressionType;
}
