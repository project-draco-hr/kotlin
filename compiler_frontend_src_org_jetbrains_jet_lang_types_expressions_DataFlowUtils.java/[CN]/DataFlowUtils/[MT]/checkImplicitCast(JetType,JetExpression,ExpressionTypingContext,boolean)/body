{
  if (expressionType != null && context.expectedType == TypeUtils.NO_EXPECTED_TYPE && !isStatement && (JetStandardClasses.isUnit(expressionType) || JetStandardClasses.isAny(expressionType))) {
    context.trace.report(IMPLICIT_CAST_TO_UNIT_OR_ANY.on(expression,expressionType));
  }
  return expressionType;
}
