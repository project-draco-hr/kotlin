{
  final ClassDescriptor enclosingClass=getEnclosingClass();
  outerExpression=enclosingClass != null ? StackValue.field(typeMapper.mapType(enclosingClass.getDefaultType(),JetTypeMapperMode.VALUE),CodegenBinding.getJvmClassName(typeMapper.getBindingTrace(),classDescriptor),CodegenUtil.THIS$0,false) : null;
}
