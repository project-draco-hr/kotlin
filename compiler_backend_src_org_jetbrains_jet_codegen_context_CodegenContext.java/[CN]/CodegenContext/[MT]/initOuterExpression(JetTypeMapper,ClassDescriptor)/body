{
  lazyOuterExpression=LockBasedStorageManager.NO_LOCKS.createNullableLazyValue(new Function0<StackValue.Field>(){
    @Override public StackValue.Field invoke(){
      ClassDescriptor enclosingClass=getEnclosingClass();
      if (enclosingClass == null)       return null;
      return canHaveOuter(typeMapper.getBindingContext(),classDescriptor) ? StackValue.field(typeMapper.mapType(enclosingClass),typeMapper.mapType(classDescriptor),CAPTURED_THIS_FIELD,false,StackValue.LOCAL_0) : null;
    }
  }
);
}
