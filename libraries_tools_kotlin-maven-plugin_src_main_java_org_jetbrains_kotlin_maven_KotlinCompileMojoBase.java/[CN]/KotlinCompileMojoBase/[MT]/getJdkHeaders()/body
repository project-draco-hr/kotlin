{
  if (jdkHeadersPath != null)   return jdkHeadersPath;
  try {
    jdkHeadersPath=extractJdkHeaders();
    if (jdkHeadersPath == null)     throw new RuntimeException("Can't find kotlin jdk headers in maven plugin resources");
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  return jdkHeadersPath;
}
