{
  if (type == null) {
    type=new JetTypeImpl(Collections.<Annotation>emptyList(),getTypeConstructor(),TypeUtils.hasNullableBound(this),Collections.<TypeProjection>emptyList(),new LazyScopeAdapter(new LazyValue<JetScope>(){
      @Override protected JetScope compute(){
        return getBoundsAsType().getMemberScope();
      }
    }
));
  }
  return type;
}
