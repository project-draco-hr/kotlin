{
  final List<TypeParameterDescriptor> r=new ArrayList<TypeParameterDescriptor>();
  new JetSignatureReader(jetSignature).accept(new JetSignatureExceptionsAdapter(){
    @Override public JetSignatureVisitor visitFormalTypeParameter(    final String name,    final TypeInfoVariance variance){
      return new JetSignatureTypeParameterVisitor(classDescriptor,clazz,name,variance){
        @Override protected void done(        TypeParameterDescriptor typeParameterDescriptor){
          r.add(typeParameterDescriptor);
        }
      }
;
    }
    @Override public JetSignatureVisitor visitSuperclass(){
      return new JetSignatureAdapter();
    }
    @Override public JetSignatureVisitor visitInterface(){
      return new JetSignatureAdapter();
    }
  }
);
  return r;
}
