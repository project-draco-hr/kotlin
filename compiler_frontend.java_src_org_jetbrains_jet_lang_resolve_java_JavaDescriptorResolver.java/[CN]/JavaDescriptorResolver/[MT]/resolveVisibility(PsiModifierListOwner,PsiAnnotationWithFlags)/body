{
  if (annotation != null) {
    BitSet flags=annotation.flags();
    if (flags.get(JvmStdlibNames.FLAG_PRIVATE_BIT)) {
      return Visibilities.PRIVATE;
    }
 else     if (flags.get(JvmStdlibNames.FLAG_INTERNAL_BIT)) {
      return Visibilities.INTERNAL;
    }
  }
  return modifierListOwner.hasModifierProperty(PsiModifier.PUBLIC) ? Visibilities.PUBLIC : (modifierListOwner.hasModifierProperty(PsiModifier.PRIVATE) ? Visibilities.PRIVATE : (modifierListOwner.hasModifierProperty(PsiModifier.PROTECTED) ? Visibilities.PROTECTED : PACKAGE_VISIBILITY));
}
