{
  VirtualFile virtualFile=getVirtualFileForPsiClass(psiClass);
  if (virtualFile != null) {
    KotlinJvmBinaryClass kotlinClass=KotlinBinaryClassCache.getKotlinBinaryClass(virtualFile);
    if (kotlinClass != null) {
      return kotlinClass.getClassHeader().getAnnotationData();
    }
  }
  return null;
}
