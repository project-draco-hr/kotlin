{
  CommonCodeStyleSettings jetCommonSettings=mySettings.getCommonSettings(JetLanguage.INSTANCE);
  ASTAlignmentStrategy strategy=ASTAlignmentStrategy.getNullStrategy();
  IElementType parentType=myNode.getElementType();
  if (parentType == VALUE_PARAMETER_LIST) {
    strategy=getAlignmentForChildInParenthesis(jetCommonSettings.ALIGN_MULTILINE_PARAMETERS,VALUE_PARAMETER,COMMA,jetCommonSettings.ALIGN_MULTILINE_METHOD_BRACKETS,LPAR,RPAR);
  }
 else   if (parentType == VALUE_ARGUMENT_LIST) {
    strategy=getAlignmentForChildInParenthesis(jetCommonSettings.ALIGN_MULTILINE_PARAMETERS_IN_CALLS,VALUE_ARGUMENT,COMMA,jetCommonSettings.ALIGN_MULTILINE_METHOD_BRACKETS,LPAR,RPAR);
  }
  return strategy;
}
