{
  isInitialized=true;
  isDeclared=true;
  for (  VariableInitializers edgeData : edgesData) {
    if (!edgeData.isInitialized) {
      isInitialized=false;
    }
    if (!edgeData.isDeclared) {
      isDeclared=false;
    }
    possibleLocalInitializers.addAll(edgeData.possibleLocalInitializers);
  }
}
