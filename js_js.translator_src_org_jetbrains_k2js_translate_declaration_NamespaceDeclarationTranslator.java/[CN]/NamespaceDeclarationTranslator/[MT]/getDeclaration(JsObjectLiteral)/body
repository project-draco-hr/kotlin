{
  JsExpression packageMapValue;
  if (context().isEcma5()) {
    packageMapValue=new JsInvocation(JsAstUtils.CREATE_OBJECT,JsLiteral.NULL,new JsDocComment(JsAstUtils.LENDS_JS_DOC_TAG,Namer.getRootNamespaceName()),rootNamespaceDefinition);
  }
 else {
    packageMapValue=rootNamespaceDefinition;
  }
  return new JsVar(context().scope().declareName(Namer.getRootNamespaceName()),packageMapValue);
}
