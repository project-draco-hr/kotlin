{
  if (field instanceof PsiEnumConstant)   return new EnumConstant(new IdentifierImpl(field.getName()),typeToType(field.getType()),elementToElement(((PsiEnumConstant)field).getArgumentList()));
  return new Field(new IdentifierImpl(field.getName()),typeToType(field.getType()),expressionToExpression(field.getInitializer()));
}
