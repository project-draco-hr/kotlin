{
  if (method.getParent() instanceof PsiClass) {
    final PsiModifierList parentModifierList=((PsiClass)method.getParent()).getModifierList();
    if ((parentModifierList != null && parentModifierList.hasExplicitModifier(Modifier.FINAL)) || ((PsiClass)method.getParent()).isEnum())     return true;
  }
  return false;
}
