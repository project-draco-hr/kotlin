{
  VariableDescriptor variableDescriptor=new LocalVariableDescriptor(containingDeclaration,annotationResolver.getResolvedAnnotations(parameter.getModifierList(),trace),JetPsiUtil.safeName(parameter.getName()),type,parameter.isMutable());
  trace.record(BindingContext.VALUE_PARAMETER,parameter,variableDescriptor);
  return variableDescriptor;
}
