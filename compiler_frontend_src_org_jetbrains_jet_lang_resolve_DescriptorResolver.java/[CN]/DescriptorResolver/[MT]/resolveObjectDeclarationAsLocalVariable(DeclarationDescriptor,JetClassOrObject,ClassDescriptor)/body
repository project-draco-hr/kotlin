{
  VariableDescriptorImpl variableDescriptor=new LocalVariableDescriptor(containingDeclaration,annotationResolver.createAnnotationStubs(objectDeclaration.getModifierList()),JetPsiUtil.safeName(objectDeclaration.getName()),classDescriptor.getDefaultType(),false);
  JetObjectDeclarationName nameAsDeclaration=objectDeclaration.getNameAsDeclaration();
  if (nameAsDeclaration != null) {
    trace.record(BindingContext.VARIABLE,nameAsDeclaration,variableDescriptor);
  }
  return variableDescriptor;
}
