{
  JetType varargElementType=null;
  JetType variableType=type;
  if (valueParameter.hasModifier(VARARG_KEYWORD)) {
    varargElementType=type;
    variableType=DescriptorUtils.getVarargParameterType(type);
  }
  ValueParameterDescriptorImpl valueParameterDescriptor=new ValueParameterDescriptorImpl(declarationDescriptor,null,index,annotations,JetPsiUtil.safeName(valueParameter.getName()),variableType,valueParameter.getDefaultValue() != null,varargElementType);
  trace.record(BindingContext.VALUE_PARAMETER,valueParameter,valueParameterDescriptor);
  return valueParameterDescriptor;
}
