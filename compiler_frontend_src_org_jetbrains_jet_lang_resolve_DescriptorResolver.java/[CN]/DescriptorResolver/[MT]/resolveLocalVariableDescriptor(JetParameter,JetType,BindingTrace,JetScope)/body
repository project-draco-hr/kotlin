{
  VariableDescriptor variableDescriptor=new LocalVariableDescriptor(scope.getContainingDeclaration(),annotationResolver.resolveAnnotationsWithArguments(scope,parameter.getModifierList(),trace),JetPsiUtil.safeName(parameter.getName()),type,false,toSourceElement(parameter));
  trace.record(BindingContext.VALUE_PARAMETER,parameter,variableDescriptor);
  return variableDescriptor;
}
