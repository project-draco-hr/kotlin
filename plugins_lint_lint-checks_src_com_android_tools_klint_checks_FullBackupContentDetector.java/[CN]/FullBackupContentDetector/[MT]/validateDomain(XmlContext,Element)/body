{
  Attr domainNode=element.getAttributeNode(ATTR_DOMAIN);
  if (domainNode == null) {
    context.report(ISSUE,element,context.getLocation(element),String.format("Missing domain attribute, expected one of %1$s",Joiner.on(", ").join(VALID_DOMAINS)));
    return null;
  }
  String domain=domainNode.getValue();
  for (  String availableDomain : VALID_DOMAINS) {
    if (availableDomain.equals(domain)) {
      return domain;
    }
  }
  context.report(ISSUE,element,context.getValueLocation(domainNode),String.format("Unexpected domain `%1$s`, expected one of %2$s",domain,Joiner.on(", ").join(VALID_DOMAINS)));
  return domain;
}
