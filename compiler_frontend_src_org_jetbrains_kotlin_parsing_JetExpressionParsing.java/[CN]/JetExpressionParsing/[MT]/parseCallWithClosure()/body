{
  boolean success=false;
  while (at(LBRACE) || isAtLabelDefinitionBeforeLBrace()) {
    PsiBuilder.Marker argument=mark();
    if (!at(LBRACE)) {
      parseLabeledExpression();
    }
 else {
      parseFunctionLiteral();
    }
    argument.done(FUNCTION_LITERAL_ARGUMENT);
    success=true;
  }
  return success;
}
