{
  file.acceptChildren(new JetVisitor(){
    @Override public void visitProperty(    JetProperty property){
      VariableDescriptor propertyDescriptor=bindingContext.getVariableDescriptor(property);
      if (propertyDescriptor instanceof PropertyDescriptor) {
        if (bindingContext.hasBackingField((PropertyDescriptor)propertyDescriptor)) {
          holder.createInfoAnnotation(property.getNameIdentifier(),"This property has a backing field").setTextAttributes(JetHighlighter.JET_PROPERTY_WITH_BACKING_FIELD_IDENTIFIER);
        }
      }
    }
    @Override public void visitJetElement(    JetElement elem){
      elem.acceptChildren(this);
    }
  }
);
}
