{
switch (descriptor.getKind()) {
case TRAIT:
    return new JsInvocation(traitCreationMethodReference());
case OBJECT:
  return new JsInvocation(objectCreationMethodReference());
case CLASS_OBJECT:
return new JsInvocation(objectCreationMethodReference());
default :
return new JsInvocation(classCreationMethodReference());
}
}
