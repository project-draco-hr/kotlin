{
  _for();
  spaceOpt();
  leftParen();
  if (x.getIterVarName() != null) {
    var();
    space();
    nameDef(x.getIterVarName());
    if (x.getIterExpr() != null) {
      spaceOpt();
      assignment();
      spaceOpt();
      accept(x.getIterExpr());
    }
  }
 else {
    accept(x.getIterExpr());
  }
  space();
  p.print(CHARS_IN);
  space();
  accept(x.getObjExpr());
  rightParen();
  _nestedPush(x.getBody());
  accept(x.getBody());
  _nestedPop(x.getBody());
}
