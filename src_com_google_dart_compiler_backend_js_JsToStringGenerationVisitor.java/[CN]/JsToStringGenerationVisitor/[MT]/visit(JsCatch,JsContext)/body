{
  spaceOpt();
  _catch();
  spaceOpt();
  leftParen();
  nameDef(x.getParameter().getName());
  JsExpression catchCond=x.getCondition();
  if (catchCond != null) {
    space();
    _if();
    space();
    accept(catchCond);
  }
  rightParen();
  spaceOpt();
  accept(x.getBody());
  return false;
}
