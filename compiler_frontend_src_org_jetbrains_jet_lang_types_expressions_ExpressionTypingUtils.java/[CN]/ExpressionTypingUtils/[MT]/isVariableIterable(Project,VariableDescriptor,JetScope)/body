{
  JetExpression expression=JetPsiFactory.createExpression(project,"fake");
  ExpressionReceiver expressionReceiver=new ExpressionReceiver(expression,variableDescriptor.getOutType());
  ExpressionTypingContext context=ExpressionTypingContext.newContext(JetSemanticServices.createSemanticServices(project),new HashMap<JetPattern,DataFlowInfo>(),new HashMap<JetPattern,List<VariableDescriptor>>(),new LabelResolver(),new BindingTraceContext(),scope,DataFlowInfo.EMPTY,TypeUtils.NO_EXPECTED_TYPE,TypeUtils.NO_EXPECTED_TYPE,false);
  return ControlStructureTypingVisitor.checkIterableConvention(expressionReceiver,context) != null;
}
