{
  final int sort=receiver.type.getSort();
  final Type type=sort == Type.OBJECT || sort == Type.ARRAY ? AsmTypeConstants.OBJECT_TYPE : sort == Type.BYTE || sort == Type.SHORT ? Type.INT_TYPE : receiver.type;
  receiver.put(type,v);
  v.invokestatic("java/lang/String","valueOf","(" + type.getDescriptor() + ")Ljava/lang/String;");
  return StackValue.onStack(JAVA_STRING_TYPE);
}
