{
  Collection<FunctionDescriptor> functions=owner.getDefaultType().getMemberScope().getFunctions(name);
  for (  FunctionDescriptor function : functions) {
    if (function.getKind() == CallableMemberDescriptor.Kind.DECLARATION && function.getTypeParameters().isEmpty() && valueParameterClassesMatch(function.getValueParameters(),Arrays.asList(valueParameterClasses)) && rawTypeMatches(function.getReturnType(),returnedClass)) {
      return function;
    }
  }
  return null;
}
