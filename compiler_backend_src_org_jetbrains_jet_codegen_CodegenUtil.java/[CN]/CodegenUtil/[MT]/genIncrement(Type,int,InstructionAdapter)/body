{
  if (expectedType == Type.LONG_TYPE) {
    v.lconst(myDelta);
  }
 else   if (expectedType == Type.FLOAT_TYPE) {
    v.fconst(myDelta);
  }
 else   if (expectedType == Type.DOUBLE_TYPE) {
    v.dconst(myDelta);
  }
 else {
    v.iconst(myDelta);
    expectedType=Type.INT_TYPE;
  }
  v.add(expectedType);
  return expectedType;
}
