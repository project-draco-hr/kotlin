{
  int arity=fd.getValueParameters().size();
  SimpleFunctionDescriptorImpl invokeDescriptor=new SimpleFunctionDescriptorImpl(fd.getExpectedThisObject() != null ? KotlinBuiltIns.getInstance().getExtensionFunction(arity) : KotlinBuiltIns.getInstance().getFunction(arity),Annotations.EMPTY,Name.identifier("invoke"),CallableMemberDescriptor.Kind.DECLARATION);
  invokeDescriptor.initialize(DescriptorUtils.getReceiverParameterType(fd.getReceiverParameter()),fd.getExpectedThisObject(),Collections.<TypeParameterDescriptorImpl>emptyList(),fd.getValueParameters(),fd.getReturnType(),Modality.FINAL,Visibilities.PUBLIC);
  return invokeDescriptor;
}
