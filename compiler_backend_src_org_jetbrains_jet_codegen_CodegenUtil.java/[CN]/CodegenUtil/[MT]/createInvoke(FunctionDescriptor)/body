{
  int arity=fd.getValueParameters().size();
  NamedFunctionDescriptorImpl invokeDescriptor=new NamedFunctionDescriptorImpl(fd.getExpectedThisObject().exists() ? JetStandardClasses.getReceiverFunction(arity) : JetStandardClasses.getFunction(arity),Collections.<AnnotationDescriptor>emptyList(),"invoke",CallableMemberDescriptor.Kind.DECLARATION);
  invokeDescriptor.initialize(fd.getReceiverParameter().exists() ? fd.getReceiverParameter().getType() : null,fd.getExpectedThisObject(),Collections.<TypeParameterDescriptor>emptyList(),fd.getValueParameters(),fd.getReturnType(),Modality.FINAL,Visibility.PUBLIC);
  return invokeDescriptor;
}
