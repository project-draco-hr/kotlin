{
  BindingContext jetContext=WholeProjectAnalyzerFacade.getLazyResolveContext((JetFile)parameters.getPosition().getContainingFile(),null);
  if (parameters.getInvocationCount() > 0 || parameters.getCompletionType() == CompletionType.CLASS_NAME) {
    JetClassCompletionContributor.addClasses(parameters,result,jetContext,new Consumer<LookupElement>(){
      @Override public void consume(      @NotNull LookupElement element){
        result.addElement(element);
      }
    }
);
  }
  result.stopHere();
}
