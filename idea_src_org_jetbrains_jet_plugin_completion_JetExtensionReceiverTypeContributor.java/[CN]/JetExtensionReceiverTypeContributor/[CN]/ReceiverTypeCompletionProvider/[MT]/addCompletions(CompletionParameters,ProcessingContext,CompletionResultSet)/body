{
  ResolveSession resolveSession=WholeProjectAnalyzerFacade.getLazyResolveSessionForFile((JetFile)parameters.getPosition().getContainingFile());
  if (parameters.getInvocationCount() > 0) {
    JetClassCompletionContributor.addClasses(parameters,result,resolveSession.getBindingContext(),resolveSession,new Consumer<LookupElement>(){
      @Override public void consume(      @NotNull LookupElement element){
        result.addElement(element);
      }
    }
);
  }
  result.stopHere();
}
