{
switch (kind) {
case DECLARATION:
    return JvmStdlibNames.FLAG_METHOD_KIND_DECLARATION;
case FAKE_OVERRIDE:
  return JvmStdlibNames.FLAG_METHOD_KIND_FAKE_OVERRIDE;
case DELEGATION:
return JvmStdlibNames.FLAG_METHOD_KIND_DELEGATION;
case SYNTHESIZED:
return JvmStdlibNames.FLAG_METHOD_KIND_SYNTHESIZED;
default :
throw new IllegalArgumentException("Unknown kind: " + kind);
}
}
