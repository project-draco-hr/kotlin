{
switch (value & JvmStdlibNames.FLAG_METHOD_KIND_MASK) {
case JvmStdlibNames.FLAG_METHOD_KIND_DECLARATION:
    return CallableMemberDescriptor.Kind.DECLARATION;
case JvmStdlibNames.FLAG_METHOD_KIND_FAKE_OVERRIDE:
  return CallableMemberDescriptor.Kind.FAKE_OVERRIDE;
case JvmStdlibNames.FLAG_METHOD_KIND_DELEGATION:
return CallableMemberDescriptor.Kind.DELEGATION;
case JvmStdlibNames.FLAG_METHOD_KIND_SYNTHESIZED:
return CallableMemberDescriptor.Kind.SYNTHESIZED;
default :
throw new IllegalArgumentException("Unknown int value of kind: " + value);
}
}
