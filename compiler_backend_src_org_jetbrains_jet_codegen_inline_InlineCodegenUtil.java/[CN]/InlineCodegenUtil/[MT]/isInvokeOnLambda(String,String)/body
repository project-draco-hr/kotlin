{
  if (!INVOKE.equals(name)) {
    return false;
  }
  for (  String prefix : Arrays.asList("jet/Function","jet/ExtensionFunction")) {
    if (owner.startsWith(prefix)) {
      String suffix=owner.substring(prefix.length());
      if (isInteger(suffix)) {
        return true;
      }
    }
  }
  return false;
}
