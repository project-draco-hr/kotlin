{
  BindingTraceContext bindingTraceContext=new BindingTraceContext();
  Project project=file.getProject();
  AnalyzingUtils.analyzeFilesWithGivenTrace(project,JsConfiguration.jsLibConfiguration(project),Collections.singletonList(file),Predicates.<PsiFile>alwaysTrue(),JetControlFlowDataTraceFactory.EMPTY,bindingTraceContext);
  return bindingTraceContext.getBindingContext();
}
