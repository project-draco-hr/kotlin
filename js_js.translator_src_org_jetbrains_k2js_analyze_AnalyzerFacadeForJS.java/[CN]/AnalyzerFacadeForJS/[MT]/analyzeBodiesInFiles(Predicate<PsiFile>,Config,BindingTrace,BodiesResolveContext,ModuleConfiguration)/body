{
  Predicate<PsiFile> completely=Predicates.and(notLibFiles(config.getLibFiles()),filesToAnalyzeCompletely);
  return AnalyzerFacadeForEverything.analyzeBodiesInFilesWithJavaIntegration(config.getProject(),Collections.<AnalyzerScriptParameter>emptyList(),completely,traceContext,bodiesResolveContext,configuration);
}
