{
  BindingTraceContext bindingTraceContext=new BindingTraceContext();
  return AnalyzingUtils.analyzeFilesWithGivenTrace(project,JavaBridgeConfiguration.createJavaBridgeConfiguration(project,bindingTraceContext,StandardConfiguration.createStandardConfiguration(project)),files,filesToAnalyzeCompletely,flowDataTraceFactory,bindingTraceContext);
}
