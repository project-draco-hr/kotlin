{
  String assertTestsPresentStr=String.format("JetTestUtils.assertAllTestsPresentByMetadata(this.getClass(), \"%s\", new File(\"%s\"), Pattern.compile(\"%s\"), %s);",generatorClassFqName,JetTestUtils.getFilePath(rootFile),StringUtil.escapeStringCharacters(filenamePattern.pattern()),recursive);
