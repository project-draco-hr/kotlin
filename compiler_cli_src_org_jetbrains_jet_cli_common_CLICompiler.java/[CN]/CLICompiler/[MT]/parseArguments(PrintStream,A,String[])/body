{
  try {
    arguments.freeArgs=Args.parse(arguments,args);
    return true;
  }
 catch (  IllegalArgumentException e) {
    errStream.println(e.getMessage());
    usage(errStream,false);
  }
catch (  Throwable t) {
    errStream.println(MessageRenderer.TAGS.render(CompilerMessageSeverity.EXCEPTION,OutputMessageUtil.renderException(t),CompilerMessageLocation.NO_LOCATION));
  }
  return false;
}
