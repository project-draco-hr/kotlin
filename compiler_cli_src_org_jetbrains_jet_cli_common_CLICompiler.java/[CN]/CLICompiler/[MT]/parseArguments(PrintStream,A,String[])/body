{
  try {
    arguments.freeArgs=Args.parse(arguments,args);
    if (arguments.freeArgs == null) {
      arguments.freeArgs=Collections.emptyList();
    }
    return true;
  }
 catch (  IllegalArgumentException e) {
    usage(errStream);
  }
catch (  Throwable t) {
    errStream.println(MessageRenderer.TAGS.renderException(t));
  }
  return false;
}
