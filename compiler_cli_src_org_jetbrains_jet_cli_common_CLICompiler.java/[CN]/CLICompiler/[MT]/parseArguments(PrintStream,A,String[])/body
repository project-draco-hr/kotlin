{
  try {
    arguments.freeArgs=Args.parse(arguments,args);
    checkArguments(arguments);
    return true;
  }
 catch (  IllegalArgumentException e) {
    errStream.println(e.getMessage());
    usage(errStream);
  }
catch (  Throwable t) {
    errStream.println(MessageRenderer.TAGS.renderException(t));
  }
  return false;
}
