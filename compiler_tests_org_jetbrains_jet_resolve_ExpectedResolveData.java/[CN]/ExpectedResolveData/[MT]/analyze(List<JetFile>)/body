{
  if (files.isEmpty()) {
    System.err.println("Suspicious: no files");
    return BindingContext.EMPTY;
  }
  Project project=files.iterator().next().getProject();
  AnalyzeExhaust analyzeExhaust=AnalyzerFacadeForJVM.analyzeFilesWithJavaIntegration(project,files,Collections.<AnalyzerScriptParameter>emptyList(),Predicates.<PsiFile>alwaysTrue());
  return analyzeExhaust.getBindingContext();
}
