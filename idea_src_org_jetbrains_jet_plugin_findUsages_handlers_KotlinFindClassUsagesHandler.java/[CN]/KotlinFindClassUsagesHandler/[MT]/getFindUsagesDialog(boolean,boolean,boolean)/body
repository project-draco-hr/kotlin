{
  PsiClass lightClass=LightClassUtil.getPsiClass(getElement());
  if (lightClass != null) {
    return new KotlinFindClassUsagesDialog(lightClass,getProject(),getFactory().getFindClassOptions(),toShowInNewTab,mustOpenInNewTab,isSingleFile,this);
  }
  return super.getFindUsagesDialog(isSingleFile,toShowInNewTab,mustOpenInNewTab);
}
