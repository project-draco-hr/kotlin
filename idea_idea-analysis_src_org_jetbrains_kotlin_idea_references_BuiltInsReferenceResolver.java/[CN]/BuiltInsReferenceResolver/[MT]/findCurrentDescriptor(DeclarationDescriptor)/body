{
  if (originalDescriptor instanceof ClassDescriptor) {
    return isFromBuiltinModule(originalDescriptor) ? findClassAcrossModuleDependencies(moduleDescriptor,getClassId((ClassDescriptor)originalDescriptor)) : null;
  }
 else   if (originalDescriptor instanceof PackageFragmentDescriptor) {
    return isFromBuiltinModule(originalDescriptor) && KotlinBuiltIns.BUILT_INS_PACKAGE_FQ_NAME.equals(((PackageFragmentDescriptor)originalDescriptor).getFqName()) ? builtinsPackageFragment : null;
  }
 else   if (originalDescriptor instanceof MemberDescriptor) {
    return findCurrentDescriptorForMember((MemberDescriptor)originalDescriptor);
  }
 else {
    return null;
  }
}
