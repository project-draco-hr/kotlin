{
  JetType type=null;
  for (  PsiElement child : getChildren()) {
    IElementType elementType=child.getNode().getElementType();
    if (elementType == JetNodeTypes.TYPE_CODE_FRAGMENT) {
      for (      PsiElement grChild : child.getChildren()) {
        if (grChild instanceof JetTypeReference) {
          if (!grChild.getText().isEmpty())           type=KotlinBuiltIns.getInstance().getAnyType();
        }
 else         if (!JetTokens.WHITE_SPACE_OR_COMMENT_BIT_SET.contains(elementType))         return null;
      }
    }
 else     if (!JetTokens.WHITE_SPACE_OR_COMMENT_BIT_SET.contains(elementType))     return null;
  }
  return type;
}
