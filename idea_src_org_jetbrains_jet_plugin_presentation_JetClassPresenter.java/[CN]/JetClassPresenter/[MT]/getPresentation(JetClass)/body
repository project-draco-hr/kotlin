{
  return new ColoredItemPresentation(){
    @Override public TextAttributesKey getTextAttributesKey(){
      return null;
    }
    @Override public String getPresentableText(){
      return item.getName();
    }
    @Override public String getLocationString(){
      FqName name=JetPsiUtil.getFQName(item);
      if (name != null) {
        return "(" + name.parent().toString() + ")";
      }
      return null;
    }
    @Override public Icon getIcon(    boolean unused){
      return JetIconProvider.INSTANCE.getIcon(item,0);
    }
  }
;
}
