{
switch (severity) {
case EXCEPTION:
    return Ansi.Color.RED;
case ERROR:
  return Ansi.Color.RED;
case WARNING:
return Ansi.Color.YELLOW;
case INFO:
return Ansi.Color.BLUE;
case LOGGING:
return Ansi.Color.BLUE;
case OUTPUT:
return Ansi.Color.BLUE;
default :
throw new UnsupportedOperationException("Unknown severity: " + severity);
}
}
