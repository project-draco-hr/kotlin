{
  if (receiverValue != null)   return receiverValue;
  if (receiverType != null) {
    return StackValue.field(receiverType,Type.getObjectType(codegen.getParentCodegen().getClassName()),AsmUtil.CAPTURED_RECEIVER_FIELD,false,StackValue.LOCAL_0);
  }
  return StackValue.local(1,signature.getAsmMethod().getArgumentTypes()[0]);
}
