{
  List<String> sortedKeys=new ArrayList<String>(groupedMessages.keySet());
  Collections.sort(sortedKeys,new Comparator<String>(){
    @Override public int compare(    String o1,    String o2){
      if (o1 == o2)       return 0;
      if (o1 == null)       return -1;
      if (o2 == null)       return 1;
      return o1.compareTo(o2);
    }
  }
);
  return sortedKeys;
}
