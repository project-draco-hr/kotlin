{
  JsStatement bodyCopy=AstUtil.deepCopy(body);
  JsExpression conditionCopy=AstUtil.deepCopy(condition);
  JsExpression incrementalExprCopy=AstUtil.deepCopy(incrementExpression);
  if (initVars != null) {
    return new JsFor(initVars.deepCopy(),conditionCopy,incrementalExprCopy,bodyCopy);
  }
 else {
    return new JsFor(initExpression.deepCopy(),conditionCopy,incrementExpression,bodyCopy);
  }
}
