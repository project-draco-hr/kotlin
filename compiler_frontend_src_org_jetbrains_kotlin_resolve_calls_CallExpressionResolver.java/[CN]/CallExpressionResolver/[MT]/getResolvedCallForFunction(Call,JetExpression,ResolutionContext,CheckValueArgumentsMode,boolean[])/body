{
  OverloadResolutionResults<FunctionDescriptor> results=callResolver.resolveFunctionCall(BasicCallResolutionContext.create(context,call,checkArguments));
  if (!results.isNothing()) {
    result[0]=true;
    return OverloadResolutionResultsUtil.getResultingCall(results,context.contextDependency);
  }
  result[0]=false;
  return null;
}
