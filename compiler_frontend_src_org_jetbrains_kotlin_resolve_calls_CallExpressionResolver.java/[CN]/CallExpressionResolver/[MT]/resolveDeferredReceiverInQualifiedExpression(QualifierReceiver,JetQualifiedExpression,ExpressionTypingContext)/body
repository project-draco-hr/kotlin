{
  if (qualifierReceiver == null)   return;
  JetExpression calleeExpression=JetPsiUtil.deparenthesize(CallUtilPackage.getCalleeExpressionIfAny(qualifiedExpression.getSelectorExpression()));
  DeclarationDescriptor selectorDescriptor=calleeExpression instanceof JetReferenceExpression ? context.trace.get(BindingContext.REFERENCE_TARGET,(JetReferenceExpression)calleeExpression) : null;
  ReceiversPackage.resolveAsReceiverInQualifiedExpression(qualifierReceiver,context,symbolUsageValidator,selectorDescriptor);
}
