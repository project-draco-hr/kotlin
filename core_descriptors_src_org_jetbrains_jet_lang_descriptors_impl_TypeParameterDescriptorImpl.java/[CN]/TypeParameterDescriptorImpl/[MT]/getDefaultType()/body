{
  if (defaultType == null) {
    defaultType=new JetTypeImpl(Annotations.EMPTY,getTypeConstructor(),TypeUtils.hasNullableLowerBound(this),Collections.<TypeProjection>emptyList(),new LazyScopeAdapter(NO_LOCKS.createLazyValue(new Function0<JetScope>(){
      @Override public JetScope invoke(){
        return getUpperBoundsAsType().getMemberScope();
      }
    }
)));
  }
  return defaultType;
}
