{
  if (defaultType == null) {
    defaultType=new JetTypeImpl(Collections.<AnnotationDescriptor>emptyList(),getTypeConstructor(),TypeUtils.hasNullableLowerBound(this),Collections.<TypeProjection>emptyList(),new LazyScopeAdapter(new RecursionIntolerantLazyValue<JetScope>(){
      @Override protected JetScope compute(){
        return getUpperBoundsAsType().getMemberScope();
      }
    }
));
  }
  return defaultType;
}
