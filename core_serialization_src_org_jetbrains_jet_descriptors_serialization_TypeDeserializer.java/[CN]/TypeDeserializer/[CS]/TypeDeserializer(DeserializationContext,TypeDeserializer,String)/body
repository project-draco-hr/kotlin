{
  this.parent=parent;
  this.debugName=debugName + (parent == null ? "" : ". Child of " + parent.debugName);
  this.context=context;
  this.classDescriptors=getComponents().getStorageManager().createMemoizedFunctionWithNullableValues(new Function1<Integer,ClassDescriptor>(){
    @Override public ClassDescriptor invoke(    Integer fqNameIndex){
      return computeClassDescriptor(fqNameIndex);
    }
  }
);
}
