{
  for (  Map.Entry<JetClassOrObject,MutableClassDescriptor> entry : context.getClasses().entrySet()) {
    JetClassOrObject classOrObject=entry.getKey();
    MutableClassDescriptor descriptor=entry.getValue();
    resolveAnonymousInitializers(classOrObject,descriptor.getUnsubstitutedPrimaryConstructor(),descriptor.getScopeForInitializers());
  }
}
