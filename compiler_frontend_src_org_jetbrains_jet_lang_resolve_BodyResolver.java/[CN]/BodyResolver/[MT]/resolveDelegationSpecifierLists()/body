{
  for (  Map.Entry<JetClassOrObject,MutableClassDescriptor> entry : context.getClasses().entrySet()) {
    JetClassOrObject classOrObject=entry.getKey();
    MutableClassDescriptor descriptor=entry.getValue();
    resolveDelegationSpecifierList(classOrObject,descriptor,descriptor.getUnsubstitutedPrimaryConstructor(),descriptor.getScopeForSupertypeResolution(),descriptor.getScopeForMemberResolution());
  }
}
