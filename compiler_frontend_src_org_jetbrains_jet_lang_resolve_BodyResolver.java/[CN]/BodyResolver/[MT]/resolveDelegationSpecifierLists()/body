{
  for (  Map.Entry<JetClassOrObject,ClassDescriptorWithResolutionScopes> entry : context.getClasses().entrySet()) {
    JetClassOrObject classOrObject=entry.getKey();
    ClassDescriptorWithResolutionScopes descriptor=entry.getValue();
    resolveDelegationSpecifierList(classOrObject,descriptor,descriptor.getUnsubstitutedPrimaryConstructor(),descriptor.getScopeForClassHeaderResolution(),descriptor.getScopeForMemberDeclarationResolution());
  }
}
