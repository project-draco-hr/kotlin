{
  List<JetFile> files=loadResourcesAsJetFiles(project,LIBRARY_FILES);
  return new ResolveSession(project,builtInsModule,new SpecialModuleConfiguration(project),new FileBasedDeclarationProviderFactory(files),new Function<FqName,Name>(){
    @Override public Name fun(    FqName name){
      return ALIASES.get(name);
    }
  }
,Predicates.in(Sets.newHashSet(new FqNameUnsafe("jet.Any"),new FqNameUnsafe("jet.Nothing"))));
}
