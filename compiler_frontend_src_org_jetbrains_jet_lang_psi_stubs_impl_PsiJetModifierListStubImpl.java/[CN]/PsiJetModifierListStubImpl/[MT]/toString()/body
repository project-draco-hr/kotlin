{
  StringBuilder sb=new StringBuilder();
  sb.append(super.toString());
  sb.append("[");
  boolean first=true;
  for (  JetModifierKeywordToken modifierKeyword : MODIFIER_KEYWORDS_ARRAY) {
    if (hasModifier(modifierKeyword)) {
      if (!first) {
        sb.append(" ");
      }
      sb.append(modifierKeyword.getValue());
      first=false;
    }
  }
  sb.append("]");
  return sb.toString();
}
