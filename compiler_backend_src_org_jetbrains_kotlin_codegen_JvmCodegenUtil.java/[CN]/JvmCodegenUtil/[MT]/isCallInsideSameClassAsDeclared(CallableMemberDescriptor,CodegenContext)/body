{
  boolean isFakeOverride=descriptor.getKind() == CallableMemberDescriptor.Kind.FAKE_OVERRIDE;
  boolean isDelegate=descriptor.getKind() == CallableMemberDescriptor.Kind.DELEGATION;
  DeclarationDescriptor containingDeclaration=descriptor.getContainingDeclaration().getOriginal();
  return !isFakeOverride && !isDelegate && (((context.hasThisDescriptor() && containingDeclaration == context.getThisDescriptor()) || (context.getParentContext() instanceof PackageContext && isSamePackageInSameModule(context.getParentContext().getContextDescriptor(),containingDeclaration))) && context.getContextKind() != OwnerKind.DEFAULT_IMPLS);
}
