{
  Project project=getPosition().getProject();
  JetShortNamesCache namesCache=JetShortNamesCache.getKotlinInstance(project);
  Collection<DeclarationDescriptor> jetCallableExtensions=namesCache.getJetCallableExtensions(jetResult.getShortNameFilter(),jetReference.getExpression(),getResolveSession(),GlobalSearchScope.allScope(project));
  jetResult.addAllElements(jetCallableExtensions);
}
