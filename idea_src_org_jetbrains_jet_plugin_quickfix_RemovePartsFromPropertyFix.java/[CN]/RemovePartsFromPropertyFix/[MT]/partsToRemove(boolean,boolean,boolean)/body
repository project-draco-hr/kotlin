{
  StringBuilder sb=new StringBuilder();
  if (hasGetter) {
    sb.append("getter");
    if (hasSetter && hasInitializer) {
      sb.append(", ");
    }
 else     if (hasSetter || hasInitializer) {
      sb.append(" and ");
    }
  }
  if (hasSetter) {
    sb.append("setter");
    if (hasInitializer) {
      sb.append(" and ");
    }
  }
  if (hasInitializer) {
    sb.append("initializer");
  }
  return sb.toString();
}
