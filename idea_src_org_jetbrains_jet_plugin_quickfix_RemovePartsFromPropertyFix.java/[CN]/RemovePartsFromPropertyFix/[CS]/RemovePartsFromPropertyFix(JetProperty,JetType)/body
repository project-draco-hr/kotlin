{
  super(element);
  if (type instanceof DeferredType) {
    this.type=((DeferredType)type).getActualType();
  }
 else {
    this.type=type;
  }
  partsToRemove=partsToRemove(element.getGetter() != null && element.getGetter().getBodyExpression() != null,element.getSetter() != null && element.getSetter().getBodyExpression() != null,element.getInitializer() != null);
}
