{
  LexicalScope accessorScope=JetScopeUtils.makeScopeForPropertyAccessor(propertyDescriptor,scope,trace);
  KotlinType type=delegatedPropertyResolver.resolveDelegateExpression(delegateExpression,property,propertyDescriptor,scope,accessorScope,trace,dataFlowInfo);
  if (type != null) {
    KotlinType getterReturnType=delegatedPropertyResolver.getDelegatedPropertyGetMethodReturnType(propertyDescriptor,delegateExpression,type,trace,accessorScope);
    if (getterReturnType != null) {
      return getterReturnType;
    }
  }
  return ErrorUtils.createErrorType("Type from delegate");
}
