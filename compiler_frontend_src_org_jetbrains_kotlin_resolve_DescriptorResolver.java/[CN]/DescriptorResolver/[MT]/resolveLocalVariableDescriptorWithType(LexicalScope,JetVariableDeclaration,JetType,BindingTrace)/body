{
  LocalVariableDescriptor variableDescriptor=new LocalVariableDescriptor(scope.getOwnerDescriptor(),annotationResolver.resolveAnnotationsWithArguments(scope,variable.getModifierList(),trace),JetPsiUtil.safeName(variable.getName()),type,variable.isVar(),toSourceElement(variable));
  trace.record(BindingContext.VARIABLE,variable,variableDescriptor);
  return variableDescriptor;
}
