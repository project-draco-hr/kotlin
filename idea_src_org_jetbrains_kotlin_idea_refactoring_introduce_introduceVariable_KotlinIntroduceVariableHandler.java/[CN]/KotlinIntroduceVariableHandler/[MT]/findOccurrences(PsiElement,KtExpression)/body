{
  return CollectionsKt.map(JetPsiRangeKt.toRange(originalExpression).match(occurrenceContainer,JetPsiUnifier.DEFAULT),new Function1<JetPsiRange.Match,KtExpression>(){
    @Override public KtExpression invoke(    JetPsiRange.Match match){
      PsiElement candidate=((JetPsiRange.ListRange)match.getRange()).getStartElement();
      if (candidate instanceof KtExpression)       return (KtExpression)candidate;
      if (candidate instanceof KtStringTemplateEntryWithExpression)       return ((KtStringTemplateEntryWithExpression)candidate).getExpression();
      throw new AssertionError("Unexpected candidate element: " + candidate.getText());
    }
  }
);
}
