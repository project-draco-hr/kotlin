{
  final String callOperator=myQualifier.isNullable() ? QUESTDOT : DOT;
  final String qualifier=myQualifier.isEmpty() ? EMPTY : myQualifier.toKotlin() + callOperator;
  return myAnonymousClass != null ? "object" + SPACE + ":"+ SPACE+ qualifier+ myName.toKotlin()+ "("+ myArguments.toKotlin()+ ")"+ myAnonymousClass.toKotlin() : qualifier + myName.toKotlin() + "("+ myArguments.toKotlin()+ ")";
}
