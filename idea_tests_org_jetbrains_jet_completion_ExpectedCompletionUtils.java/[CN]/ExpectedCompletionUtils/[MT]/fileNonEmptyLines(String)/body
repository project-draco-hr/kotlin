{
  ArrayList<String> result=new ArrayList<String>();
  try {
    BufferedReader reader=new BufferedReader(new StringReader(fileText));
    try {
      String line;
      while ((line=reader.readLine()) != null) {
        if (!line.isEmpty()) {
          result.add(line.trim());
        }
      }
    }
  finally {
      reader.close();
    }
  }
 catch (  IOException e) {
    throw new AssertionError(e);
  }
  return result;
}
