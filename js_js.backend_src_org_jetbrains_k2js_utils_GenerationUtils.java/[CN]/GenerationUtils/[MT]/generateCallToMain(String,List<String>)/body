{
  String constructArguments="var args = [];\n";
  int index=0;
  for (  String argument : arguments) {
    constructArguments=constructArguments + "args[" + index+ "]= \""+ argument+ "\";\n";
    index++;
  }
  String callMain=namespaceName + ".main(args);\n";
  return constructArguments + callMain;
}
