{
  Object _value=value;
  if (_value != null)   return Nulls.unescape(_value);
synchronized (lock) {
    _value=value;
    if (_value != null)     return Nulls.unescape(_value);
    T typedValue=computable.compute();
    value=Nulls.escape(typedValue);
    postCompute(typedValue);
    return typedValue;
  }
}
