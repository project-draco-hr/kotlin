{
  try {
    String actual=normalize(executeCompilerGrabOutput(args)).replace(new File("compiler/testData/cli/").getAbsolutePath(),"$TESTDATA_DIR$").replace("\\","/");
    String expected=normalize(FileUtil.loadFile(new File("compiler/testData/cli/" + testName.getMethodName() + ".out")));
    Assert.assertEquals(expected,actual);
  }
 catch (  Exception e) {
    throw ExceptionUtils.rethrow(e);
  }
}
