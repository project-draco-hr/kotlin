{
  try {
    StringBuilder sb=new StringBuilder();
    BufferedReader reader=new BufferedReader(new StringReader(input));
    while (true) {
      String line=reader.readLine();
      if (line == null) {
        return sb.toString();
      }
      sb.append(line + "\n");
    }
  }
 catch (  Exception e) {
    throw ExceptionUtils.rethrow(e);
  }
}
