{
  String className=element.getAttributeNS(ANDROID_URI,ATTR_NAME);
  if (className.startsWith(".")) {
    return context.getMainProject().getPackage() + className;
  }
 else   if (className.indexOf('.') == -1) {
    return context.getMainProject().getPackage() + '.' + className;
  }
  return className;
}
