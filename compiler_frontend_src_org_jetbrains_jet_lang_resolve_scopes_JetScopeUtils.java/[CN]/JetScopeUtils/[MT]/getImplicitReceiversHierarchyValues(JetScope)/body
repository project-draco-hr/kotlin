{
  Collection<ReceiverParameterDescriptor> hierarchy=scope.getImplicitReceiversHierarchy();
  return Lists.newArrayList(Collections2.transform(hierarchy,new Function<ReceiverParameterDescriptor,ReceiverDescriptor>(){
    @Override public ReceiverDescriptor apply(    ReceiverParameterDescriptor receiverParameterDescriptor){
      return receiverParameterDescriptor.getValue();
    }
  }
));
}
