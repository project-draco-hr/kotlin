{
  WritableScopeImpl thisScope=new WritableScopeImpl(JetScope.Empty.INSTANCE$,this,RedeclarationHandler.DO_NOTHING,"Scope with 'this' for " + getName(),this.getThisAsReceiverParameter(),this);
  thisScope.changeLockLevel(WritableScope.LockLevel.READING);
  return new ChainedScope(this,"ScopeForMemberDeclarationResolution: " + getName(),thisScope,getScopeForMemberLookup(),getScopeForClassHeaderResolution(),getCompanionObjectScope(),getStaticScope());
}
