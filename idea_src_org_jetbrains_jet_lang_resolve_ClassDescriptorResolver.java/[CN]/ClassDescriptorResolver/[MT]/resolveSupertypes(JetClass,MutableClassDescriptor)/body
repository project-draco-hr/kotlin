{
  List<JetDelegationSpecifier> delegationSpecifiers=jetClass.getDelegationSpecifiers();
  Collection<? extends JetType> superclasses=delegationSpecifiers.isEmpty() ? Collections.singleton(JetStandardClasses.getAnyType()) : resolveDelegationSpecifiers(descriptor.getScopeForMemberResolution(),delegationSpecifiers,typeResolverNotCheckingBounds);
  ((TypeConstructorImpl)descriptor.getTypeConstructor()).getSupertypes().addAll(superclasses);
  for (  JetType superclass : superclasses) {
    descriptor.getScopeForMemberResolution().importScope(superclass.getMemberScope());
  }
}
