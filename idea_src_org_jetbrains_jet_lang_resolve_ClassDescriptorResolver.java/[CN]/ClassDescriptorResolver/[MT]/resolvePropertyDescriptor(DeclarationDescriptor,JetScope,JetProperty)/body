{
  JetType type=getType(scope,property);
  VariableDescriptorImpl propertyDescriptor=new PropertyDescriptor(containingDeclaration,AttributeResolver.INSTANCE.resolveAttributes(property.getModifierList()),JetPsiUtil.safeName(property.getName()),property.isVar() ? type : null,type);
  trace.recordDeclarationResolution(property,propertyDescriptor);
  return propertyDescriptor;
}
