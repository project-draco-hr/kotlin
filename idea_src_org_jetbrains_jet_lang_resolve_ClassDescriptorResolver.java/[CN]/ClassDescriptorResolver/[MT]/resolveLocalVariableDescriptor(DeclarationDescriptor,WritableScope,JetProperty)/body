{
  JetType type=getType(scope,property);
  VariableDescriptorImpl propertyDescriptor=new LocalVariableDescriptor(containingDeclaration,AttributeResolver.INSTANCE.resolveAttributes(property.getModifierList()),JetPsiUtil.safeName(property.getName()),type,property.isVar());
  trace.recordDeclarationResolution(property,propertyDescriptor);
  return propertyDescriptor;
}
