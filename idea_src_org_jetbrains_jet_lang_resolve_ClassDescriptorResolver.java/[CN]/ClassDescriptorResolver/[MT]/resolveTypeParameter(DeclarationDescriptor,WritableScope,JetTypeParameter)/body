{
  JetTypeReference extendsBound=typeParameter.getExtendsBound();
  TypeParameterDescriptor typeParameterDescriptor=new TypeParameterDescriptor(containingDescriptor,AttributeResolver.INSTANCE.resolveAttributes(typeParameter.getModifierList()),typeParameter.getVariance(),typeParameter.getName(),extendsBound == null ? Collections.<JetType>singleton(JetStandardClasses.getAnyType()) : Collections.singleton(typeResolver.resolveType(extensibleScope,extendsBound)));
  extensibleScope.addTypeParameterDescriptor(typeParameterDescriptor);
  trace.recordDeclarationResolution(typeParameter,typeParameterDescriptor);
  return typeParameterDescriptor;
}
