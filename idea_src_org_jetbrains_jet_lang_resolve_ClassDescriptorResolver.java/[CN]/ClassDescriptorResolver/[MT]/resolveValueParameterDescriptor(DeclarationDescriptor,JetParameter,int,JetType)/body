{
  MutableValueParameterDescriptor valueParameterDescriptor=new ValueParameterDescriptorImpl(declarationDescriptor,index,AnnotationResolver.INSTANCE.resolveAnnotations(valueParameter.getModifierList()),JetPsiUtil.safeName(valueParameter.getName()),valueParameter.isMutable() ? type : null,type,valueParameter.getDefaultValue() != null,false);
  trace.recordDeclarationResolution(valueParameter,valueParameterDescriptor);
  return valueParameterDescriptor;
}
