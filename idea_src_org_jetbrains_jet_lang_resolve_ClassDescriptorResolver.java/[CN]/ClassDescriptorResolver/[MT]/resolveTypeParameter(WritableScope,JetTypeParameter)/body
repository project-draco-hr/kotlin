{
  JetTypeReference extendsBound=typeParameter.getExtendsBound();
  TypeParameterDescriptor typeParameterDescriptor=new TypeParameterDescriptor(typeParameter,AttributeResolver.INSTANCE.resolveAttributes(typeParameter.getModifierList()),typeParameter.getVariance(),typeParameter.getName(),extendsBound == null ? Collections.<Type>singleton(JetStandardClasses.getAnyType()) : Collections.singleton(TypeResolver.INSTANCE.resolveType(extensibleScope,extendsBound)));
  extensibleScope.addTypeParameterDescriptor(typeParameterDescriptor);
  return typeParameterDescriptor;
}
