{
  JetType type=getParameterType(scope,parameter);
  String name=parameter.getName();
  PropertyDescriptorImpl propertyDescriptor=new PropertyDescriptorImpl(classDescriptor,AttributeResolver.INSTANCE.resolveAttributes(parameter.getModifierList()),name == null ? "<no name>" : name,parameter.isMutable() ? type : null,type);
  trace.recordDeclarationResolution(parameter,propertyDescriptor);
  return propertyDescriptor;
}
