{
  AbstractResourceRepository resources=context.getClient().getProjectResources(context.getProject(),true);
  if (resources != null) {
    List<ResourceItem> items=resources.getResourceItem(ResourceType.ID,stripIdPrefix(id));
    if (items == null || items.isEmpty()) {
      return false;
    }
    for (    ResourceItem item : items) {
      ResourceFile source=item.getSource();
      if (source != null) {
        File file=source.getFile();
        if (file.getParentFile().getName().startsWith(FD_RES_VALUES)) {
          if (mDeclaredIds == null) {
            mDeclaredIds=Sets.newHashSet();
          }
          mDeclaredIds.add(id);
          continue;
        }
        if (!isSameResourceFile(file,notIn)) {
          return true;
        }
      }
    }
  }
  return false;
}
