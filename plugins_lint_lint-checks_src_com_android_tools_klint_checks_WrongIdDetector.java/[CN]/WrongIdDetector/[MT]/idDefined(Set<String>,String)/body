{
  if (ids == null) {
    return false;
  }
  boolean definedLocally=ids.contains(id);
  if (!definedLocally) {
    if (id.startsWith(NEW_ID_PREFIX)) {
      definedLocally=ids.contains(ID_PREFIX + id.substring(NEW_ID_PREFIX.length()));
    }
 else     if (id.startsWith(ID_PREFIX)) {
      definedLocally=ids.contains(NEW_ID_PREFIX + id.substring(ID_PREFIX.length()));
    }
  }
  return definedLocally;
}
