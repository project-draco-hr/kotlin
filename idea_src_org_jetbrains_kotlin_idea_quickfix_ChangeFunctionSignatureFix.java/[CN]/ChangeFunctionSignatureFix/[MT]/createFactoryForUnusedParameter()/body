{
  return new JetSingleIntentionActionFactory(){
    @Override public ChangeFunctionSignatureFix createAction(    @NotNull Diagnostic diagnostic){
      @SuppressWarnings("unchecked") Object descriptor=UNUSED_PARAMETER.cast(diagnostic).getA();
      if (descriptor instanceof ValueParameterDescriptor) {
        return createFix(null,diagnostic.getPsiElement(),(CallableDescriptor)descriptor);
      }
 else {
        return null;
      }
    }
  }
;
}
