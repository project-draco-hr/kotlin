{
  JetCoreEnvironment environment=getEnvironment(ktFile);
  Project project=environment.getProject();
  List<JetFile> jetFiles=Collections.singletonList(JetTestUtils.loadJetFile(project,ktFile));
  return AnalyzerFacadeForJVM.analyzeFilesWithJavaIntegration(project,jetFiles,Collections.<AnalyzerScriptParameter>emptyList(),Predicates.<PsiFile>alwaysTrue()).getBindingContext();
}
