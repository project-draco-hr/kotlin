{
  candidateCall.getDataFlowInfoForArguments().setInitialDataFlowInfo(context.dataFlowInfo);
  return new CallCandidateResolutionContext<D>(candidateCall,tracing,trace,context.scope,call,context.expectedType,context.dataFlowInfo,context.contextDependency,context.checkArguments,context.resolutionResultsCache,context.dataFlowInfoForArguments,context.callChecker,context.additionalTypeChecker,context.statementFilter,explicitExtensionReceiverForInvoke,context.isAnnotationContext,context.collectAllCandidates,context.insideCallChain);
}
