{
  return new CallCandidateResolutionContext<D>(candidateCall,tracing,trace,scope,call,expectedType,dataFlowInfo,contextDependency,checkArguments,resolutionResultsCache,dataFlowInfoForArguments,statementFilter,candidateResolveMode,isAnnotationContext,isDebuggerContext,collectAllCandidates,callPosition,expressionContextProvider);
}
