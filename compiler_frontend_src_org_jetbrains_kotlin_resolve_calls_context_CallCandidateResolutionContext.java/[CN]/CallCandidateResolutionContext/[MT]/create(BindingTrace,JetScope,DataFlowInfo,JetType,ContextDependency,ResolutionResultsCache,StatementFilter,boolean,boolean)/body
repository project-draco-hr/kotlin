{
  return new CallCandidateResolutionContext<D>(candidateCall,tracing,trace,scope,call,expectedType,dataFlowInfo,contextDependency,checkArguments,resolutionResultsCache,dataFlowInfoForArguments,callChecker,symbolUsageValidator,additionalTypeChecker,statementFilter,explicitExtensionReceiverForInvoke,candidateResolveMode,isAnnotationContext,collectAllCandidates,insideSafeCallChain);
