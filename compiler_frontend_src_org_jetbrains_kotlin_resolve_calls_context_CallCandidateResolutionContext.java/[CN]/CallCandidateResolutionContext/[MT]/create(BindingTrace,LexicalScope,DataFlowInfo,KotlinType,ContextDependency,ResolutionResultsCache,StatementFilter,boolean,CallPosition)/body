{
  return new CallCandidateResolutionContext<D>(candidateCall,tracing,trace,scope,call,expectedType,dataFlowInfo,contextDependency,checkArguments,resolutionResultsCache,dataFlowInfoForArguments,callChecker,statementFilter,explicitExtensionReceiverForInvoke,candidateResolveMode,isAnnotationContext,isDebuggerContext,collectAllCandidates,callPosition);
