{
  JetChangeInfo changeInfo=getChangeInfo();
  changeInfo.setNewVisibility(Visibilities.PROTECTED);
  JetPsiFactory psiFactory=new JetPsiFactory(getProject());
  JetParameterInfo newParameter=new JetParameterInfo(changeInfo.getMethodDescriptor().getBaseDescriptor(),-1,"x",BUILT_INS.getAnyType(),null,psiFactory.createExpression("12"),JetValVar.Val,null);
  changeInfo.addParameter(newParameter);
  doTest(changeInfo);
}
