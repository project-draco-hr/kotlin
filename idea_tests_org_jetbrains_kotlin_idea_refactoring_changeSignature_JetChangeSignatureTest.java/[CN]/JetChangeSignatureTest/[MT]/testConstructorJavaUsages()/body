{
  JetChangeInfo changeInfo=getChangeInfo();
  JetPsiFactory psiFactory=new JetPsiFactory(getProject());
  JetParameterInfo newParameter=new JetParameterInfo(changeInfo.getMethodDescriptor().getBaseDescriptor(),-1,"s",BUILT_INS.getStringType(),null,psiFactory.createExpression("\"abc\""),JetValVar.None,null);
  changeInfo.addParameter(newParameter);
  doTest(changeInfo);
}
