{
  JetChangeInfo changeInfo=getChangeInfo();
  JetPsiFactory psiFactory=new JetPsiFactory(getProject());
  JetParameterInfo newParameter=new JetParameterInfo(changeInfo.getMethodDescriptor().getBaseDescriptor(),-1,"n",BUILT_INS.getIntType(),null,psiFactory.createExpression("1"),JetValVar.None,null);
  changeInfo.addParameter(newParameter);
  doTest(changeInfo);
}
