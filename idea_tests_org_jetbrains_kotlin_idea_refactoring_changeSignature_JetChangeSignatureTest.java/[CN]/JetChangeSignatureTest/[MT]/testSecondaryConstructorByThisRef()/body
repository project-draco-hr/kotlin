{
  JetChangeInfo changeInfo=getChangeInfo();
  JetPsiFactory psiFactory=new JetPsiFactory(getProject());
  changeInfo.addParameter(new JetParameterInfo(changeInfo.getMethodDescriptor().getBaseDescriptor(),-1,"s",KotlinBuiltIns.getInstance().getStringType(),null,psiFactory.createExpression("\"foo\""),JetValVar.None,null));
  doTest(changeInfo);
}
