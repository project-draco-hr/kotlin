{
  JetChangeInfo changeInfo=getChangeInfo();
  JetPsiFactory psiFactory=new JetPsiFactory(getProject());
  JetParameterInfo newParameter=new JetParameterInfo(-1,"n",KotlinBuiltIns.getInstance().getIntType(),null,psiFactory.createExpression("1"),JetValVar.None,null);
  changeInfo.addParameter(newParameter);
  doTest(changeInfo);
}
