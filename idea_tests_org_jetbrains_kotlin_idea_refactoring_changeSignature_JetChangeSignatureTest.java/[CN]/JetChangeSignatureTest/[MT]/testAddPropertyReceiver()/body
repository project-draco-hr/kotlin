{
  JetChangeInfo changeInfo=getChangeInfo();
  JetParameterInfo newParameter=new JetParameterInfo(changeInfo.getMethodDescriptor().getBaseDescriptor(),-1,"receiver",BUILT_INS.getStringType(),null,new KtPsiFactory(getProject()).createExpression("\"\""),JetValVar.None,null);
  changeInfo.setReceiverParameterInfo(newParameter);
  doTest(changeInfo);
}
