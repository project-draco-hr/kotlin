{
  JetChangeInfo changeInfo=getChangeInfo();
  JetPsiFactory psiFactory=new JetPsiFactory(getProject());
  changeInfo.addParameter(new JetParameterInfo(changeInfo.getMethodDescriptor().getBaseDescriptor(),-1,"i",BUILT_INS.getIntType(),null,psiFactory.createExpression("0"),JetValVar.None,null));
  doTest(changeInfo);
}
