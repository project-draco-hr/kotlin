{
  JetChangeInfo changeInfo=getChangeInfo();
  JetPsiFactory psiFactory=new JetPsiFactory(getProject());
  JetParameterInfo parameterInfo=new JetParameterInfo(-1,"_",KotlinBuiltIns.getInstance().getAnyType(),null,psiFactory.createExpression("X(0)"),JetValVar.None,null);
  parameterInfo.setCurrentTypeText("X");
  changeInfo.setReceiverParameterInfo(parameterInfo);
  doTest(changeInfo);
}
