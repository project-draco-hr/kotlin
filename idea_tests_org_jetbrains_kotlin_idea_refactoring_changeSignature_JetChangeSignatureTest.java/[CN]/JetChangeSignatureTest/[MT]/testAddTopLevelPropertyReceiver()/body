{
  JetChangeInfo changeInfo=getChangeInfo();
  JetParameterInfo newParameter=new JetParameterInfo(changeInfo.getMethodDescriptor().getBaseDescriptor(),-1,"receiver",null,null,new KtPsiFactory(getProject()).createExpression("A()"),JetValVar.None,null);
  newParameter.setCurrentTypeText("test.A");
  changeInfo.setReceiverParameterInfo(newParameter);
  doTest(changeInfo);
}
