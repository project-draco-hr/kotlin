{
  Collection<KtFile> allFiles=JsConfig.withJsLibAdded(files,config);
  LazyTopDownAnalyzerForTopLevel analyzerForJs=InjectionKt.createTopDownAnalyzerForJs(moduleContext,trace,new FileBasedDeclarationProviderFactory(moduleContext.getStorageManager(),allFiles),config.getConfiguration().get(CommonConfigurationKeys.LANGUAGE_FEATURE_SETTINGS,LanguageVersionSettingsImpl.DEFAULT));
  analyzerForJs.analyzeFiles(TopDownAnalysisMode.TopLevelDeclarations,files,Collections.<PackageFragmentProvider>emptyList());
  return JsAnalysisResult.success(trace,moduleContext.getModule());
}
