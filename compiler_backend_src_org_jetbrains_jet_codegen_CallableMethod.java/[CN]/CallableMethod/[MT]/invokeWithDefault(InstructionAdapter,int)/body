{
  v.iconst(mask);
  String desc=getSignature().getAsmMethod().getDescriptor().replace(")","I)");
  if ("<init>".equals(getSignature().getAsmMethod().getName())) {
    v.visitMethodInsn(Opcodes.INVOKESPECIAL,getOwner(),"<init>",desc);
  }
 else {
    if (getInvokeOpcode() != Opcodes.INVOKESTATIC)     desc=desc.replace("(","(L" + getOwner() + ";");
    v.visitMethodInsn(Opcodes.INVOKESTATIC,getInvokeOpcode() == Opcodes.INVOKEINTERFACE ? getOwner() + JvmAbi.TRAIT_IMPL_SUFFIX : getOwner(),getSignature().getAsmMethod().getName() + JvmAbi.DEFAULT_PARAMS_IMPL_SUFFIX,desc);
  }
}
