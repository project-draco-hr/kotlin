{
  String paramsString=StringUtil.join(method.getPsiMethod().getSignature(PsiSubstitutor.EMPTY).getParameterTypes(),new Function<PsiType,String>(){
    @Override public String fun(    PsiType psiType){
      return psiType.getPresentableText();
    }
  }
,", ");
  return String.format("%s(%s)",altFunDeclaration.getName(),paramsString);
}
