{
  Project project=getProject();
  KtExpression ktExpression=KtPsiFactoryKt.KtPsiFactory(project).createExpression(expression);
  KotlinType type=expressionTypingServices.getType(ScopeUtilsKt.asLexicalScope(addImports(scope)),ktExpression,TypeUtils.NO_EXPECTED_TYPE,DataFlowInfo.EMPTY,new BindingTraceContext());
  KotlinType expectedType=expectedTypeStr == null ? null : makeType(expectedTypeStr);
  assertEquals(expectedType,type);
}
