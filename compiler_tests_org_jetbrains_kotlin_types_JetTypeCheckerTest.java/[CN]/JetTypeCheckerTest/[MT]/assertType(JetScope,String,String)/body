{
  Project project=getProject();
  JetExpression jetExpression=JetPsiFactory(project).createExpression(expression);
  JetType type=expressionTypingServices.getType(asLexicalScope(addImports(scope)),jetExpression,TypeUtils.NO_EXPECTED_TYPE,DataFlowInfo.EMPTY,new BindingTraceContext());
  JetType expectedType=expectedTypeStr == null ? null : makeType(expectedTypeStr);
  assertEquals(expectedType,type);
}
