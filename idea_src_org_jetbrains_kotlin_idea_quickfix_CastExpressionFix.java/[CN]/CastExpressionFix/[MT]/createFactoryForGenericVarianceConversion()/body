{
  return new JetSingleIntentionActionFactory(){
    @Nullable @Override public IntentionAction createAction(    @NotNull Diagnostic diagnostic){
      DiagnosticWithParameters2<JetExpression,JetType,JetType> diagnosticWithParameters=ErrorsJvm.JAVA_TYPE_MISMATCH.cast(diagnostic);
      return new CastExpressionFix(diagnosticWithParameters.getPsiElement(),TypesPackage.flexibility(diagnosticWithParameters.getB()).getUpperBound());
    }
  }
;
}
