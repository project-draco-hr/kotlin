{
switch (jvmPrimitiveType) {
case BOOLEAN:
    return BOOLEAN_TYPE;
case CHAR:
  return CHAR_TYPE;
case BYTE:
return BYTE_TYPE;
case SHORT:
return SHORT_TYPE;
case INT:
return INT_TYPE;
case FLOAT:
return FLOAT_TYPE;
case LONG:
return LONG_TYPE;
case DOUBLE:
return DOUBLE_TYPE;
default :
throw new IllegalStateException("Unknown primitive type: " + jvmPrimitiveType);
}
}
