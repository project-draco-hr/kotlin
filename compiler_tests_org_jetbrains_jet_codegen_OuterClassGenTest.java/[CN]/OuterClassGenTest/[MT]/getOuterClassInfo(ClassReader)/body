{
  final OuterClassInfo info=new OuterClassInfo();
  reader.accept(new ClassVisitor(Opcodes.ASM4){
    @Override public void visitOuterClass(    String owner,    String name,    String desc){
      info.owner=owner;
      info.method=name;
      info.descriptor=desc;
    }
  }
,0);
  return info;
}
