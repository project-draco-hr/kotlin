{
  return getSourcePropertyOrFunction(decompiledProperty,decompiledProperty.getReceiverTypeRef(),new Matcher<JetProperty,VariableDescriptor>(){
    @Override public boolean areSame(    JetProperty declaration,    VariableDescriptor descriptor){
      return true;
    }
    @Override public Set<VariableDescriptor> getCandidatesFromScope(    JetScope scope,    String name){
      return scope.getProperties(name);
    }
  }
);
}
