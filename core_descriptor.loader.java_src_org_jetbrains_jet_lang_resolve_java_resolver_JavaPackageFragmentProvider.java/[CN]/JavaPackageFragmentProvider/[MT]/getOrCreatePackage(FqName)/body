{
  if (packageFragments.containsKey(fqName)) {
    return packageFragments.get(fqName);
  }
  JavaPackageFragmentDescriptor packageFragment=JavaPackageFragmentDescriptor.create(this,fqName,new NullableFunction<JavaPackageFragmentDescriptor,JavaPackageFragmentScope>(){
    @Override @Nullable public JavaPackageFragmentScope fun(    JavaPackageFragmentDescriptor packageFragment){
      return createPackageScope(fqName,packageFragment);
    }
  }
);
  packageFragments.put(fqName,packageFragment);
  return packageFragment;
}
