{
  StringBuilder sb=new StringBuilder();
  sb.append(renderTypeName(type.getConstructor(),shortNamesOnly));
  if (!type.getArguments().isEmpty()) {
    sb.append("<");
    appendTypeProjections(sb,type.getArguments(),shortNamesOnly);
    sb.append(">");
  }
  if (type.isNullable()) {
    sb.append("?");
  }
  return sb.toString();
}
