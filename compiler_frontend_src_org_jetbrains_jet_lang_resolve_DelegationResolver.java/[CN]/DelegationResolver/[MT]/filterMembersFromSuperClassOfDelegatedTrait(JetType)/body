{
  final Collection<T> membersToSkip=getMembersFromClassSupertypeOfTrait(delegatedTraitType);
  return Collections2.filter(callback.getMembersByType(delegatedTraitType),new Predicate<CallableMemberDescriptor>(){
    @Override public boolean apply(    @Nullable CallableMemberDescriptor descriptor){
      for (      CallableMemberDescriptor memberToSkip : membersToSkip) {
        if (haveSameSignatures(memberToSkip,descriptor)) {
          return false;
        }
      }
      return true;
    }
  }
);
}
