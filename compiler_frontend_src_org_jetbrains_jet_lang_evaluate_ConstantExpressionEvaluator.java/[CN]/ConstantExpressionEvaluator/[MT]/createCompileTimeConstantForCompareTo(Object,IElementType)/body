{
  if (result instanceof Integer) {
    int resultCode=((Integer)result).intValue();
    if (resultCode == 0) {
      if (operationToken == JetTokens.EQEQ || operationToken == JetTokens.LTEQ || operationToken == JetTokens.GTEQ) {
        return BooleanValue.TRUE;
      }
 else {
        return BooleanValue.FALSE;
      }
    }
    if (resultCode == 1) {
      if (operationToken == JetTokens.GT || operationToken == JetTokens.GTEQ) {
        return BooleanValue.TRUE;
      }
 else {
        return BooleanValue.FALSE;
      }
    }
    if (resultCode == -1) {
      if (operationToken == JetTokens.LT || operationToken == JetTokens.LTEQ) {
        return BooleanValue.TRUE;
      }
 else {
        return BooleanValue.FALSE;
      }
    }
  }
  return null;
}
