{
  JetTypeInfo typeInfo=getCallExpressionTypeInfoWithoutFinalTypeCheck(callExpression,receiver,callOperationNode,context,resolveMode,resolutionResultsCache);
  if (resolveMode == ResolveMode.TOP_LEVEL_CALL) {
    DataFlowUtils.checkType(typeInfo.getType(),callExpression,context,typeInfo.getDataFlowInfo());
  }
  return typeInfo;
}
