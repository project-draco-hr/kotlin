{
  serialize(type.getConstructor().getDeclarationDescriptor());
  if (!type.getArguments().isEmpty()) {
    sb.append("<");
    boolean first=true;
    for (    TypeProjection proj : type.getArguments()) {
      if (!first) {
        sb.append(", ");
      }
      serialize(proj.getProjectionKind());
      serialize(proj.getType());
      first=false;
    }
    sb.append(">");
  }
}
