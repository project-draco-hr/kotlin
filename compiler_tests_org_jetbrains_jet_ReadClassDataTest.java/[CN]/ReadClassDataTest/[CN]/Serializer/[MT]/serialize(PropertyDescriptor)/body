{
  if (prop.isVar()) {
    sb.append("var ");
  }
 else {
    sb.append("val ");
  }
  if (prop.getReceiverParameter().exists()) {
    new Serializer(sb).serialize(prop.getReceiverParameter().getType());
    sb.append(".");
  }
  sb.append(prop.getName());
  sb.append(": ");
  new Serializer(sb).serialize(prop.getOutType());
}
