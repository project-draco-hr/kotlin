{
  JetType type=components.reflectionTypes.getKPropertyType(Annotations.EMPTY,receiverType,descriptor.getType(),isExtension,descriptor.isVar());
  LocalVariableDescriptor localVariable=new LocalVariableDescriptor(context.scope.getContainingDeclaration(),Annotations.EMPTY,Name.special("<anonymous>"),type,false,toSourceElement(expression));
  context.trace.record(VARIABLE,expression,localVariable);
  return type;
}
