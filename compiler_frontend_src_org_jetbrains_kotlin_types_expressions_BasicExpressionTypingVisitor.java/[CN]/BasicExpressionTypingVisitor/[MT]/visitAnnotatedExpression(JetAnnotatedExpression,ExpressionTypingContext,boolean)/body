{
  components.expressionTypingServices.getAnnotationResolver().resolveAnnotationsWithArguments(context.scope,expression.getAnnotationEntries(),context.trace);
  JetExpression baseExpression=expression.getBaseExpression();
  if (baseExpression == null) {
    return TypeInfoFactoryPackage.noTypeInfo(context);
  }
  return facade.getTypeInfo(baseExpression,context,isStatement);
}
