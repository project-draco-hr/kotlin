{
  return new BasicCallResolutionContext(context.trace,context.scope,call,context.expectedType,context.dataFlowInfo,context.contextDependency,checkArguments,context.resolutionResultsCache,dataFlowInfoForArguments,context.callChecker,context.symbolUsageValidator,context.additionalTypeChecker,context.statementFilter,context.isAnnotationContext,context.collectAllCandidates,context.insideCallChain);
