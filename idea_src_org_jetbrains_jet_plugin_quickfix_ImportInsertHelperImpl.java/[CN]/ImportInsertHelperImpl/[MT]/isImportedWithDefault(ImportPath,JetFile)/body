{
  List<ImportPath> defaultImports=ProjectStructureUtil.isJsKotlinModule(contextFile) ? TopDownAnalyzerFacadeForJS.DEFAULT_IMPORTS : TopDownAnalyzerFacadeForJVM.DEFAULT_IMPORTS;
  if (importPath.fqnPart().asString().startsWith("java.lang.") && !JavaToKotlinClassMap.getInstance().mapPlatformClass(importPath.fqnPart()).isEmpty()) {
    return false;
  }
  return NamePackage.isImported(importPath,defaultImports);
}
