{
  JetTypeReference typeRef=psi.getPropertyTypeRef();
  JetExpression expression=psi.getInitializer();
  return new PsiJetPropertyStubImpl(JetStubElementTypes.PROPERTY,parentStub,psi.getName(),psi.isVar(),psi.isLocal(),typeRef != null ? typeRef.getText() : null,expression != null ? expression.getText() : null);
}
