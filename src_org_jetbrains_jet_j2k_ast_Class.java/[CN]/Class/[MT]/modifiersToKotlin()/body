{
  List<String> modifierList=new LinkedList<String>();
  if (needAbstractModifier())   modifierList.add(Modifier.ABSTRACT);
  modifierList.add(accessModifier());
  if (needOpenModifier())   modifierList.add(Modifier.OPEN);
  if (modifierList.size() > 0)   return AstUtil.join(modifierList,SPACE) + SPACE;
  return EMPTY;
}
