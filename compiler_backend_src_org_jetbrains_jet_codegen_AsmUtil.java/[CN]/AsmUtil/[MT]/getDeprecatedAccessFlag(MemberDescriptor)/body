{
  if (descriptor instanceof PropertyAccessorDescriptor) {
    return KotlinBuiltIns.getInstance().isDeprecated(descriptor) ? ACC_DEPRECATED : getDeprecatedAccessFlag(((PropertyAccessorDescriptor)descriptor).getCorrespondingProperty());
  }
 else   if (KotlinBuiltIns.getInstance().isDeprecated(descriptor)) {
    return ACC_DEPRECATED;
  }
  return 0;
}
