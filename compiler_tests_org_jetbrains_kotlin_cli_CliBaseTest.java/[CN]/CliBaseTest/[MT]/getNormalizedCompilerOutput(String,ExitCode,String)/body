{
  String normalizedOutputWithoutExitCode=pureOutput.replace(new File(testDataDir).getAbsolutePath(),"$TESTDATA_DIR$").replace("expected ABI version is " + JvmBytecodeBinaryVersion.INSTANCE,"expected ABI version is $ABI_VERSION$").replace("\\","/").replace(KotlinVersion.VERSION,"$VERSION$");
  return removePerfOutput(normalizedOutputWithoutExitCode) + exitCode;
}
