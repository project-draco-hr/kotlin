{
  String normalizedOutputWithoutExitCode=pureOutput.replace(new File(testDataDir).getAbsolutePath(),"$TESTDATA_DIR$").replace("expected ABI version is " + JvmAbi.VERSION,"expected ABI version is $ABI_VERSION$").replace("\\","/");
  return removePerfOutput(normalizedOutputWithoutExitCode) + exitCode;
}
