{
  List<String> lines=IoPackage.readLines(new File(argsFilePath),Charsets.UTF_8);
  return ContainerUtil.mapNotNull(lines,new Function<String,String>(){
    @Override public String fun(    String arg){
      if (arg.isEmpty()) {
        return null;
      }
      String argsWithColonsReplaced=arg.replace("\\:","$COLON$").replace(":",File.pathSeparator).replace("$COLON$",":");
      return argsWithColonsReplaced.replace("$TEMP_DIR$",tempDir).replace("$TESTDATA_DIR$",testDataDir);
    }
  }
);
}
