{
  return WhenChecker.isWhenByEnum(expression,bindingContext) && SwitchCodegenUtil.checkAllItemsAreConstantsSatisfying(expression,bindingContext,new Function1<CompileTimeConstant,Boolean>(){
    @Override public Boolean invoke(    @NotNull CompileTimeConstant constant){
      return constant instanceof EnumValue;
    }
  }
);
}
