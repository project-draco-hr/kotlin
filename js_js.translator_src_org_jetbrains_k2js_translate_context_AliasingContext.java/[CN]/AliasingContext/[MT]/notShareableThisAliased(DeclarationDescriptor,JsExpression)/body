{
  return new AliasingContext(this){
    @Nullable @Override protected JsExpression getAliasForDescriptor(    @NotNull DeclarationDescriptor descriptor,    boolean fromChild){
      if (!fromChild && thisDescriptor == descriptor) {
        return alias;
      }
      return super.getAliasForDescriptor(descriptor,fromChild);
    }
  }
;
}
