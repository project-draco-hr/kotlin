{
  loadText("annotation class A");
  Class<?> aClass=generateClass("A");
  Class[] interfaces=aClass.getInterfaces();
  assertEquals(0,aClass.getDeclaredMethods().length);
  assertTrue(aClass.isAnnotation());
  assertEquals(2,interfaces.length);
  assertEquals(Sets.newHashSet("java.lang.annotation.Annotation","kotlin.jvm.internal.KObject"),Sets.newHashSet(interfaces[0].getName(),interfaces[1].getName()));
}
