{
  ResolvedCallImpl<D> candidateCall=ResolvedCallImpl.create(candidate,candidateTrace,task.tracing,task.dataFlowInfoForArguments);
  return Collections.singleton(CallCandidateResolutionContext.create(candidateCall,task,candidateTrace,task.tracing,task.call,ReceiverValue.NO_RECEIVER,candidateResolveMode));
}
