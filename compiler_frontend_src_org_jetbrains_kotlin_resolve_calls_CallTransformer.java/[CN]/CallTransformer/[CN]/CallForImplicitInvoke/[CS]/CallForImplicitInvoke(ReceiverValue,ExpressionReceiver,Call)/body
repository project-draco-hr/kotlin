{
  super(call);
  this.outerCall=call;
  this.explicitExtensionReceiver=explicitExtensionReceiver;
  this.calleeExpressionAsDispatchReceiver=calleeExpressionAsDispatchReceiver;
  this.fakeInvokeExpression=(JetSimpleNameExpression)JetPsiFactory(call.getCallElement()).createExpression(OperatorNameConventions.INVOKE.asString());
}
