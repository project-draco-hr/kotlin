{
  this.project=project;
  this.cachedValue=CachedValuesManager.getManager(project).createCachedValue(new CachedValueProvider<CacheData>(){
    @Nullable @Override public Result<CacheData> compute(){
      return Result.create(new CacheData(),PsiModificationTracker.OUT_OF_CODE_BLOCK_MODIFICATION_COUNT);
    }
  }
,false);
}
