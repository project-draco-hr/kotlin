{
  StringBuilder buffer=new StringBuilder();
  if (isConstructor()) {
    buffer.append(newName);
    if (newVisibility != null && newVisibility != Visibilities.PUBLIC)     buffer.append(' ').append(newVisibility.toString()).append(' ');
  }
 else {
    if (newVisibility != null && newVisibility != Visibilities.INTERNAL)     buffer.append(newVisibility.toString()).append(' ');
    buffer.append(JetTokens.FUN_KEYWORD).append(' ').append(newName);
  }
  buffer.append(getNewParametersSignature(inheritedFunction,isInherited,buffer.length()));
  if (newReturnType != null && !KotlinBuiltIns.isUnit(newReturnType) && !isConstructor())   buffer.append(": ").append(newReturnTypeText);
  return buffer.toString();
}
