{
  if (fields == null) {
    PsiField[] psiFields=psiClass.getFields();
    List<PsiFieldWrapper> fields=new ArrayList<PsiFieldWrapper>(psiFields.length);
    for (    PsiField psiField : psiFields) {
      fields.add(new PsiFieldWrapper(psiField));
    }
    this.fields=fields;
  }
  return fields;
}
