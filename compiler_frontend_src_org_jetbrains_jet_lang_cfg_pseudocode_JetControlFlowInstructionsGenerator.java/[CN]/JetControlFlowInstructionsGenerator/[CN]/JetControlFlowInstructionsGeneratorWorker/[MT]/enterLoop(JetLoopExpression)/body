{
  Label loopEntryLabel=createUnboundLabel("loop entry point");
  bindLabel(loopEntryLabel);
  LoopInfo blockInfo=new LoopInfo(expression,loopEntryLabel,createUnboundLabel("loop exit point"),createUnboundLabel("body entry point"),createUnboundLabel("body exit point"),createUnboundLabel("condition entry point"));
  elementToBlockInfo.put(expression,blockInfo);
  return blockInfo;
}
