{
switch (descriptor.getKind()) {
case TRAIT:
    return JetHighlightingColors.TRAIT;
case ANNOTATION_CLASS:
  return JetHighlightingColors.ANNOTATION;
case OBJECT:
case ENUM_ENTRY:
return JetHighlightingColors.INSTANCE_PROPERTY;
default :
return descriptor.getModality() == Modality.ABSTRACT ? JetHighlightingColors.ABSTRACT_CLASS : JetHighlightingColors.CLASS;
}
}
