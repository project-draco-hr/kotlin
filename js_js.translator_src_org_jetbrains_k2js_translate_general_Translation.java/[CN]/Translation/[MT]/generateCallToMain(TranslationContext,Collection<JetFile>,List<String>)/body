{
  JetNamedFunction mainFunction=getMainFunction(files);
  if (mainFunction == null) {
    return null;
  }
  FunctionDescriptor functionDescriptor=getFunctionDescriptor(context.bindingContext(),mainFunction);
  return CallBuilder.build(context).args(new JsArrayLiteral(toStringLiteralList(arguments,context.program()))).descriptor(functionDescriptor).translate().makeStmt();
}
