{
  UExpression condition=node.getCondition();
  UExpression body=node.getThenBranch();
  UElement elseBody=node.getElseBranch();
  process(condition);
  if (body != null) {
    boolean wasReachable=mReachable;
    process(body);
    mReachable=wasReachable;
  }
  if (elseBody != null) {
    boolean wasReachable=mReachable;
    process(elseBody);
    mReachable=wasReachable;
  }
  return false;
}
