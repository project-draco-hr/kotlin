{
  boolean stubs=compilerDependencies.getCompilerSpecialMode().isStubs();
  GenerationState generationState=KotlinToJVMBytecodeCompiler.analyzeAndGenerate(environment,compilerDependencies,messageCollector,stubs);
  if (generationState == null) {
    return null;
  }
  return generationState.getFactory();
}
