{
  if (myType.getKind() == Kind.ARRAY_TYPE) {
    if (myExpressions.size() == 1)     return oneDim(myType,myExpressions.get(0));
    if (myExpressions.size() == 2) {
      Type innerType=((ArrayType)myType).getInnerType();
      return oneDim(myType,myExpressions.get(0),"{" + SPACE + oneDim(innerType,myExpressions.get(1))+ SPACE+ "}");
    }
  }
  return getConstructorName(myType);
}
