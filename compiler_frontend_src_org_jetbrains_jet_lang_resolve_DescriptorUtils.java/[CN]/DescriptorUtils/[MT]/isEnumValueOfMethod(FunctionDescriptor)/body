{
  List<ValueParameterDescriptor> methodTypeParameters=functionDescriptor.getValueParameters();
  JetType nullableString=TypeUtils.makeNullable(KotlinBuiltIns.getInstance().getStringType());
  return "valueOf".equals(functionDescriptor.getName().getName()) && methodTypeParameters.size() == 1 && JetTypeChecker.INSTANCE.isSubtypeOf(methodTypeParameters.get(0).getType(),nullableString);
}
