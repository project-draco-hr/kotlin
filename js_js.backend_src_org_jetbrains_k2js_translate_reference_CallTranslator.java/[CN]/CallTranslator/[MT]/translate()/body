{
  if (isIntrinsic()) {
    return intrinsicInvocation();
  }
  if (isConstructor()) {
    return constructorCall();
  }
  if (isNative()) {
    return methodCall();
  }
  if (isExtensionFunctionLiteral()) {
    return extensionFunctionLiteralCall();
  }
  if (isExtensionFunction()) {
    return extensionFunctionCall();
  }
  return methodCall();
}
