{
  lock.readLock().lock();
  Signature sig=map.get(klass);
  lock.readLock().unlock();
  if (sig == null) {
    lock.writeLock().lock();
    sig=map.get(klass);
    if (sig == null) {
      sig=internalParse(klass);
      map.put(klass,sig);
    }
    lock.writeLock().unlock();
  }
  return sig;
}
