{
  try {
    return DescriptorRenderer.Companion.getDEBUG_TEXT().render(descriptor) + "[" + descriptor.getClass().getSimpleName()+ "@"+ Integer.toHexString(System.identityHashCode(descriptor))+ "]";
  }
 catch (  Throwable e) {
    return descriptor.getClass().getSimpleName() + " " + descriptor.getName();
  }
}
