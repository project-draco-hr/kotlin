{
  TemporaryBindingTrace tempTrace=TemporaryBindingTrace.create(resolveSession.getTrace(),"Transient trace for default imports lazy resolve");
  return LazyFileScope.Factory.create(resolveSession,file,defaultImports.invoke(),additionalFileScopeProvider.scopes(file),resolveSession.getTrace(),tempTrace,"LazyFileScope for file " + file.getName());
}
