{
  if (libraryContext == null) {
    AnalyzeExhaust exhaust=AnalyzerFacadeForJS.analyzeFiles(files,Predicates.<PsiFile>alwaysFalse(),Config.getEmptyConfig(project));
    libraryContext=exhaust.getBindingContext();
    AnalyzerFacadeForJS.checkForErrors(files,libraryContext);
  }
  return libraryContext;
}
