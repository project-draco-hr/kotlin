{
  if (KotlinBuiltIns.getInstance().isFunctionType(calleeType)) {
    LOG.assertTrue(call.getExplicitReceiver().exists(),"'Invoke' is not found on expression of function type (" + calleeType + "): "+ PsiUtilPackage.getTextWithLocation(call.getCallElement()));
    trace.report(NO_RECEIVER_ALLOWED.on(reference));
  }
 else {
    trace.report(FUNCTION_EXPECTED.on(reference,reference,calleeType));
  }
}
