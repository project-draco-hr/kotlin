{
  ExpressionTypingContext context=ExpressionTypingContext.newContext(semanticServices,new HashMap<JetPattern,DataFlowInfo>(),new HashMap<JetPattern,List<VariableDescriptor>>(),new LabelResolver(),trace,scope,DataFlowInfo.getEmpty(),NO_EXPECTED_TYPE,FORBIDDEN,true);
  return expressionTypingFacade.getType(expression,context);
}
