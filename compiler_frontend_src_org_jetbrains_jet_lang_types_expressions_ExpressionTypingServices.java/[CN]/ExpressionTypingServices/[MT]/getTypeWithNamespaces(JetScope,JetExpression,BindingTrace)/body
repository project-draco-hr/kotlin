{
  ExpressionTypingContext context=ExpressionTypingContext.newContext(this,new HashMap<JetPattern,DataFlowInfo>(),new HashMap<JetPattern,List<VariableDescriptor>>(),new LabelResolver(),trace,scope,DataFlowInfo.EMPTY,NO_EXPECTED_TYPE,true);
  return expressionTypingFacade.getType(expression,context);
}
