{
  ExpressionTypingContext context=ExpressionTypingContext.newContext(expressionTypingContextContext,expression.getProject(),semanticServices,new HashMap<JetPattern,DataFlowInfo>(),new HashMap<JetPattern,List<VariableDescriptor>>(),new LabelResolver(),trace,scope,DataFlowInfo.EMPTY,NO_EXPECTED_TYPE,FORBIDDEN,true);
  return expressionTypingFacade.getType(expression,context);
}
