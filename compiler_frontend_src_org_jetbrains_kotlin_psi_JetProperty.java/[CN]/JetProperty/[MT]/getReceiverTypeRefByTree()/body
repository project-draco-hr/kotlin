{
  ASTNode node=getNode().getFirstChildNode();
  while (node != null) {
    IElementType tt=node.getElementType();
    if (tt == JetTokens.COLON)     break;
    if (tt == JetNodeTypes.TYPE_REFERENCE) {
      return (JetTypeReference)node.getPsi();
    }
    node=node.getTreeNext();
  }
  return null;
}
