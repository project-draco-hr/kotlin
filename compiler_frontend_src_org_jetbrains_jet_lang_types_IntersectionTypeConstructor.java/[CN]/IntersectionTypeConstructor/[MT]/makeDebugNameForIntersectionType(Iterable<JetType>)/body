{
  StringBuilder debugName=new StringBuilder("{");
  for (Iterator<JetType> iterator=resultingTypes.iterator(); iterator.hasNext(); ) {
    JetType type=iterator.next();
    debugName.append(type.toString());
    if (iterator.hasNext()) {
      debugName.append(" & ");
    }
  }
  debugName.append("}");
  return debugName.toString();
}
