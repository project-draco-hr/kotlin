{
  ExpressionReceiver receiver=safeGetExpressionReceiver(facade,binaryExpression.getLeft(),context.replaceScope(scope));
  OverloadResolutionResults<FunctionDescriptor> results=context.replaceScope(scope).resolveCallWithGivenNameToDescriptor(CallMaker.makeCall(receiver,binaryExpression),binaryExpression.getOperationReference(),name);
  if (results.isSuccess()) {
    return results.getResultingDescriptor().getReturnType();
  }
  return null;
}
