{
  context.expressionTypingServices.getAnnotationResolver().resolveAnnotationsWithArguments(context.scope,expression.getAnnotationEntries(),context.trace);
  JetExpression baseExpression=expression.getBaseExpression();
  if (baseExpression == null) {
    return JetTypeInfo.create(null,context.dataFlowInfo);
  }
  return facade.getTypeInfo(baseExpression,context,isStatement);
}
