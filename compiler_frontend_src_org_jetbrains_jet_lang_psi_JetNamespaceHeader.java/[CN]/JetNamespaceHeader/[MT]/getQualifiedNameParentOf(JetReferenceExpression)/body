{
  StringBuilder builder=new StringBuilder();
  for (  JetSimpleNameExpression e : findChildrenByClass(JetSimpleNameExpression.class)) {
    if (e == nameExpression) {
      break;
    }
    if (builder.length() > 0) {
      builder.append(".");
    }
    builder.append(e.getReferencedName());
  }
  return builder.toString();
}
