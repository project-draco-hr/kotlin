{
  Collection<ClassDescriptorWithResolutionScopes> scriptClasses=KotlinPackage.map(getScripts().values(),new Function1<ScriptDescriptor,ClassDescriptorWithResolutionScopes>(){
    @Override public ClassDescriptorWithResolutionScopes invoke(    ScriptDescriptor scriptDescriptor){
      return (ClassDescriptorWithResolutionScopes)scriptDescriptor.getClassDescriptor();
    }
  }
);
  return KotlinPackage.plus(getClasses().values(),scriptClasses);
}
