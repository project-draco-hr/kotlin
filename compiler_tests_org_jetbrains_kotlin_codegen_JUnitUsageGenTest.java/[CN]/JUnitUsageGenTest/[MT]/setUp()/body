{
  super.setUp();
  File junitJar=new File("libraries/lib/junit-4.11.jar");
  if (!junitJar.exists()) {
    throw new AssertionError("JUnit jar wasn't found");
  }
  myEnvironment=KotlinCoreEnvironment.createForTests(getTestRootDisposable(),KotlinTestUtils.compilerConfigurationForTests(ConfigurationKind.ALL,TestJdkKind.MOCK_JDK,junitJar),EnvironmentConfigFiles.JVM_CONFIG_FILES);
}
