{
  try {
    if (!expectedFile.exists()) {
      FileUtil.writeToFile(expectedFile,actual);
      Assert.fail("Expected data file did not exist. Generating: " + expectedFile);
    }
    String expected=FileUtil.loadFile(expectedFile,true);
    Assert.assertEquals("Expected and actual namespaces differ from " + expectedFile.getName(),StringUtil.convertLineSeparators(expected),StringUtil.convertLineSeparators(actual));
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
