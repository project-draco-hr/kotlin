{
  List<String> librariesWithStdlib=new ArrayList<String>(LibrarySourcesConfig.JS_STDLIB);
  if (libraries != null) {
    librariesWithStdlib.addAll(libraries);
  }
  return new LibrarySourcesConfig.Builder(project,moduleId,librariesWithStdlib).ecmaVersion(ecmaVersion).sourceMap(shouldGenerateSourceMap()).inlineEnabled(IS_INLINE_ENABLED).isUnitTestConfig(shouldBeTranslateAsUnitTestClass()).metaInfo(shouldGenerateMetaInfo()).build();
}
