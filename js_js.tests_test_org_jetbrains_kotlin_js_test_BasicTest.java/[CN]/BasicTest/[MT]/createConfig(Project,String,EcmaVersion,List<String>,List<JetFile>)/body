{
  for (  JetFile file : jetFiles) {
    String text=file.getText();
    if (isDirectiveDefined(text,NO_INLINE_DIRECTIVE)) {
      isInlineEnabled=false;
      break;
    }
  }
  List<String> librariesWithStdlib=new ArrayList<String>(LibrarySourcesConfig.JS_STDLIB);
  if (libraries != null) {
    librariesWithStdlib.addAll(libraries);
  }
  return new LibrarySourcesConfig.Builder(project,moduleId,librariesWithStdlib).ecmaVersion(ecmaVersion).sourceMap(shouldGenerateSourceMap()).inlineEnabled(isInlineEnabled).isUnitTestConfig(shouldBeTranslateAsUnitTestClass()).metaInfo(shouldGenerateMetaInfo()).build();
}
