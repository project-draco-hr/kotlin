{
  super.setUp();
  File junitJar=new File("libraries/lib/junit-4.9.jar");
  if (!junitJar.exists()) {
    throw new AssertionError("JUnit jar wasn't found");
  }
  myEnvironment=new JetCoreEnvironment(getTestRootDisposable(),JetTestUtils.compilerConfigurationForTests(ConfigurationKind.ALL,TestJdkKind.MOCK_JDK,junitJar));
}
