{
  ModuleDescriptorImpl module=TopDownAnalyzerFacadeForJVM.createJavaModule("<module>");
  module.addDependencyOnModule(module);
  module.addDependencyOnModule(KotlinBuiltIns.getInstance().getBuiltInsModule());
  module.seal();
  BindingTrace trace=new CliLightClassGenerationSupport.CliBindingTrace();
  return TopDownAnalyzerFacadeForJVM.analyzeFilesWithJavaIntegrationWithCustomContext(project,ContextPackage.GlobalContext(),files,trace,module,null,null);
}
