{
  if ((test & EOL_HINT_MASK) == 0 && eolChar(test))   Context.codeBug();
  if (test >= 128 && formatChar(test))   Context.codeBug();
  for (; ; ) {
    if (end == offset && !fill())     return false;
    int c=buffer[offset];
    if (test == c) {
      ++offset;
      return true;
    }
    if (c < 128 || !formatChar(c)) {
      return false;
    }
    skipFormatChar();
  }
}
