{
  LoopInfo info=new LoopInfo(expression,createUnboundLabel("loop entry point"),createUnboundLabel("loop exit point"),createUnboundLabel("body entry point"),createUnboundLabel("body exit point"),createUnboundLabel("condition entry point"));
  bindLabel(info.getEntryPoint());
  elementToBlockInfo.put(expression,info);
  return info;
}
