{
  if (type != Type.BOOLEAN_TYPE) {
    throw new UnsupportedOperationException("don't know how to put a compare as a non-boolean type");
  }
  Label ifTrue=new Label();
  Label end=new Label();
  condJump(ifTrue,false,v);
  v.iconst(0);
  v.goTo(end);
  v.mark(ifTrue);
  v.iconst(1);
  v.mark(end);
}
