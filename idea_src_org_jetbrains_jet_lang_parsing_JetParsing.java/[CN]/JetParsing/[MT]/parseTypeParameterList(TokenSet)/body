{
  PsiBuilder.Marker list=mark();
  if (at(LT)) {
    advance();
    while (true) {
      if (at(COMMA))       errorAndAdvance("Expecting type parameter declaration");
      parseTypeParameter();
      if (!at(COMMA))       break;
      advance();
    }
    expect(GT,"Missing '>'",recoverySet);
  }
  list.done(TYPE_PARAMETER_LIST);
}
