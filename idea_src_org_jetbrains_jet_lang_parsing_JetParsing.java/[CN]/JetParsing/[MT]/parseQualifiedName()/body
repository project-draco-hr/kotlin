{
  while (true) {
    if (tt() != IDENTIFIER) {
      if (tt() != DOT) {
        error("Expecting qualified name");
        break;
      }
      error("Qualified name must start with an identifier");
      advance();
    }
 else {
      advance();
      if (tt() != DOT)       break;
      advance();
    }
  }
}
