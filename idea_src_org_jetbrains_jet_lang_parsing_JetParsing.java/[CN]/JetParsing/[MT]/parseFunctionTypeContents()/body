{
  PsiBuilder.Marker parameters=mark();
  expect(LPAR,"Expecting '(");
  if (!at(RPAR)) {
    while (true) {
      if (!parseValueParameter()) {
        parseTypeRef();
      }
      if (!at(COMMA))       break;
      advance();
    }
  }
  expect(RPAR,"Expecting ')'");
  parameters.done(VALUE_PARAMETER_LIST);
  expect(COLON,"Expecting ':' followed by a return type",TYPE_REF_FIRST);
  parseTypeRef();
}
