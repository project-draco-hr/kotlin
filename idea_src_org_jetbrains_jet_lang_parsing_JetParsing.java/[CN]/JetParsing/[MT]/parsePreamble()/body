{
  PsiBuilder.Marker firstEntry=mark();
  parseModifierList(MODIFIER_LIST);
  if (at(NAMESPACE_KEYWORD)) {
    advance();
    parseNamespaceName();
    if (at(LBRACE)) {
      firstEntry.rollbackTo();
      return;
    }
    firstEntry.drop();
    consumeIf(SEMICOLON);
  }
 else {
    firstEntry.rollbackTo();
  }
  while (at(IMPORT_KEYWORD)) {
    parseImportDirective();
  }
}
