{
  PsiBuilder.Marker list=mark();
  boolean empty=true;
  while (!eof()) {
    if (atSet(MODIFIER_KEYWORDS)) {
      if (tokenConsumer != null)       tokenConsumer.consume(tt());
      advance();
    }
 else     if (at(LBRACKET) || (allowShortAnnotations && at(IDENTIFIER))) {
      parseAnnotation(allowShortAnnotations);
    }
 else {
      break;
    }
    empty=false;
  }
  if (empty) {
    list.drop();
  }
 else {
    list.done(nodeType);
  }
  return !empty;
}
