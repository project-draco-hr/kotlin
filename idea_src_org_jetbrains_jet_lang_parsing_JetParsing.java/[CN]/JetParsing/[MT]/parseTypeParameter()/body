{
  if (TYPE_PARAMETER_GT_RECOVERY_SET.contains(tt())) {
    error("Type parameter declaration expected");
    return;
  }
  PsiBuilder.Marker mark=mark();
  parseModifierList();
  expect(IDENTIFIER,"Type parameter name expected",TokenSet.EMPTY);
  if (at(COLON)) {
    advance();
    parseTypeRef();
  }
  mark.done(TYPE_PARAMETER);
}
