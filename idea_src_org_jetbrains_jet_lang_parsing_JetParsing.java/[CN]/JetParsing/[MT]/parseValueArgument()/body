{
  PsiBuilder.Marker argument=mark();
  JetNodeType type=VALUE_ARGUMENT;
  if (at(IDENTIFIER) && lookahead(1) == EQ) {
    advance();
    advance();
    type=NAMED_ARGUMENT;
  }
  if (at(OUT_KEYWORD) || at(REF_KEYWORD))   advance();
  myExpressionParsing.parseExpression();
  argument.done(type);
}
