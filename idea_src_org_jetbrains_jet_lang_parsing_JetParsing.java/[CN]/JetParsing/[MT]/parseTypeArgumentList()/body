{
  if (!at(LT))   return null;
  PsiBuilder.Marker list=mark();
  myBuilder.disableNewlines();
  advance();
  while (true) {
    parseModifierList();
    parseTypeRef();
    if (!at(COMMA))     break;
    advance();
  }
  expect(GT,"Expecting a '>'");
  myBuilder.restoreNewlinesState();
  list.done(TYPE_ARGUMENT_LIST);
  return list;
}
