{
  PsiBuilder.Marker type=mark();
  parseAttributeList();
  while (true) {
    if (at(IDENTIFIER)) {
      parseSimpleUserType();
    }
 else     if (at(LBRACE)) {
      parseSimpleFunctionType();
    }
 else     if (at(LPAR)) {
      parseTupleType();
    }
 else {
      error("Type expected");
      break;
    }
    if (!at(DOT))     break;
    advance();
  }
  type.done(TYPE_REFERENCE);
}
