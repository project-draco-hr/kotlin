{
  ModuleDescriptor moduleDescriptor=LazyResolveTestUtil.resolveEagerly(Collections.singletonList(JetTestUtils.loadJetFile(getProject(),new File(path))),getEnvironment());
  PackageFragmentDescriptor testData=DescriptorUtils.getExactlyOnePackageFragment(moduleDescriptor,new FqName("testData"));
  return addImports(testData.getMemberScope());
}
