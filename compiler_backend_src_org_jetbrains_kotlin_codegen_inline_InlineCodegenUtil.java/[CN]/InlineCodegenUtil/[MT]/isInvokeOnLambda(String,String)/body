{
  if (!OperatorConventions.INVOKE.asString().equals(name)) {
    return false;
  }
  for (  String prefix : Arrays.asList("kotlin/Function","kotlin/ExtensionFunction")) {
    if (owner.startsWith(prefix)) {
      String suffix=owner.substring(prefix.length());
      if (isInteger(suffix)) {
        return true;
      }
    }
  }
  return false;
}
