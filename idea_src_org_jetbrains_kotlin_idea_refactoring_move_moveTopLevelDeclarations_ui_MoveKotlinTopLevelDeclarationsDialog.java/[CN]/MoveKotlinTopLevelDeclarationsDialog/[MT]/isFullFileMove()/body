{
  Map<KtFile,List<? extends KtNamedDeclaration>> fileToElements=CollectionsKt.groupBy(getSelectedElementsToMove(),new Function1<KtNamedDeclaration,KtFile>(){
    @Override public KtFile invoke(    KtNamedDeclaration declaration){
      return declaration.getContainingJetFile();
    }
  }
);
  for (  Map.Entry<KtFile,List<? extends KtNamedDeclaration>> entry : fileToElements.entrySet()) {
    if (entry.getKey().getDeclarations().size() != entry.getValue().size())     return false;
  }
  return true;
}
