{
  Map<JetFile,List<? extends JetNamedDeclaration>> fileToElements=KotlinPackage.groupBy(getSelectedElementsToMove(),new Function1<JetNamedDeclaration,JetFile>(){
    @Override public JetFile invoke(    JetNamedDeclaration declaration){
      return declaration.getContainingJetFile();
    }
  }
);
  for (  Map.Entry<JetFile,List<? extends JetNamedDeclaration>> entry : fileToElements.entrySet()) {
    if (entry.getKey().getDeclarations().size() != entry.getValue().size())     return false;
  }
  return true;
}
