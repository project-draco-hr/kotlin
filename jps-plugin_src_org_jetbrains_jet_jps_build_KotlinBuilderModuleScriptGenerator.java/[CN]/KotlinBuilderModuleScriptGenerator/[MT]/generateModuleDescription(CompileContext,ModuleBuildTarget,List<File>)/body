{
  CharSequence moduleScriptText=GENERATOR.generateModuleScript(target.getId(),getKotlinModuleDependencies(context,target),sourceFiles,target.isTests(),Collections.singleton(target.getOutputDir()));
  File scriptFile=new File(target.getOutputDir(),"script." + GENERATOR.getFileExtension());
  writeScriptToFile(context,moduleScriptText,scriptFile);
  return scriptFile;
}
