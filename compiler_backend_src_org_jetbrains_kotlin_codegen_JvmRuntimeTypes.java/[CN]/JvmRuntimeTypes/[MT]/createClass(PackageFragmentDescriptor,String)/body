{
  MutableClassDescriptor descriptor=new MutableClassDescriptor(packageFragment,ClassKind.CLASS,false,Name.identifier(name),SourceElement.NO_SOURCE);
  descriptor.setModality(Modality.FINAL);
  descriptor.setVisibility(Visibilities.PUBLIC);
  descriptor.setTypeParameterDescriptors(Collections.<TypeParameterDescriptor>emptyList());
  descriptor.createTypeConstructor();
  return descriptor;
}
