{
  ReceiverParameterDescriptor extensionReceiver=descriptor.getExtensionReceiverParameter();
  ReceiverParameterDescriptor dispatchReceiver=descriptor.getDispatchReceiverParameter();
  JetType receiverType=extensionReceiver != null ? extensionReceiver.getType() : dispatchReceiver != null ? dispatchReceiver.getType() : null;
  JetType functionType=getBuiltIns(descriptor).getFunctionType(Annotations.EMPTY,receiverType,ExpressionTypingUtils.getValueParametersTypes(descriptor.getValueParameters()),descriptor.getReturnType());
  return Arrays.asList(functionReference.getDefaultType(),functionType);
}
