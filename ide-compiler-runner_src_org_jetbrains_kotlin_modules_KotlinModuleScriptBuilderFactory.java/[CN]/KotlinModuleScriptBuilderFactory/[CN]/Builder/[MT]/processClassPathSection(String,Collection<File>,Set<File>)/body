{
  script.append("        // " + sectionDescription + "\n");
  for (  File file : files) {
    if (directoriesToFilterOut.contains(file)) {
      script.append("        // Output directory, commented out\n");
      script.append("        // ");
    }
    script.append("        classpath += \"" + toSystemIndependentName(file.getPath()) + "\"\n");
  }
}
