{
  if (lbraceType.equals(JetTokens.LONG_TEMPLATE_ENTRY_START)) {
    return false;
  }
  if (lbraceType == KDocTokens.WIKI_LINK_OPEN)   return false;
  return JetTokens.WHITE_SPACE_OR_COMMENT_BIT_SET.contains(contextType) || contextType == JetTokens.SEMICOLON || contextType == JetTokens.COMMA || contextType == JetTokens.RPAR || contextType == JetTokens.RBRACKET || contextType == JetTokens.RBRACE || contextType == JetTokens.LBRACE || contextType == JetTokens.LONG_TEMPLATE_ENTRY_END;
}
