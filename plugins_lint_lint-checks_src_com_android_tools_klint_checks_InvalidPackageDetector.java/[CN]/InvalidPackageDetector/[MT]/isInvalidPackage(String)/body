{
  if (owner.startsWith(JAVA_PKG_PREFIX)) {
    return !mApiDatabase.isValidJavaPackage(owner);
  }
  if (owner.startsWith(JAVAX_PKG_PREFIX)) {
    if (owner.startsWith("javax/annotation/") || owner.startsWith("javax/lang/model")) {
      return false;
    }
    return !mApiDatabase.isValidJavaPackage(owner);
  }
  return false;
}
