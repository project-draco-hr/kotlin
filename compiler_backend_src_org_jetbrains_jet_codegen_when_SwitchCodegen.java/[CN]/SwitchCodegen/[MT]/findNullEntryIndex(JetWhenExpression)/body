{
  int entryIndex=0;
  for (  JetWhenEntry entry : expression.getEntries()) {
    for (    CompileTimeConstant constant : SwitchCodegenUtil.getConstantsFromEntry(entry,bindingContext)) {
      if (constant instanceof NullValue) {
        return entryIndex;
      }
    }
    entryIndex++;
  }
  return -1;
}
