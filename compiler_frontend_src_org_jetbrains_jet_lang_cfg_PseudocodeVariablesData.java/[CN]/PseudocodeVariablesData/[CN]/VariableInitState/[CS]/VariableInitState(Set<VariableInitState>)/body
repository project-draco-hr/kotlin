{
  boolean isInitialized=true;
  boolean isDeclared=true;
  for (  VariableInitState edgeData : edgesData) {
    if (!edgeData.isInitialized) {
      isInitialized=false;
    }
    if (!edgeData.isDeclared) {
      isDeclared=false;
    }
  }
  this.isInitialized=isInitialized;
  this.isDeclared=isDeclared;
}
