{
  Assert.assertEquals(classToKotlin("enum Color {\n" + " WHITE(21), BLACK(22), RED(23), YELLOW(24), BLUE(25);\n" + "\n"+ " private int code;\n"+ "\n"+ " private Color(int c) {\n"+ "   code = c;\n"+ " }\n"+ "\n"+ " public int getCode() {\n"+ "   return code;\n"+ " }"),"enum Color(c : Int) {\n" + "WHITE(21)\n" + "BLACK(22)\n"+ "RED(23)\n"+ "YELLOW(24)\n"+ "BLUE(25)\n"+ "var code : Int\n"+ "public fun getCode() : Int {\n"+ "return code\n"+ "}\n"+ "}");
