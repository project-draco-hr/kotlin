{
  if (innerPsiClass.getName().equals(JvmAbi.CLASS_OBJECT_CLASS_NAME) || DescriptorResolverUtils.isInnerEnum(innerPsiClass,owner)) {
    return true;
  }
  if (innerPsiClass.isInterface() || psiClass.isInterface()) {
    return isStatic;
  }
 else {
    return innerPsiClass.hasModifierProperty(PsiModifier.STATIC) != isStatic;
  }
}
