{
  PsiMethod lightMethod=LightClassUtil.getLightClassMethod(getElement());
  if (lightMethod != null) {
    return new KotlinFindMethodUsagesDialog(lightMethod,getProject(),getFactory().getFindMethodOptions(),toShowInNewTab,mustOpenInNewTab,isSingleFile,this);
  }
  return super.getFindUsagesDialog(isSingleFile,toShowInNewTab,mustOpenInNewTab);
}
