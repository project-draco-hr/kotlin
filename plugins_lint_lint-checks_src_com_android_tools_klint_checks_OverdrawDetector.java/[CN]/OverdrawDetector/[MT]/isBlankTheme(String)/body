{
  if (name.startsWith("@android:style/Theme_")) {
    if (name.contains("NoFrame") || name.contains("Theme_Wallpaper") || name.contains("Theme_Holo_Wallpaper")|| name.contains("Theme_Translucent")|| name.contains("Theme_Dialog_NoFrame")|| name.contains("Theme_Holo_Dialog_Alert")|| name.contains("Theme_Holo_Light_Dialog_Alert")|| name.contains("Theme_Dialog_Alert")|| name.contains("Theme_Panel")|| name.contains("Theme_Light_Panel")|| name.contains("Theme_Holo_Panel")|| name.contains("Theme_Holo_Light_Panel")) {
      return true;
    }
  }
  return mBlankThemes != null && mBlankThemes.contains(name);
}
