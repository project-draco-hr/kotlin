{
  if (!member.getContainingClass().isEnum())   return false;
  if (member instanceof JavaField && ((JavaField)member).isEnumEntry())   return true;
  if (!(member instanceof JavaMethod))   return false;
  String signature=PsiFormatUtil.formatMethod(((JavaMethod)member).getPsi(),PsiSubstitutor.EMPTY,SHOW_NAME | SHOW_PARAMETERS,SHOW_TYPE | SHOW_FQ_CLASS_NAMES);
  return "values()".equals(signature) || "valueOf(java.lang.String)".equals(signature);
}
