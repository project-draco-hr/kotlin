{
  List<String> labels=new ArrayList<String>();
  if (mIgnore == null) {
    mIgnore=new HashSet<Element>();
  }
  for (  Element view : views) {
    if (view.getTagName().equals(BUTTON)) {
      String text=view.getAttributeNS(ANDROID_URI,ATTR_TEXT);
      String label=getLabel(text);
      labels.add(label);
      mIgnore.add(view);
    }
  }
  return labels;
}
