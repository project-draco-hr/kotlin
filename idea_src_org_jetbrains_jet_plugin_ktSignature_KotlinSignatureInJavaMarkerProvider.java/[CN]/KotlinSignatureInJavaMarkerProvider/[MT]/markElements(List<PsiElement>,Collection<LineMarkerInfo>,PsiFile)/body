{
  try {
    for (    PsiElement element : elements) {
      markElement(element,result);
    }
  }
 catch (  AssertionError error) {
    VirtualFile virtualFile=psiFile.getVirtualFile();
    Attachment[] attachments=virtualFile != null ? new Attachment[]{AttachmentFactory.createAttachment(virtualFile)} : new Attachment[]{};
    LOG.error(LogMessageEx.createEvent("Exception while collecting KotlinSignature markers",ExceptionUtil.getThrowableText(error),attachments));
  }
}
