{
  List<String> lines=loadLines(argsFilePath);
  return ArrayUtil.toStringArray(ContainerUtil.mapNotNull(lines,new Function<String,String>(){
    @Override public String fun(    String arg){
      if (arg.isEmpty()) {
        return null;
      }
      return arg.replace(":",File.pathSeparator).replace("$TEMP_DIR$",tempDir).replace("$TESTDATA_DIR$",testDataDir);
    }
  }
));
}
