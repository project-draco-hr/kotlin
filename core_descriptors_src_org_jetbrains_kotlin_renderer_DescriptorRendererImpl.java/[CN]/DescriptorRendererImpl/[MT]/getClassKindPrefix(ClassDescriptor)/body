{
  if (klass.isCompanionObject()) {
    return "companion object";
  }
switch (klass.getKind()) {
case CLASS:
    return "class";
case INTERFACE:
  return "interface";
case ENUM_CLASS:
return "enum class";
case OBJECT:
return "object";
case ANNOTATION_CLASS:
return "annotation class";
case ENUM_ENTRY:
return "enum entry";
default :
throw new IllegalStateException("unknown class kind: " + klass.getKind());
}
}
