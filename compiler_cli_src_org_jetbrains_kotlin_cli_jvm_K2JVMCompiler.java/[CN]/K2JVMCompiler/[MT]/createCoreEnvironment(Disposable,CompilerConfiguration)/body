{
  KotlinCoreEnvironment result=KotlinCoreEnvironment.createForProduction(rootDisposable,configuration,EnvironmentConfigFiles.JVM_CONFIG_FILES);
  long initNanos=System.nanoTime() - initStartNanos;
  reportPerf(configuration,"Compiler initialized in " + TimeUnit.NANOSECONDS.toMillis(initNanos) + " ms");
  return result;
}
