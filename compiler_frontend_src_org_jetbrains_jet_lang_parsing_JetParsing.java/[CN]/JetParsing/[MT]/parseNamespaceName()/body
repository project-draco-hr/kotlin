{
  PsiBuilder.Marker nsName=mark();
  while (true) {
    expect(IDENTIFIER,"Namespace name must be a '.'-separated identifier list",NAMESPACE_NAME_RECOVERY_SET);
    if (at(DOT)) {
      nsName.done(REFERENCE_EXPRESSION);
      advance();
      nsName=mark();
    }
 else {
      nsName.drop();
      break;
    }
  }
}
