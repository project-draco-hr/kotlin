{
  if (lastDot == -1) {
    parseAnnotations(REGULAR_ANNOTATIONS_ONLY_WITH_BRACKETS);
  }
 else {
    createTruncatedBuilder(lastDot).parseTypeRef();
    if (atSet(RECEIVER_TYPE_TERMINATORS)) {
      advance();
    }
 else {
      errorWithRecovery("Expecting '.' before a " + title + " name",nameFollow);
    }
  }
}
