{
  myBuilder.disableNewlines();
  advance();
  while (true) {
    PsiBuilder.Marker projection=mark();
    parseModifierList(MODIFIER_LIST,false);
    if (at(MUL)) {
      advance();
    }
 else {
      parseTypeRef(extraRecoverySet);
    }
    projection.done(TYPE_PROJECTION);
    if (!at(COMMA))     break;
    advance();
  }
  boolean atGT=at(GT);
  if (!atGT) {
    error("Expecting a '>'");
  }
 else {
    advance();
  }
  myBuilder.restoreNewlinesState();
  return atGT;
}
