{
  CancelableResolveSession resolveSession=WholeProjectAnalyzerFacade.getLazyResolveResultForFile((JetFile)getExpression().getContainingFile());
  BindingContext bindingContext=resolveSession.resolveToElement(getExpression());
  return DescriptorLookupConverter.collectLookupElements(resolveSession,bindingContext,TipsManager.getReferenceVariants(myExpression,bindingContext));
}
