{
  if (hasWhere()) {
    List<String> wheres=new LinkedList<String>();
    for (    Element t : myTypeParameters)     if (t instanceof TypeParameter)     wheres.add(((TypeParameter)t).getWhereToKotlin());
    return SPACE + "where" + SPACE+ join(wheres,COMMA_WITH_SPACE)+ SPACE;
  }
  return EMPTY;
}
