{
  PsiExpression initializer=((JavaFieldImpl)field).getInitializer();
  Object evaluatedExpression=JavaConstantExpressionEvaluator.computeConstantExpression(initializer,false);
  if (evaluatedExpression != null) {
    return ConstantsPackage.createCompileTimeConstant(evaluatedExpression,new CompileTimeConstant.Parameters.Impl(ConstantExpressionEvaluator.isPropertyCompileTimeConstant(descriptor),false,true),descriptor.getType());
  }
  return null;
}
