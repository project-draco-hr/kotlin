{
  return TranslatorTestCaseBuilder.suiteForDirectory(BasicTest.pathToTestFilesRoot() + "sourcemap/cases/",new TranslatorTestCaseBuilder.NamedTestFactory(){
    @NotNull @Override public Test createTest(    @NotNull String filename){
      SourceMapTest examplesTest=new SourceMapTest(filename);
      examplesTest.setName(filename);
      return examplesTest;
    }
  }
);
}
