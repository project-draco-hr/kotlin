{
  final JetExpression lhs=arguments.get(0);
  final Type lhsType=codegen.expressionType(lhs);
  codegen.generateStringBuilderConstructor();
  StackValue value=codegen.generateIntermediateValue(lhs);
  value.dupReceiver(v,1);
  value.put(lhsType,v);
  codegen.invokeAppendMethod(lhsType);
  codegen.invokeAppend(arguments.get(1));
  v.invokevirtual(ExpressionCodegen.CLASS_STRING_BUILDER,"toString","()Ljava/lang/String;");
  value.store(v);
  return null;
}
