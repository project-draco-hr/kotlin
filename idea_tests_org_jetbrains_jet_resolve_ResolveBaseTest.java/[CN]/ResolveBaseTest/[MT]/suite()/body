{
  TestSuite suite=new TestSuite();
  JetTestCaseBuilder.appendTestsInDirectory(PluginTestCaseBase.getTestDataPathBase(),"/resolve/",false,JetTestCaseBuilder.emptyFilter,new JetTestCaseBuilder.NamedTestFactory(){
    @NotNull @Override public Test createTest(    @NotNull String dataPath,    @NotNull String name,    @NotNull File file){
      return new ResolveBaseTest(dataPath,name);
    }
  }
,suite);
  return suite;
}
