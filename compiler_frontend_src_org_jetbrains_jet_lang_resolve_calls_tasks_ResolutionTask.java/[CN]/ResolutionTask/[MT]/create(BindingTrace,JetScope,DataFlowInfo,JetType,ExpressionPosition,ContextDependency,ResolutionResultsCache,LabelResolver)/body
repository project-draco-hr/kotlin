{
  ResolutionTask<D,F> newTask=new ResolutionTask<D,F>(candidates,reference,tracing,trace,scope,call,expectedType,dataFlowInfo,contextDependency,checkArguments,expressionPosition,resolutionResultsCache,labelResolver,dataFlowInfoForArguments);
  newTask.setCheckingStrategy(checkingStrategy);
  return newTask;
}
