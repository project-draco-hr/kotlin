{
  ResolutionTask<D,F> newTask=new ResolutionTask<D,F>(lazyCandidates,tracing,trace,scope,call,expectedType,dataFlowInfo,contextDependency,checkArguments,resolutionResultsCache,dataFlowInfoForArguments,callResolverExtension,resolvedCalls,isAnnotationContext,collectAllCandidates);
  newTask.setCheckingStrategy(checkingStrategy);
  return newTask;
}
