{
  BindingTraceContext bindingTraceContext=new BindingTraceContext();
  Project project=file.getProject();
  JetSemanticServices semanticServices=JetSemanticServices.createSemanticServices(project);
  return AnalyzingUtils.analyzeFilesWithGivenTrace(project,JsConfiguration.jsLibConfiguration(project),Collections.singletonList(file),Predicates.<PsiFile>alwaysTrue(),JetControlFlowDataTraceFactory.EMPTY,bindingTraceContext,semanticServices);
}
