{
  Object pn=shiftExpr(ts);
  while (ts.matchToken(ts.RELOP)) {
    int op=ts.getOp();
    if (inForInit && op == ts.IN) {
      ts.ungetToken(ts.RELOP);
      break;
    }
    sourceAdd((char)ts.RELOP);
    sourceAdd((char)op);
    pn=nf.createBinary(ts.RELOP,op,pn,shiftExpr(ts));
  }
  return pn;
}
