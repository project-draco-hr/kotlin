{
  File originalJavaFile=new File(javaFileName);
  File expectedFile=getTxtFile(javaFileName);
  File testPackageDir=new File(tmpdir,"test");
  assertTrue(testPackageDir.mkdir());
  FileUtil.copy(originalJavaFile,new File(testPackageDir,originalJavaFile.getName()));
  Pair<NamespaceDescriptor,BindingContext> javaNamespaceAndContext=loadTestNamespaceAndBindingContextFromJavaRoot(tmpdir,getTestRootDisposable(),ConfigurationKind.JDK_ONLY);
  checkJavaNamespace(expectedFile,javaNamespaceAndContext.first,javaNamespaceAndContext.second,DONT_INCLUDE_METHODS_OF_OBJECT.withValidationStrategy(ALLOW_ERROR_TYPES));
}
