{
  ExitCode exitCode=new K2JVMCompiler().exec(System.out,"-output",destDir.getPath(),"-src",src,"-noStdlib","-noJdkAnnotations","-suppress","warnings","-annotations","./jdk-annotations","-classpath",classPath);
  if (exitCode != ExitCode.OK) {
    throw new IllegalStateException("Compilation of " + debugName + " failed: "+ exitCode);
  }
}
