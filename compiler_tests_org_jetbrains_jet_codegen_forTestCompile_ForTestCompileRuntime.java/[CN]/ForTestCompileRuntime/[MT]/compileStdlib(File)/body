{
  ExitCode exitCode=new K2JVMCompiler().exec(System.out,"-output",destDir.getPath(),"-src","./libraries/stdlib/src","-noStdlib","-noJdkAnnotations","-suppress","warnings","-annotations","./jdk-annotations","-classpath","out/production/builtins" + File.pathSeparator + "out/production/runtime.jvm");
  if (exitCode != ExitCode.OK) {
    throw new IllegalStateException("stdlib for test compilation failed: " + exitCode);
  }
}
