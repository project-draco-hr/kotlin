{
  KotlinCompiler.ExitCode exitCode=new KotlinCompiler().exec(System.err,"-output",destdir.getPath(),"-src","./libraries/stdlib/src","-mode","stdlib","-classpath","out/production/runtime");
  if (exitCode != KotlinCompiler.ExitCode.OK) {
    throw new IllegalStateException("stdlib for test compilation failed: " + exitCode);
  }
}
