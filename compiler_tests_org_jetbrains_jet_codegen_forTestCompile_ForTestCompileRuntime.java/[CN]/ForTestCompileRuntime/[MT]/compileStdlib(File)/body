{
  ExitCode exitCode=new K2JVMCompiler().exec(System.out,"-output",destdir.getPath(),"-src","./libraries/stdlib/src","-noStdlib","-noJdkAnnotations","-suppress","warnings","-annotations","./jdk-annotations","-classpath","out/production/runtime");
  if (exitCode != ExitCode.OK) {
    throw new IllegalStateException("stdlib for test compilation failed: " + exitCode);
  }
}
