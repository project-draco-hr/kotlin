{
  String src=BUILT_INS_SRC_PATH + File.pathSeparator + RUNTIME_JVM_SRC_PATH;
  compileKotlinToJvm("built-ins",destDir,src,src);
  JetTestUtils.compileJavaFiles(ContainerUtil.concat(javaFilesUnder(BUILT_INS_SRC_PATH),javaFilesUnder(RUNTIME_JVM_SRC_PATH)),Arrays.asList("-classpath",destDir.getPath(),"-d",destDir.getPath()));
}
