{
  JvmMethodSignature descriptor=erasedInvokeSignature(fd);
  JvmClassName owner=getInternalClassName(fd);
  final CallableMethod result=new CallableMethod(owner,null,null,descriptor,INVOKEVIRTUAL,getInternalType(fd),fd.getReceiverParameter().exists() ? fd.getOriginal() : null,getInternalClassName(fd).getAsmType());
  return result;
}
