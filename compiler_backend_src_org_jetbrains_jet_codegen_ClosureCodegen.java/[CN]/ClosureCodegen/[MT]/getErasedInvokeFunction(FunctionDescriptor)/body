{
  int arity=funDescriptor.getValueParameters().size();
  ClassDescriptor funClass=funDescriptor.getReceiverParameter() == null ? KotlinBuiltIns.getInstance().getFunction(arity) : KotlinBuiltIns.getInstance().getExtensionFunction(arity);
  return funClass.getDefaultType().getMemberScope().getFunctions(Name.identifier("invoke")).iterator().next();
}
