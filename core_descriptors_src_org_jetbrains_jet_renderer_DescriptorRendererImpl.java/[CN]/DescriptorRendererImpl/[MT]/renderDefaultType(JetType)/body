{
  StringBuilder sb=new StringBuilder();
  if (type.isError()) {
    sb.append(type.getConstructor().toString());
  }
 else {
    sb.append(renderTypeName(type.getConstructor()));
  }
  sb.append(renderTypeArguments(type.getArguments()));
  if (type.isNullable()) {
    sb.append("?");
  }
  return sb.toString();
}
