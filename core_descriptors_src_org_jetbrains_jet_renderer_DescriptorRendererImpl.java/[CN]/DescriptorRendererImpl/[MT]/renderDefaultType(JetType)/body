{
  StringBuilder sb=new StringBuilder();
  if (type.isError()) {
    sb.append(type.getConstructor().toString());
  }
 else {
    sb.append(renderTypeName(type.getConstructor()));
  }
  if (!type.getArguments().isEmpty()) {
    sb.append("<");
    appendTypeProjections(type.getArguments(),sb);
    sb.append(">");
  }
  if (type.isNullable()) {
    sb.append("?");
  }
  return sb.toString();
}
