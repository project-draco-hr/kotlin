{
  if (TypesPackage.isFlexible(type)) {
    if (!debugMode) {
      return renderFlexibleType(type);
    }
 else {
      return "(" + renderNormalizedType(TypesPackage.flexibility(type).getLowerBound()) + ".."+ renderNormalizedType(TypesPackage.flexibility(type).getUpperBound())+ ")";
    }
  }
  return renderInflexibleType(type);
}
