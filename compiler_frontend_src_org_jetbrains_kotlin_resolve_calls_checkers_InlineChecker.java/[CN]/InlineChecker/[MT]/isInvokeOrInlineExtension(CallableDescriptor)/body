{
  if (!(descriptor instanceof SimpleFunctionDescriptor)) {
    return false;
  }
  DeclarationDescriptor containingDeclaration=descriptor.getContainingDeclaration();
  boolean isInvoke=descriptor.getName().equals(OperatorNameConventions.INVOKE) && containingDeclaration instanceof ClassDescriptor && KotlinBuiltIns.isExactFunctionOrExtensionFunctionType(((ClassDescriptor)containingDeclaration).getDefaultType());
  return isInvoke || InlineUtil.isInline(descriptor);
}
