{
  if (!(super.validate() && (myJavaStep == null || myJavaStep.validate())))   return false;
  Boolean selected=isLibrarySelected();
  if (selected != null && !selected) {
    int result=Messages.showYesNoDialog("Do you want to continue with no Kotlin Runtime library?","No Kotlin Runtime Specified",Messages.getWarningIcon());
    if (result != Messages.YES) {
      return false;
    }
  }
  return true;
}
