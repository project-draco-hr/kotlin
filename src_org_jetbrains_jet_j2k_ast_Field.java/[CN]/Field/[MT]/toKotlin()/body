{
  String modifier=modifiersToKotlin();
  if (myInitializer.isEmpty())   return modifier + myIdentifier.toKotlin() + SPACE+ COLON+ SPACE+ myType.toKotlin()+ SPACE+ EQUAL+ SPACE+ getDefaultInitializer(this);
  return modifier + myIdentifier.toKotlin() + SPACE+ COLON+ SPACE+ myType.toKotlin()+ SPACE+ EQUAL+ SPACE+ myInitializer.toKotlin();
}
