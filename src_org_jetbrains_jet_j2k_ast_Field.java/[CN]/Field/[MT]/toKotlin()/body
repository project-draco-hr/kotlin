{
  String modifier=(myModifiers.contains(Modifier.FINAL) ? "val" : "var") + SPACE;
  if (myInitializer.toKotlin().isEmpty())   return modifier + myIdentifier.toKotlin() + SPACE+ COLON+ SPACE+ myType.toKotlin();
  return modifier + myIdentifier.toKotlin() + SPACE+ COLON+ SPACE+ myType.toKotlin()+ SPACE+ EQUAL+ SPACE+ myInitializer.toKotlin();
}
