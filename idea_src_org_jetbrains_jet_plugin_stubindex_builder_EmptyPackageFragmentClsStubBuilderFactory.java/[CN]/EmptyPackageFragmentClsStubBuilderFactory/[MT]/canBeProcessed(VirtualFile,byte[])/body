{
  if (file.getName().contains(PackageClassUtils.PACKAGE_CLASS_NAME_SUFFIX + "-") && StdFileTypes.CLASS.getDefaultExtension().equals(file.getExtension())) {
    KotlinJvmBinaryClass kotlinClass=new VirtualFileKotlinClass(file);
    return KotlinClassHeader.read(kotlinClass) instanceof PackageFragmentClassHeader;
  }
  return false;
}
