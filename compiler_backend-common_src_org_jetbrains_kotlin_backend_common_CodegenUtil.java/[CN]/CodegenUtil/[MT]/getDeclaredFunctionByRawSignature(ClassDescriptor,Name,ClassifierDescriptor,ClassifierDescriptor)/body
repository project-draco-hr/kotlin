{
  Collection<FunctionDescriptor> functions=owner.getDefaultType().getMemberScope().getFunctions(name,UsageLocation.NO_LOCATION);
  for (  FunctionDescriptor function : functions) {
    if (!CallResolverUtilPackage.isOrOverridesSynthesized(function) && function.getTypeParameters().isEmpty() && valueParameterClassesMatch(function.getValueParameters(),Arrays.asList(valueParameterClassifiers))&& rawTypeMatches(function.getReturnType(),returnedClassifier)) {
      return function;
    }
  }
  return null;
}
