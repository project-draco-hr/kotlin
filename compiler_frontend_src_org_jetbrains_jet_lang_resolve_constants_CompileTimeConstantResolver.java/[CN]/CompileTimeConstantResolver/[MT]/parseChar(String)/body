{
  if (text.length() < 2 || text.charAt(0) != '\'' || text.charAt(text.length() - 1) != '\'') {
    return null;
  }
  text=text.substring(1,text.length() - 1);
  if (text.length() == 0) {
    return null;
  }
  if (text.charAt(0) != '\\') {
    if (text.length() == 1) {
      return text.charAt(0);
    }
  }
  return escapedStringToChar(text);
}
