{
  return ContainerUtil.map(getDelegate().getMethods(),new Function<PsiMethod,PsiMethod>(){
    @Override public PsiMethod fun(    PsiMethod method){
      JetDeclaration declaration=ClsWrapperStubPsiFactory.getOriginalDeclaration(method);
      return declaration != null ? new KotlinLightMethodForDeclaration(myManager,method,declaration,KotlinWrappingLightClass.this) : new LightMethod(myManager,method,KotlinWrappingLightClass.this);
    }
  }
);
}
