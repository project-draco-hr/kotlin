{
  return ContainerUtil.map(getDelegate().getFields(),new Function<PsiField,PsiField>(){
    @Override public PsiField fun(    PsiField field){
      JetDeclaration declaration=ClsWrapperStubPsiFactory.getOriginalDeclaration(field);
      return declaration instanceof JetProperty ? new KotlinLightField(myManager,(JetProperty)declaration,field,KotlinWrappingLightClass.this) : new LightField(myManager,field,KotlinWrappingLightClass.this);
    }
  }
);
}
