{
  if (targetDirectory == null)   return Collections.emptyList();
  List<String> fileNames=targetFileName != null ? Collections.singletonList(targetFileName) : CollectionsKt.map(sourceFiles,new Function1<KtFile,String>(){
    @Override public String invoke(    KtFile jetFile){
      return jetFile.getName();
    }
  }
);
  return CollectionsKt.filterNotNull(CollectionsKt.map(fileNames,new Function1<String,PsiFile>(){
    @Override public PsiFile invoke(    String s){
      return targetDirectory.findFile(s);
    }
  }
));
}
