{
  return HAS_ANNOTATIONS.toFlags(hasAnnotations) | VISIBILITY.toFlags(visibility(visibility)) | MODALITY.toFlags(modality(modality))| MEMBER_KIND.toFlags(memberKind(memberKind))| IS_OPERATOR.toFlags(isOperator)| IS_INFIX.toFlags(isInfix)| IS_INLINE.toFlags(isInline)| IS_TAILREC.toFlags(isTailrec)| IS_EXTERNAL_FUNCTION.toFlags(isExternal);
}
