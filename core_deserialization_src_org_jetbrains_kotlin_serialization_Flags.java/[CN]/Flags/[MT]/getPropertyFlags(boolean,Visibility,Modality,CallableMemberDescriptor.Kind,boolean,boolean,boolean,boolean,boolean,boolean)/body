{
  return HAS_ANNOTATIONS.toFlags(hasAnnotations) | VISIBILITY.toFlags(visibility(visibility)) | MODALITY.toFlags(modality(modality))| MEMBER_KIND.toFlags(memberKind(memberKind))| IS_VAR.toFlags(isVar)| HAS_GETTER.toFlags(hasGetter)| HAS_SETTER.toFlags(hasSetter)| IS_CONST.toFlags(isConst)| LATE_INIT.toFlags(lateInit)| HAS_CONSTANT.toFlags(hasConstant);
}
