{
  KotlinFileStub stub=getStub();
  JetModifierList[] danglingModifierLists=stub == null ? findChildrenByClass(JetModifierList.class) : stub.getChildrenByType(JetStubElementTypes.MODIFIER_LIST,JetStubElementTypes.MODIFIER_LIST.getArrayFactory());
  return ArraysKt.flatMap(danglingModifierLists,new Function1<JetModifierList,Iterable<JetAnnotationEntry>>(){
    @Override public Iterable<JetAnnotationEntry> invoke(    JetModifierList modifierList){
      return modifierList.getAnnotationEntries();
    }
  }
);
}
