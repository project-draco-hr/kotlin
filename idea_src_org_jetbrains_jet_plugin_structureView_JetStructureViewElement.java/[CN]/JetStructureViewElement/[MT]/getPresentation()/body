{
  return new ItemPresentation(){
    @Override public String getPresentableText(){
      if (elementText == null) {
        elementText=getElementText();
      }
      return elementText;
    }
    @Override public String getLocationString(){
      return null;
    }
    @Override public Icon getIcon(    boolean open){
      if (myElement.isValid()) {
        return PsiIconUtil.getProvidersIcon(myElement,open ? Iconable.ICON_FLAG_OPEN : Iconable.ICON_FLAG_CLOSED);
      }
      return null;
    }
  }
;
}
