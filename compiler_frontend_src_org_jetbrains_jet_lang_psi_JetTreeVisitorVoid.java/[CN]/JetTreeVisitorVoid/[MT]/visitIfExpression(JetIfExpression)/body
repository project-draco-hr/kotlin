{
  JetExpression condition=expression.getCondition();
  if (condition != null) {
    condition.accept(this);
  }
  JetExpression then=expression.getThen();
  if (then != null) {
    then.accept(this);
  }
  JetExpression anElse=expression.getElse();
  if (anElse != null) {
    anElse.accept(this);
  }
}
