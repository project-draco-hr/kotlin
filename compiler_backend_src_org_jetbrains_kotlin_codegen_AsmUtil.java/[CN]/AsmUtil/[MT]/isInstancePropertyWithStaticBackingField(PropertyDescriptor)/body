{
  if (propertyDescriptor.getKind() == CallableMemberDescriptor.Kind.FAKE_OVERRIDE) {
    return false;
  }
  DeclarationDescriptor container=propertyDescriptor.getContainingDeclaration();
  return isNonCompanionObject(container) || isPropertyWithBackingFieldInOuterClass(propertyDescriptor) || (isCompanionObject(container) && isInterface(container.getContainingDeclaration()));
}
