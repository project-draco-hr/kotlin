{
  if (propertyDescriptor.getKind() == CallableMemberDescriptor.Kind.FAKE_OVERRIDE) {
    return false;
  }
  return isNonCompanionObject(propertyDescriptor.getContainingDeclaration()) || isPropertyWithBackingFieldInOuterClass(propertyDescriptor) || isInterfaceCompanionObject(propertyDescriptor.getContainingDeclaration());
}
