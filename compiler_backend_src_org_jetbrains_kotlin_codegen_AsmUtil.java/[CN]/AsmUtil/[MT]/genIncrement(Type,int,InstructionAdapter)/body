{
  if (expectedType == Type.LONG_TYPE) {
    v.lconst(myDelta);
  }
 else   if (expectedType == Type.FLOAT_TYPE) {
    v.fconst(myDelta);
  }
 else   if (expectedType == Type.DOUBLE_TYPE) {
    v.dconst(myDelta);
  }
 else {
    v.iconst(myDelta);
    v.add(Type.INT_TYPE);
    StackValue.coerce(Type.INT_TYPE,expectedType,v);
    return;
  }
  v.add(expectedType);
}
