{
  Project project=file.getProject();
  if (file.getOriginalFile() != file) {
    return CachedValuesManager.getManager(project).getParameterizedCachedValue(project,PER_FILE_LAZY_RESOLVE_SESSION,PerFileLazyResolveSessionProvider.INSTANCE,true,file);
  }
  return KotlinCacheManager.getInstance(project).getLazyResolveSession(TargetPlatformDetector.getPlatform(file));
}
