{
  List<File> extras=new ArrayList<File>();
  extras.addAll(extraClassPath);
  extras.add(JetTestUtils.getAnnotationsJar());
  return new JetCoreEnvironment(getTestRootDisposable(),JetTestUtils.compilerConfigurationForTests(ConfigurationKind.ALL,TestJdkKind.MOCK_JDK,extras.toArray(new File[extras.size()])));
}
