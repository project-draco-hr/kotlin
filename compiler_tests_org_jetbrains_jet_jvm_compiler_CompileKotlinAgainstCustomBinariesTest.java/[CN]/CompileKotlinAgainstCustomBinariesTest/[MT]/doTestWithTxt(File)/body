{
  PackageViewDescriptor packageView=analyzeFileToPackageView(extraClassPath);
  RecursiveDescriptorComparator.Configuration comparator=RecursiveDescriptorComparator.DONT_INCLUDE_METHODS_OF_OBJECT.withValidationStrategy(DescriptorValidator.ValidationVisitor.ALLOW_ERROR_TYPES);
  validateAndCompareDescriptorWithFile(packageView,comparator,getTestDataFileWithExtension("txt"));
}
