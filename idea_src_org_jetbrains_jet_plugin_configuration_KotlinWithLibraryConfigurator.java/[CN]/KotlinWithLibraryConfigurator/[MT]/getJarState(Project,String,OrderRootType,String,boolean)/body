{
  String pathFromLibrary=getPathFromLibrary(project,jarType);
  if (getFileInDir(jarName,copyPath).exists()) {
    return FileState.EXISTS;
  }
 else   if (pathFromLibrary != null) {
    return FileState.COPY;
  }
 else   if (useBundled) {
    return FileState.DO_NOT_COPY;
  }
 else {
    return FileState.COPY;
  }
}
