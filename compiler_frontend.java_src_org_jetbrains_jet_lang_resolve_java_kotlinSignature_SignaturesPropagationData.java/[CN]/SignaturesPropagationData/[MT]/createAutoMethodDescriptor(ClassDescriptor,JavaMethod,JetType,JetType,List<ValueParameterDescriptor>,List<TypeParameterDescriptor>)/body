{
  JavaMethodDescriptor autoMethodDescriptor=JavaMethodDescriptor.createJavaMethod(containingClass,Annotations.EMPTY,method.getName());
  autoMethodDescriptor.initialize(receiverType,containingClass.getThisAsReceiverParameter(),autoTypeParameters,autoValueParameters,autoReturnType,Modality.OPEN,Visibilities.PUBLIC);
  return autoMethodDescriptor;
}
