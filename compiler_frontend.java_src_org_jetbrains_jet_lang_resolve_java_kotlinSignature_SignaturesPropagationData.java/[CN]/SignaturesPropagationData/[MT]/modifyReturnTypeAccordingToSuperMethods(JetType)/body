{
  List<TypeAndVariance> typesFromSuperMethods=ContainerUtil.map(superFunctions,new Function<FunctionDescriptor,TypeAndVariance>(){
    @Override public TypeAndVariance fun(    FunctionDescriptor superFunction){
      return new TypeAndVariance(superFunction.getReturnType(),Variance.OUT_VARIANCE);
    }
  }
);
  return modifyTypeAccordingToSuperMethods(autoType,typesFromSuperMethods);
}
