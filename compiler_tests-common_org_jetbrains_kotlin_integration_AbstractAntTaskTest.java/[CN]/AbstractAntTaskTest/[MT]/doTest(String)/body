{
  String testDataDir=new File(testFile).getAbsolutePath();
  runJava(testDataDir,"build.log","-jar",getAntHome() + File.separator + "lib"+ File.separator+ "ant-launcher.jar","-Dkotlin.lib=" + KotlinIntegrationTestBase.getCompilerLib(),"-Dkotlin.runtime.jar=" + ForTestCompileRuntime.runtimeJarForTests().getAbsolutePath(),"-Dkotlin.reflect.jar=" + ForTestCompileRuntime.reflectJarForTests().getAbsolutePath(),"-Dtest.data=" + testDataDir,"-Dtemp=" + tmpdir,"-f","build.xml");
}
