{
  return argument instanceof UUnaryExpression && ((UUnaryExpression)argument).getOperator() == UastPrefixOperator.UNARY_MINUS && UastLiteralUtils.isIntegralLiteral(((UUnaryExpression)argument).getOperand()) && UastLiteralUtils.getLongValue((ULiteralExpression)((UUnaryExpression)argument).getOperand()) == 1;
}
