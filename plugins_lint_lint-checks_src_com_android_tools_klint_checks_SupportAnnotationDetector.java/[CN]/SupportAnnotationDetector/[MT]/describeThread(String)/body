{
  if (UI_THREAD_ANNOTATION.equals(annotation)) {
    return "UI";
  }
 else   if (MAIN_THREAD_ANNOTATION.equals(annotation)) {
    return "main";
  }
 else   if (BINDER_THREAD_ANNOTATION.equals(annotation)) {
    return "binder";
  }
 else   if (WORKER_THREAD_ANNOTATION.equals(annotation)) {
    return "worker";
  }
 else {
    return "other";
  }
}
