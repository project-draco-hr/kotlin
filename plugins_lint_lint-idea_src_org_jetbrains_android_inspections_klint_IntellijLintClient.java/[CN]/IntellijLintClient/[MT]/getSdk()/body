{
  if (mSdk == null) {
    Module module=getModule();
    LocalSdk sdk=getLocalSdk(module);
    if (sdk != null) {
      mSdk=sdk;
    }
 else {
      for (      Module m : ModuleManager.getInstance(myProject).getModules()) {
        sdk=getLocalSdk(m);
        if (sdk != null) {
          mSdk=sdk;
          break;
        }
      }
      if (mSdk == null) {
        mSdk=super.getSdk();
      }
    }
  }
  return mSdk;
}
