{
  myClsFile=clsFile;
  Project project=myClsFile.getProject();
  JavaSemanticServices jss=new JavaSemanticServices(project,JetSemanticServices.createSemanticServices(project),new BindingTraceContext());
  myBindingContext=jss.getTrace().getBindingContext();
  myJavaDescriptorResolver=jss.getDescriptorResolver();
}
