{
  ProgressIndicatorProvider.checkCanceled();
  ReceiverValue dispatchReceiver=c.context.call.getDispatchReceiver();
  boolean resolveInvoke=dispatchReceiver.exists() && !TypesPackage.isDynamic(dispatchReceiver.getType());
  if (resolveInvoke) {
    addCandidatesForInvoke(receiver,c);
    return;
  }
  Collection<ReceiverValue> implicitReceivers=Sets.newLinkedHashSet(JetScopeUtils.getImplicitReceiversHierarchyValues(c.scope));
  if (receiver.exists()) {
    addCandidatesForExplicitReceiver(receiver,implicitReceivers,c,true);
    addMembers(receiver,c,true,true);
    return;
  }
  addCandidatesForNoReceiver(implicitReceivers,c);
}
