{
  ResolutionTaskHolder<D,F> result=new ResolutionTaskHolder<D,F>(storageManager,context,new MyPriorityProvider<D>(context),tracing);
  result.addCandidates(new Function0<Collection<? extends ResolutionCandidate<D>>>(){
    @Override public Collection<? extends ResolutionCandidate<D>> invoke(){
      return candidates;
    }
  }
);
  return result.getTasks();
}
