{
  CompilerConfiguration configurationWithADirInClasspath=JetTestUtils.compilerConfigurationForTests(ConfigurationKind.ALL,TestJdkKind.MOCK_JDK,JetTestUtils.getAnnotationsJar(),aDir);
  KotlinCoreEnvironment environment=KotlinCoreEnvironment.createForTests(getTestRootDisposable(),configurationWithADirInClasspath,EnvironmentConfigFiles.JVM_CONFIG_FILES);
  return compileKotlin(ktBFile,bDir,environment,getTestRootDisposable());
}
