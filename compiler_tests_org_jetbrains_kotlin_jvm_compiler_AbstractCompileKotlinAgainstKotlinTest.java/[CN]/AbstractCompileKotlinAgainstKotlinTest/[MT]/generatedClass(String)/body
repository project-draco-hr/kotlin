{
  URLClassLoader classLoader=new URLClassLoader(new URL[]{bDir.toURI().toURL(),aDir.toURI().toURL()},ForTestCompileRuntime.runtimeAndReflectJarClassLoader());
  String fileLastName=new File(fileName).getName();
  return classLoader.loadClass(PackagePartClassUtils.getFilePartShortName(fileLastName));
}
