{
  this.v=v;
  name=fqName;
  this.state=state;
  this.files=files;
  final PsiFile sourceFile=files.iterator().next().getContainingFile();
  v.addOptionalDeclaration(new ClassBuilderOnDemand.ClassBuilderCallback(){
    @Override public void doSomething(    @NotNull ClassBuilder v){
      v.defineClass(sourceFile,V1_6,ACC_PUBLIC,getJVMClassNameForKotlinNs(fqName).getInternalName(),null,"java/lang/Object",new String[0]);
      v.visitSource(sourceFile.getName(),null);
    }
  }
);
}
