{
  if (fqName.length() == 0) {
    return JvmAbi.PACKAGE_CLASS;
  }
  String name=fqName.replace('.','/') + "/" + JvmAbi.PACKAGE_CLASS;
  if (name.startsWith("<java_root>")) {
    name=name.substring("<java_root>".length() + 1,name.length() - 1 - JvmAbi.PACKAGE_CLASS.length());
  }
  return name;
}
