{
  if (fqName.isRoot()) {
    return JvmAbi.PACKAGE_CLASS;
  }
  String name=fqName.getFqName().replace('.','/');
  if (name.startsWith("<java_root>")) {
    name=name.substring("<java_root>".length() + 1,name.length());
  }
  if (namespace) {
    name+="/" + JvmAbi.PACKAGE_CLASS;
  }
  return name;
}
