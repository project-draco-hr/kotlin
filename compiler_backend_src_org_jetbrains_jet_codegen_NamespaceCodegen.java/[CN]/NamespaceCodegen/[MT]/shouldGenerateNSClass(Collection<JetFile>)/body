{
  checkAllFilesHaveSameNamespace(namespaceFiles);
  for (  JetFile file : namespaceFiles) {
    for (    JetDeclaration declaration : file.getDeclarations()) {
      if (declaration instanceof JetProperty || declaration instanceof JetNamedFunction || declaration instanceof JetObjectDeclaration) {
        return true;
      }
    }
  }
  return false;
}
