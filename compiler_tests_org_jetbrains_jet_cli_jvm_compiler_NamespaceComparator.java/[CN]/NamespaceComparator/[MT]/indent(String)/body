{
  try {
    StringBuilder r=new StringBuilder();
    BufferedReader reader=new BufferedReader(new StringReader(string));
    for (; ; ) {
      String line=reader.readLine();
      if (line == null) {
        break;
      }
      r.append("    ");
      r.append(line);
      r.append("\n");
    }
    return r.toString();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
