{
  if (assignmentType != null && !KotlinBuiltIns.isUnit(assignmentType) && !noExpectedType(context.expectedType) && TypeUtils.equalTypes(context.expectedType,assignmentType)) {
    context.trace.report(Errors.ASSIGNMENT_TYPE_MISMATCH.on(expression,context.expectedType));
    return null;
  }
  return DataFlowUtils.checkStatementType(expression,context);
}
