{
  final List<KtType> variants=getSmartCastVariants(receiverToCast,bindingContext,containingDeclarationOrModule,dataFlowInfo);
  return CollectionsKt.filter(variants,new Function1<KtType,Boolean>(){
    @Override public Boolean invoke(    final KtType type){
      return !CollectionsKt.any(variants,new Function1<KtType,Boolean>(){
        @Override public Boolean invoke(        KtType another){
          return another != type && KotlinTypeChecker.DEFAULT.isSubtypeOf(another,type);
        }
      }
);
    }
  }
);
}
