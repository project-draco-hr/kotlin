{
  final List<KotlinType> variants=getSmartCastVariants(receiverToCast,bindingContext,containingDeclarationOrModule,dataFlowInfo);
  return CollectionsKt.filter(variants,new Function1<KotlinType,Boolean>(){
    @Override public Boolean invoke(    final KotlinType type){
      return !CollectionsKt.any(variants,new Function1<KotlinType,Boolean>(){
        @Override public Boolean invoke(        KotlinType another){
          return another != type && KotlinTypeChecker.DEFAULT.isSubtypeOf(another,type);
        }
      }
);
    }
  }
);
}
