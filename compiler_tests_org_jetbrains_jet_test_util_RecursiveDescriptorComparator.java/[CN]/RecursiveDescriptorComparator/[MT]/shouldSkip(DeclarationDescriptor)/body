{
  return subDescriptor.getContainingDeclaration() instanceof ClassDescriptor && subDescriptor instanceof FunctionDescriptor && KOTLIN_ANY_METHOD_NAMES.contains(subDescriptor.getName().asString())&& !conf.includeMethodsOfKotlinAny || subDescriptor instanceof PackageViewDescriptor && !conf.recurseIntoPackage.apply(((PackageViewDescriptor)subDescriptor).getFqName());
}
