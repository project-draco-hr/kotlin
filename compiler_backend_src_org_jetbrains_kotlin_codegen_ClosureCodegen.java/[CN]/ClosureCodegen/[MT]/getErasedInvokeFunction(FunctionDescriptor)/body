{
  int arity=elementDescriptor.getValueParameters().size();
  ClassDescriptor elementClass=elementDescriptor.getExtensionReceiverParameter() == null ? DescriptorUtilsKt.getBuiltIns(elementDescriptor).getFunction(arity) : DescriptorUtilsKt.getBuiltIns(elementDescriptor).getExtensionFunction(arity);
  KtScope scope=elementClass.getDefaultType().getMemberScope();
  return scope.getFunctions(OperatorNameConventions.INVOKE,NoLookupLocation.FROM_BACKEND).iterator().next();
}
