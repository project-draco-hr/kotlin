{
  Set<VariableDescriptor> result=Sets.newLinkedHashSet();
  Collection<JetProperty> declarations=declarationProvider.getPropertyDeclarations(name);
  for (  JetProperty propertyDeclaration : declarations) {
    JetScope resolutionScope=getScopeForMemberDeclarationResolution(propertyDeclaration);
    PropertyDescriptor propertyDescriptor=resolveSession.getDescriptorResolver().resolvePropertyDescriptor(thisDescriptor,resolutionScope,propertyDeclaration,trace,DataFlowInfo.EMPTY);
    result.add(propertyDescriptor);
    resolveSession.getAnnotationResolver().resolveAnnotationsArguments(propertyDescriptor,trace,resolutionScope);
  }
  getNonDeclaredProperties(name,result);
  return result;
}
