{
  String description;
  String kind=psiClass.isInterface() ? "interface " : "class ";
  description=kind + PsiFormatUtil.formatClass(psiClass,PsiFormatUtilBase.SHOW_CONTAINING_CLASS | PsiFormatUtilBase.SHOW_NAME | PsiFormatUtilBase.SHOW_PARAMETERS| PsiFormatUtilBase.SHOW_TYPE);
  description=wrapOrSkip(description,inCode);
  return markAsJava ? "[Java] " + description : description;
}
