{
  if (requestor == this) {
    return false;
  }
  FunctionDescriptor paramOwner=(FunctionDescriptor)parameterDescriptor.getContainingDeclaration();
  UsageTracker p=parent;
  do {
    if (p.memberDescriptor == paramOwner) {
      return true;
    }
  }
 while ((p=p.parent) != null && p != requestor);
  return false;
}
