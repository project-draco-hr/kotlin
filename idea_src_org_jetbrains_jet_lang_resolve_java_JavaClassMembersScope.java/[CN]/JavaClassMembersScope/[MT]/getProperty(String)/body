{
  PsiField field=psiClass.findFieldByName(name,true);
  if (field == null)   return null;
  if (field.hasModifierProperty(PsiModifier.STATIC) != staticMembers) {
    return null;
  }
  PropertyDescriptorImpl propertyDescriptor=new PropertyDescriptorImpl(containingDeclaration,Collections.<Attribute>emptyList(),field.getName(),semanticServices.getTypeTransformer().transform(field.getType()));
  semanticServices.getTrace().recordDeclarationResolution(field,propertyDescriptor);
  return propertyDescriptor;
}
