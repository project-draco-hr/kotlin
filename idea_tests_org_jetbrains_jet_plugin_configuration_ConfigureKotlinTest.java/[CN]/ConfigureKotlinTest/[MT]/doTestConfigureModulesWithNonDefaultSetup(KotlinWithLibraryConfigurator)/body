{
  assertNoFilesInDefaultPaths();
  Module[] modules=getModules();
  for (  Module module : modules) {
    assertNotConfigured(module,configurator);
  }
  configurator.configure(myProject);
  assertNoFilesInDefaultPaths();
  for (  Module module : modules) {
    assertProperlyConfigured(module,configurator);
  }
}
