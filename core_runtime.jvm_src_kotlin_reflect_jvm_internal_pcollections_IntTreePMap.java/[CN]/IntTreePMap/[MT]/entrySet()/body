{
  if (entrySet == null)   entrySet=new AbstractSet<Entry<Integer,V>>(){
    @Override public int size(){
      return IntTreePMap.this.size();
    }
    @Override public Iterator<Entry<Integer,V>> iterator(){
      return root.iterator();
    }
    @Override public boolean contains(    final Object e){
      if (!(e instanceof Entry))       return false;
      V value=get(((Entry<?,?>)e).getKey());
      return value != null && value.equals(((Entry<?,?>)e).getValue());
    }
  }
;
  return entrySet;
}
