{
  if (getAnnotationByName(descriptor,ENUMERABLE) != null) {
    return true;
  }
  ClassDescriptor containingClass=getContainingClass(descriptor);
  return containingClass != null && (getAnnotationByName(containingClass,ENUMERABLE) != null || (containingClass.getKind().equals(ClassKind.OBJECT) && containingClass.getName().isSpecial()));
}
