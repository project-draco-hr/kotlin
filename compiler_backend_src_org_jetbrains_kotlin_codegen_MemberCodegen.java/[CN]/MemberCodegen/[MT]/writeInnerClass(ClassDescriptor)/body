{
  DeclarationDescriptor containing=innerClass.getContainingDeclaration();
  String outerClassInternalName=containing instanceof ClassDescriptor ? typeMapper.mapClass((ClassDescriptor)containing).getInternalName() : null;
  String innerName=isClassObject(innerClass) ? JvmAbi.CLASS_OBJECT_CLASS_NAME : innerClass.getName().isSpecial() ? null : innerClass.getName().asString();
  String innerClassInternalName=typeMapper.mapClass(innerClass).getInternalName();
  v.visitInnerClass(innerClassInternalName,outerClassInternalName,innerName,calculateInnerClassAccessFlags(innerClass));
}
