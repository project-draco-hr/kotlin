{
  Collection<JetClassOrObject> jetClassOrObjects=JetFullClassNameIndex.getInstance().get(classFQName.asString(),project,GlobalSearchScope.allScope(project));
  if (jetClassOrObjects.isEmpty()) {
    return Collections.emptyList();
  }
  return ResolveSessionUtils.getClassDescriptorsByFqName(analyzer,classFQName);
}
