{
  Assert.assertEquals(statementToKotlin("try {" + "} catch (Exception e) {" + "    println(1);"+ "} catch (IOException e) {"+ "    println(0);"+ "} finally {}"),"try\n" + "{\n" + "}\n"+ "catch (e : Exception?) {\n"+ "println(1)\n"+ "}\n"+ "catch (e : IOException?) {\n"+ "println(0)\n"+ "}\n"+ "finally\n"+ "{\n"+ "}");
}
