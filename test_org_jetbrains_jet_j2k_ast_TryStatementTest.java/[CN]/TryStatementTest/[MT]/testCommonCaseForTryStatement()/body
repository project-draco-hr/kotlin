{
  Assert.assertEquals(statementToKotlin("try {" + "    callMethod(params);" + "} catch (Exception e) {"+ "    println(1);"+ "} catch (IOException e) {"+ "    println(0);"+ "} finally {"+ "    println(3);"+ "}"),"try\n" + "{\n" + "callMethod(params)\n"+ "}\n"+ "catch (e : Exception?) {\n"+ "println(1)\n"+ "}\n"+ "catch (e : IOException?) {\n"+ "println(0)\n"+ "}\n"+ "finally\n"+ "{\n"+ "println(3)\n"+ "}");
}
