{
switch (value) {
case 0:
    return BYTE;
case 1:
  return CHAR;
case 2:
return SHORT;
case 3:
return INT;
case 4:
return LONG;
case 5:
return FLOAT;
case 6:
return DOUBLE;
case 7:
return BOOLEAN;
case 8:
return STRING;
case 9:
return CLASS;
case 10:
return ENUM;
case 11:
return ANNOTATION;
case 12:
return ARRAY;
default :
return null;
}
}
