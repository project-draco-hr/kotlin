{
  for (int i=0; i < getArgumentCount(); i++) {
    if (!getArgument(i).isInitialized()) {
      return false;
    }
  }
  if (hasFlexibleUpperBound()) {
    if (!getFlexibleUpperBound().isInitialized()) {
      return false;
    }
  }
  if (!extensionsAreInitialized()) {
    return false;
  }
  return true;
}
