{
  Pair<DoubleColonLHS,OverloadResolutionResults<?>> pair=doubleColonExpressionResolver.resolveCallableReference(callableReferenceExpression,ExpressionTypingContext.newContext(context),SHAPE_FUNCTION_ARGUMENTS);
  DoubleColonLHS lhs=pair.getFirst();
  KotlinType receiverType=lhs != null ? lhs.getType() : null;
  return CallableReferencesResolutionUtilsKt.getResolvedCallableReferenceShapeType(callableReferenceExpression,receiverType,pair.getSecond(),context,expectedTypeIsUnknown,reflectionTypes,builtIns,functionPlaceholders);
}
