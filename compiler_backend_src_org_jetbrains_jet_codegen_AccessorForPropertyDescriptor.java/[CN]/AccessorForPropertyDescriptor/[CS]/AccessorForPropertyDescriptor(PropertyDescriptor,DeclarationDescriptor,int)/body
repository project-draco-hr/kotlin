{
  super(containingDeclaration,Collections.<AnnotationDescriptor>emptyList(),Modality.FINAL,Visibilities.LOCAL,pd.isVar(),Name.identifier(pd.getName() + "$b$" + index),Kind.DECLARATION);
  boolean isStaticProperty=AsmUtil.isPropertyWithBackingFieldInOuterClass(pd) && !AsmUtil.isClassObjectWithBackingFieldsInOuter(containingDeclaration);
  JetType receiverType=!isStaticProperty ? DescriptorUtils.getReceiverParameterType(pd.getReceiverParameter()) : null;
  setType(pd.getType(),Collections.<TypeParameterDescriptorImpl>emptyList(),isStaticProperty ? null : pd.getExpectedThisObject(),receiverType);
  initialize(new Getter(this),new Setter(this));
}
