{
  if (textStartsWith != null) {
    Assert.assertTrue("Message should start with:\n" + textStartsWith + "\nBut it is:\n"+ other.message,other.message.startsWith(textStartsWith));
    return;
  }
  if (textMatchesRegexp != null) {
    Assert.assertTrue("Message should match regexp:\n" + textMatchesRegexp + "\nBut it is:\n"+ other.message,other.message.matches(textMatchesRegexp));
    return;
  }
  Assert.assertEquals(other.message,this.message);
}
