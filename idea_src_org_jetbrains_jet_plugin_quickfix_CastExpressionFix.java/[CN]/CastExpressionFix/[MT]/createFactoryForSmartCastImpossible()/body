{
  return new JetSingleIntentionActionFactory(){
    @Nullable @Override public IntentionAction createAction(    Diagnostic diagnostic){
      DiagnosticWithParameters2<JetExpression,JetType,String> diagnosticWithParameters=Errors.SMARTCAST_IMPOSSIBLE.cast(diagnostic);
      return new CastExpressionFix(diagnosticWithParameters.getPsiElement(),diagnosticWithParameters.getA());
    }
  }
;
}
