{
  if (!IncrementalCompilation.ENABLED) {
    return null;
  }
  for (  PackageFragmentDescriptor anotherFragment : packageFragment.getContainingDeclaration().getPackageFragmentProvider().getPackageFragments(packageFragment.getFqName())) {
    if (anotherFragment.getMemberScope() instanceof LazyJavaPackageFragmentScope) {
      return anotherFragment;
    }
  }
  return null;
}
