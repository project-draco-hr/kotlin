{
  if (!IncrementalCompilation.ENABLED) {
    return null;
  }
  for (  PackageFragmentDescriptor fragment : state.getModule().getPackageFragmentProvider().getPackageFragments(fqName)) {
    if (fragment instanceof IncrementalPackageFragmentProvider.IncrementalPackageFragment && ((IncrementalPackageFragmentProvider.IncrementalPackageFragment)fragment).getModuleId().equals(state.getModuleId())) {
      return fragment;
    }
  }
  return null;
}
