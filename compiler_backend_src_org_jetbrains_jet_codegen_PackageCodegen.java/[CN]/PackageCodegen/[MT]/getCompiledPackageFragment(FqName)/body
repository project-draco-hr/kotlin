{
  if (!IncrementalCompilation.ENABLED) {
    return null;
  }
  for (  PackageFragmentDescriptor anotherFragment : state.getModule().getPackageFragmentProvider().getPackageFragments(fqName)) {
    if (anotherFragment instanceof IncrementalPackageFragmentProvider.IncrementalPackageFragment) {
      return anotherFragment;
    }
  }
  return null;
}
