{
  return new Computable<HierarchyTreeStructure>(){
    @Override public HierarchyTreeStructure compute(){
      return new KotlinCallerMethodsTreeStructure(getProject(),getElementAtCaret(LanguageCallHierarchy.INSTANCE.forLanguage(getLanguage())),HierarchyBrowserBaseEx.SCOPE_PROJECT);
    }
  }
;
}
