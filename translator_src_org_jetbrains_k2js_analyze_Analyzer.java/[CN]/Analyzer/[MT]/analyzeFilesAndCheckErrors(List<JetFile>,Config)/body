{
  final List<JetFile> jsLibFiles=config.getLibFiles();
  List<JetFile> allFiles=withJsLibAdded(files,config);
  BindingContext bindingContext=AnalyzingUtils.analyzeFiles(config.getProject(),JsConfiguration.jsLibConfiguration(config.getProject()),allFiles,notLibFiles(jsLibFiles),JetControlFlowDataTraceFactory.EMPTY);
  checkForErrors(allFiles,bindingContext);
  return bindingContext;
}
