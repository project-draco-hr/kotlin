{
  InjectorForTopDownAnalyzer tdaInjectorForLazy=createInjectorForTDA(environment);
  tdaInjectorForLazy.getTopDownAnalyzer().analyzeFiles(Collections.singletonList(JetPsiFactory.createFile(environment.getProject(),"")),Collections.<AnalyzerScriptParameter>emptyList());
  return tdaInjectorForLazy;
}
