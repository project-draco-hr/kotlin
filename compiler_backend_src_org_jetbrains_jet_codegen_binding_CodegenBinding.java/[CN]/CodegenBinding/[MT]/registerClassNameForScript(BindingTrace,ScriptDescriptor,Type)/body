{
  ClassDescriptorImpl classDescriptor=new ClassDescriptorImpl(scriptDescriptor,Name.special("<script-" + asmType.getInternalName() + ">"),Modality.FINAL,Collections.singleton(KotlinBuiltIns.getInstance().getAnyType()),SourceElement.NO_SOURCE);
  classDescriptor.initialize(JetScope.EMPTY,Collections.<ConstructorDescriptor>emptySet(),null);
  recordClosure(bindingTrace,null,classDescriptor,null,asmType);
  bindingTrace.record(CLASS_FOR_SCRIPT,scriptDescriptor,classDescriptor);
}
