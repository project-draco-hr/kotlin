{
  ClassDescriptor toClass=(ClassDescriptor)overridden.getContainingDeclaration();
  PropertyGetterDescriptor getter=delegate.getGetter();
  if (getter != null) {
    functionCodegen.genDelegate(getter,toClass,field,typeMapper.mapSignature(getter),typeMapper.mapSignature(overridden.getGetter().getOriginal()));
  }
  PropertySetterDescriptor setter=delegate.getSetter();
  if (setter != null) {
    functionCodegen.genDelegate(setter,toClass,field,typeMapper.mapSignature(setter),typeMapper.mapSignature(overridden.getSetter().getOriginal()));
  }
}
