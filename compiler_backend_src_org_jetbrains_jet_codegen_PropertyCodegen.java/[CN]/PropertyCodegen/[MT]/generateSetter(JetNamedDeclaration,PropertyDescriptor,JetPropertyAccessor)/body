{
  if (setter != null && setter.getBodyExpression() != null || isExternallyAccessible(propertyDescriptor) && propertyDescriptor.isVar()) {
    JvmPropertyAccessorSignature signature=typeMapper.mapSetterSignature(propertyDescriptor,kind);
    PropertySetterDescriptor setterDescriptor=propertyDescriptor.getSetter();
    setterDescriptor=setterDescriptor != null ? setterDescriptor : DescriptorResolver.createDefaultSetter(propertyDescriptor);
    functionCodegen.generateMethod(setter != null ? setter : p,signature.getJvmMethodSignature(),true,signature.getPropertyTypeKotlinSignature(),setterDescriptor);
  }
}
