{
  InjectorForBodyResolve bodyResolve=new InjectorForBodyResolve(file.getProject(),createParameters(resolveSession),trace,resolveSession.getModuleDescriptor(),getAdditionalCheckerProvider(file));
  BodyResolver resolver=bodyResolve.getBodyResolver();
  resolver.getExpressionTypingServices().setFilter(filter);
  return resolver;
}
