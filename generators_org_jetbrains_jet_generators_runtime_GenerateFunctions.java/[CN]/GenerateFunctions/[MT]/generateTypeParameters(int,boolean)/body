{
  out.print("<");
  if (kind.hasReceiverParameter) {
    if (kotlinVariance)     out.print("in ");
    out.print("T, ");
  }
  for (int j=1; j <= i; j++) {
    if (kotlinVariance)     out.print("in ");
    out.print("P" + j + ", ");
  }
  if (kotlinVariance)   out.print("out ");
  out.print("R>");
}
