{
  ASTNode node=getNode().getFirstChildNode();
  while (node != null) {
    IElementType tt=node.getElementType();
    if (tt == KtTokens.COLON)     break;
    if (tt == KtNodeTypes.TYPE_REFERENCE) {
      return (KtTypeReference)node.getPsi();
    }
    node=node.getTreeNext();
  }
  return null;
}
