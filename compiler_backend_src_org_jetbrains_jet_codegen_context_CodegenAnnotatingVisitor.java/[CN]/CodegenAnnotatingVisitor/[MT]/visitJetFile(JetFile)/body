{
  if (file.isScript()) {
    final ClassDescriptor classDescriptor=bindingContext.get(CLASS_FOR_FUNCTION,bindingContext.get(SCRIPT,file.getScript()));
    classStack.push(classDescriptor);
    nameStack.push(classNameForScriptPsi(bindingContext,file.getScript()).getInternalName());
  }
 else {
    nameStack.push(JetPsiUtil.getFQName(file).getFqName().replace('.','/'));
  }
  file.acceptChildren(this);
  nameStack.pop();
  if (file.isScript()) {
    classStack.pop();
  }
}
