{
  String fqName=NamespaceCodegen.getJVMClassName(JetPsiUtil.getFQName(myFile),true).replace("/",".");
  try {
    return createClassLoader(state).loadClass(fqName);
  }
 catch (  ClassNotFoundException e) {
    e.printStackTrace();
  }
catch (  MalformedURLException e) {
    e.printStackTrace();
  }
  return null;
}
