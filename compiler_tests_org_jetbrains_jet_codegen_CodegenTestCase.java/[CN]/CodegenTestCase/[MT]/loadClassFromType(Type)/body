{
  try {
switch (type.getSort()) {
case Type.OBJECT:
      return Class.forName(type.getClassName());
case Type.INT:
    return int.class;
case Type.LONG:
  return long.class;
default :
if (type.getDescriptor().equals("[Ljava/lang/String;")) {
  return String[].class;
}
throw new IllegalStateException("not implemented: " + type.getDescriptor());
}
}
 catch (Exception e) {
throw new RuntimeException(e);
}
}
