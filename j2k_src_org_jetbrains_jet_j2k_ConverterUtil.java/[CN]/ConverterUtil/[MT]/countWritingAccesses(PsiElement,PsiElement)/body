{
  int counter=0;
  if (container != null) {
    ReferenceCollector visitor=new ReferenceCollector();
    container.accept(visitor);
    for (    PsiReferenceExpression e : visitor.getCollectedReferences())     if (e.isReferenceTo(element) && PsiUtil.isAccessedForWriting(e))     counter++;
  }
  return counter;
}
