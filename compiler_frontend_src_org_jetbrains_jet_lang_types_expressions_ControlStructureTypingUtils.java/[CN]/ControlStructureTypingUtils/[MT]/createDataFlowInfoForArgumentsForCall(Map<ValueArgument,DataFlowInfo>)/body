{
  return new MutableDataFlowInfoForArguments(){
    private DataFlowInfo initialDataFlowInfo;
    @Override public void setInitialDataFlowInfo(    @NotNull DataFlowInfo dataFlowInfo){
      this.initialDataFlowInfo=dataFlowInfo;
    }
    @Override public void updateInfo(    @NotNull ValueArgument valueArgument,    @NotNull DataFlowInfo dataFlowInfo){
    }
    @NotNull @Override public DataFlowInfo getInfo(    @NotNull ValueArgument valueArgument){
      return dataFlowInfoForArgumentsMap.get(valueArgument);
    }
    @NotNull @Override public DataFlowInfo getResultInfo(){
      return initialDataFlowInfo;
    }
  }
;
}
