{
  return getLibraryRootsWithAbiIncompatibleVersion(project,KotlinAbiVersionIndex.INSTANCE$,new Function1<Module,Boolean>(){
    @Override public Boolean invoke(    @Nullable Module module){
      return module != null && ProjectStructureUtil.isJavaKotlinModule(module);
    }
  }
,new Function1<Integer,Boolean>(){
    @Override public Boolean invoke(    Integer abiVersion){
      return !AbiVersionUtil.isAbiVersionCompatible(abiVersion);
    }
  }
);
}
