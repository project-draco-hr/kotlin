{
switch (kind) {
case CLASS:
    sb.append("class");
  break;
case TRAIT:
sb.append("trait");
break;
case OBJECT:
sb.append("object");
break;
case ANNOTATION_CLASS:
sb.append("annotation class");
break;
case ENUM_CLASS:
sb.append("enum class");
break;
case ENUM_ENTRY:
sb.append("enum entry");
break;
default :
throw new IllegalStateException("unknown class kind: " + kind);
}
}
