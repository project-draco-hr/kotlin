{
  if (arguments.size() == 1) {
    if (receiver != null) {
      receiver.put(expectedType,v);
    }
    codegen.gen(arguments.get(0),expectedType);
  }
 else {
    codegen.gen(arguments.get(0),expectedType);
    codegen.gen(arguments.get(1),expectedType);
  }
  v.visitInsn(expectedType.getOpcode(opcode));
  return StackValue.onStack(expectedType);
}
