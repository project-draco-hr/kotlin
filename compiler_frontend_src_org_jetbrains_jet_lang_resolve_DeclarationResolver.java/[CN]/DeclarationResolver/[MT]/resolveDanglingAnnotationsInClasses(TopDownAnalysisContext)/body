{
  for (  Map.Entry<JetClassOrObject,ClassDescriptorWithResolutionScopes> entry : c.getDeclaredClasses().entrySet()) {
    JetClassBody body=entry.getKey().getBody();
    if (body != null) {
      annotationResolver.resolveAnnotationsWithArguments(entry.getValue().getScopeForMemberDeclarationResolution(),body.getDanglingAnnotations(),trace);
    }
  }
}
