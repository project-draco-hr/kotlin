{
  JetExpression left=JetPsiUtil.deparenthesize(expression.getLeft());
  if (left != null) {
    generateInstructions(left);
  }
  JetExpression right=expression.getRight();
  if (right != null) {
    generateInstructions(right);
  }
  mark(expression);
  createUnresolvedCall(expression,left,right);
}
