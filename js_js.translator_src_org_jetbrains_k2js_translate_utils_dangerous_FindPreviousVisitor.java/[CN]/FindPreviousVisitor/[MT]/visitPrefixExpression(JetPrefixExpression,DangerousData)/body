{
  if (data.getDangerousNode() == expression) {
    return null;
  }
  if (!hasDangerous(expression)) {
    addElement(expression,data);
    return null;
  }
 else {
    if (hasDangerous(getBaseExpression(expression))) {
      return null;
    }
 else {
      throw new IllegalStateException();
    }
  }
}
