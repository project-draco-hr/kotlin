{
  PsiElement e=element;
  while (e != null && !(e instanceof KtClassOrObject)) {
    if (e instanceof KtModifierList) {
      e=e.getParent();
      if (e != null) {
        e=e.getParent();
      }
      continue;
    }
    if (e instanceof KtProperty || e instanceof KtNamedFunction) {
      return false;
    }
    e=e.getParent();
  }
  return e != null;
}
