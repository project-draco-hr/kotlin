{
  PsiElement e=element;
  while (e != null && !(e instanceof JetClassOrObject)) {
    if (e instanceof JetModifierList) {
      e=e.getParent();
      if (e != null) {
        e=e.getParent();
      }
      continue;
    }
    if (e instanceof JetProperty || e instanceof JetNamedFunction) {
      return false;
    }
    e=e.getParent();
  }
  return e != null;
}
