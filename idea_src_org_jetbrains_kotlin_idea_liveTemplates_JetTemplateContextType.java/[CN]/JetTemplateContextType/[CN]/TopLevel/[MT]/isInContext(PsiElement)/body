{
  PsiElement e=element;
  while (e != null) {
    if (e instanceof KtModifierList) {
      e=e.getParent();
      if (e != null) {
        e=e.getParent();
      }
      continue;
    }
    if (e instanceof KtProperty || e instanceof KtNamedFunction || e instanceof KtClassOrObject) {
      return false;
    }
    e=e.getParent();
  }
  return true;
}
