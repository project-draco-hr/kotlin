{
  File file=new File(workDir,sourceFileName);
  change(file.getAbsolutePath());
  makeAll().assertSuccessful();
  String outputDirPrefix="out/production/" + moduleName + "/";
  assertDeleted(outputDirPrefix + packageClassFqName.replace('.','/') + ".class",outputDirPrefix + getInternalNameForPackagePartClass(file,packageClassFqName) + ".class");
}
