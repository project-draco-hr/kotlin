{
  if (!(descriptor instanceof SimpleFunctionDescriptor)) {
    return false;
  }
  DeclarationDescriptor containingDeclaration=descriptor.getContainingDeclaration();
  boolean isInvoke=descriptor.getName().asString().equals("invoke") && containingDeclaration instanceof ClassDescriptor && isExactFunctionOrExtensionFunctionType(((ClassDescriptor)containingDeclaration).getDefaultType());
  return isInvoke || ((SimpleFunctionDescriptor)descriptor).isInline();
}
