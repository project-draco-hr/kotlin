{
  NodeList childNodes=element.getChildNodes();
  int childCount=0;
  for (int i=0, n=childNodes.getLength(); i < n; i++) {
    Node child=childNodes.item(i);
    if (child.getNodeType() == Node.ELEMENT_NODE && !REQUEST_FOCUS.equals(child.getNodeName())) {
      childCount++;
    }
  }
  return childCount;
}
