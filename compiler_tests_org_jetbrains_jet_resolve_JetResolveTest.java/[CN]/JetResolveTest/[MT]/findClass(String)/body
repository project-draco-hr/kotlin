{
  Project project=getProject();
  InjectorForJavaSemanticServices injector=new InjectorForJavaSemanticServices(myEnvironment.getCompilerDependencies().getCompilerSpecialMode(),project);
  return injector.getPsiClassFinder().findPsiClass(new FqName(qualifiedName),PsiClassFinder.RuntimeClassesHandleMode.THROW);
}
