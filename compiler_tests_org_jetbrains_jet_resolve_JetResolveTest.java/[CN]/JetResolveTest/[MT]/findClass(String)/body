{
  Project project=getProject();
  InjectorForJavaSemanticServices injector=new InjectorForJavaSemanticServices(CompileCompilerDependenciesTest.compilerDependenciesForTests(CompilerSpecialMode.REGULAR,true),project);
  return injector.getPsiClassFinderForJvm().findPsiClass(new FqName(qualifiedName),PsiClassFinder.RuntimeClassesHandleMode.THROW);
}
