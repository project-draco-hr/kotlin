{
  List<ValueParameterDescriptor> result=new ArrayList<ValueParameterDescriptor>();
  for (int i=0, parametersLength=parameters.length; i < parametersLength; i++) {
    PsiParameter parameter=parameters[i];
    result.add(new ValueParameterDescriptorImpl(i,Collections.<Attribute>emptyList(),parameter.getName(),semanticServices.getTypeTransformer().transform(parameter.getType()),false,parameter.isVarArgs()));
  }
  return result;
}
