{
  List<ValueParameterDescriptor> result=new ArrayList<ValueParameterDescriptor>();
  for (int i=0, parametersLength=parameters.length; i < parametersLength; i++) {
    PsiParameter parameter=parameters[i];
    String name=parameter.getName();
    result.add(new ValueParameterDescriptorImpl(containingDeclaration,i,Collections.<Annotation>emptyList(),name == null ? "p" + i : name,null,semanticServices.getTypeTransformer().transformToType(parameter.getType()),false,parameter.isVarArgs()));
  }
  return result;
}
