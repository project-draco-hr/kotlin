{
  try {
    configureByFileNoComplete(getBeforeFileName());
    setType(type);
    complete(time);
    if (completeItem != null) {
      selectItem(LookupElementBuilder.create(completeItem),'\t');
    }
    checkResultByFile(getAfterFileName());
  }
 catch (  Exception e) {
    throw new AssertionError(e);
  }
}
