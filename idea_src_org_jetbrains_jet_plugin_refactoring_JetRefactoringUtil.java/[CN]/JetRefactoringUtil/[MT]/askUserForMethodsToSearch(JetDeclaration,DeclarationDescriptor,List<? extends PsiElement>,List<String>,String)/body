{
  String superClassesStr="\n" + StringUtil.join(superClasses,"");
  String message=JetBundle.message("x.overrides.y.in.class.list",DescriptorRenderer.COMPACT.render(declarationDescriptor),DescriptorRenderer.SOURCE_CODE_SHORT_NAMES_IN_TYPES.render(declarationDescriptor.getContainingDeclaration()),superClassesStr,JetBundle.message(actionStringKey));
  int exitCode=Messages.showYesNoCancelDialog(declaration.getProject(),message,IdeBundle.message("title.warning"),Messages.getQuestionIcon());
switch (exitCode) {
case Messages.YES:
    return superMethods;
case Messages.NO:
  return Collections.singletonList(declaration);
default :
return Collections.emptyList();
}
}
