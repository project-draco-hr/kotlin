{
  PsiMethod lightMethod=LightClassUtil.getLightClassMethod(function);
  if (lightMethod == null)   return Collections.emptyList();
  return ContainerUtil.map(DeepestSuperMethodsSearch.search(lightMethod).findAll(),new Function<PsiMethod,PsiElement>(){
    @Override public PsiElement fun(    PsiMethod method){
      return toJetDeclarationOrMethod(method);
    }
  }
);
}
