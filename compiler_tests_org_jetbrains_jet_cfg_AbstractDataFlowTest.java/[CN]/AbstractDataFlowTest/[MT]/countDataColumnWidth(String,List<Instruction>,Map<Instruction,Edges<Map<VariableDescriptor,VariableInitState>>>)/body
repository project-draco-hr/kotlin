{
  int maxWidth=0;
  for (  Instruction instruction : instructions) {
    Edges<Map<VariableDescriptor,VariableInitState>> edges=data.get(instruction);
    if (edges == null)     continue;
    int length=dumpEdgesData(prefix,edges).length();
    if (maxWidth < length) {
      maxWidth=length;
    }
  }
  return maxWidth;
}
