{
  JsBinaryOperator operator=isNegated ? JsBinaryOperator.REF_NEQ : JsBinaryOperator.REF_EQ;
  return new JsBinaryOperation(isNegated ? JsBinaryOperator.AND : JsBinaryOperator.OR,new JsBinaryOperation(operator,expressionToCheck,JsLiteral.NULL),new JsBinaryOperation(operator,expressionToCheck,JsLiteral.UNDEFINED));
}
