{
  return template.replace("$RANGE_EXPR_ESCAPED",StringUtil.escapeStringCharacters(rangeExpression)).replace("$RANGE_EXPR",rangeExpression).replace("$LIST_ELEMENTS",expectedListElements).replace("$LIST","list" + number).replace("$RANGE","range" + number).replace("$TYPE",elementType).replace("\n",LineSeparator.getSystemLineSeparator().getSeparatorString());
}
