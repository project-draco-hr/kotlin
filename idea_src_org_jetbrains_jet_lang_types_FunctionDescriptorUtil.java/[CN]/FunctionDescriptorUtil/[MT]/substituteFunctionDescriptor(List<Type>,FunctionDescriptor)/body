{
  if (functionDescriptor.getTypeParameters().isEmpty()) {
    return functionDescriptor;
  }
  return new FunctionDescriptorImpl(ResolveUtil.getJetFunction(functionDescriptor),functionDescriptor,functionDescriptor.getAttributes(),functionDescriptor.getName(),Collections.<TypeParameterDescriptor>emptyList(),getSubstitutedValueParameters(functionDescriptor,typeArguments),getSubstitutedReturnType(functionDescriptor,typeArguments));
}
