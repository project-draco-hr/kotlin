{
  if (suggestions.isEmpty()) {
    return false;
  }
  final Project project=editor.getProject();
  if (project == null) {
    return false;
  }
  String hintText=ShowAutoImportPass.getMessage(suggestions.size() > 1,suggestions.get(0));
  if (!ApplicationManager.getApplication().isUnitTestMode() && !HintManager.getInstance().hasShownHintsThatWillHideByOtherHint()) {
    HintManager.getInstance().showQuestionHint(editor,hintText,element.getTextOffset(),element.getTextRange().getEndOffset(),createAction(project,editor));
  }
  return true;
}
