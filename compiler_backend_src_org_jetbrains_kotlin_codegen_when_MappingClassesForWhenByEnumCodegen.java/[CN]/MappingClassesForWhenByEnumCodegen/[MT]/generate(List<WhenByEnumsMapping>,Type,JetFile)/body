{
  ClassBuilder cb=state.getFactory().newVisitor(JvmDeclarationOrigin.NO_ORIGIN,mappingsClass,srcFile);
  cb.defineClass(srcFile,V1_6,ACC_FINAL | ACC_SYNTHETIC,mappingsClass.getInternalName(),null,OBJECT_TYPE.getInternalName(),ArrayUtil.EMPTY_STRING_ARRAY);
  generateFields(cb,mappings);
  generateInitialization(cb,mappings);
  writeKotlinSyntheticClassAnnotation(cb);
  cb.done();
}
