{
  File javaFilesDir=new File(FileUtil.getTempDirectory(),"java-files");
  try {
    JetTestUtils.mkdirs(javaFilesDir);
  }
 catch (  IOException e) {
    throw ExceptionUtils.rethrow(e);
  }
  return JetCoreEnvironment.createForTests(getTestRootDisposable(),JetTestUtils.compilerConfigurationForTests(ConfigurationKind.JDK_AND_ANNOTATIONS,TestJdkKind.MOCK_JDK,Arrays.asList(JetTestUtils.getAnnotationsJar()),Arrays.asList(javaFilesDir)));
}
