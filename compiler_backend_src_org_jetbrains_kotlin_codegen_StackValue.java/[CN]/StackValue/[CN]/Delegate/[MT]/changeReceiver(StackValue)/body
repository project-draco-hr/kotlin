{
  StackValue newDelegateValue=delegateValue instanceof StackValueWithSimpleReceiver ? ((StackValueWithSimpleReceiver)delegateValue).changeReceiver(newReceiver) : delegateValue;
  StackValue newMetadataValue=metadataValue instanceof StackValueWithSimpleReceiver ? ((StackValueWithSimpleReceiver)metadataValue).changeReceiver(newReceiver) : metadataValue;
  return delegate(type,newDelegateValue,newMetadataValue,getter,setter);
}
