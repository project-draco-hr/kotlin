{
  JetScope scope=javaSemanticServices.getDescriptorResolver().resolveJavaPackageScope(DescriptorUtils.getFQName(namespaceDescriptor).toSafe(),namespaceDescriptor);
  if (scope != null) {
    namespaceMemberScope.importScope(scope);
  }
  delegateConfiguration.extendNamespaceScope(trace,namespaceDescriptor,namespaceMemberScope);
}
