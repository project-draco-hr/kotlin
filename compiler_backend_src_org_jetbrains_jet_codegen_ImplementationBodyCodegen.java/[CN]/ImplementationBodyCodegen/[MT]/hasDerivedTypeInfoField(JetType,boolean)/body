{
  if (!exceptOwn) {
    if (!state.getTypeMapper().isInterface((ClassDescriptor)type.getConstructor().getDeclarationDescriptor()))     if (isParametrizedClass(type))     return true;
  }
  for (  JetType jetType : type.getConstructor().getSupertypes()) {
    if (hasDerivedTypeInfoField(jetType,false))     return true;
  }
  return false;
}
