{
  JsNameRef node=null;
  int endPos=-1;
  int startPos=0;
  do {
    endPos=name.indexOf('.',startPos);
    String part=(endPos == -1 ? name.substring(startPos) : name.substring(startPos,endPos));
    node=newNameRef(node,part);
    startPos=endPos + 1;
  }
 while (endPos != -1);
  return node;
}
