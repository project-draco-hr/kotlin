{
  if (api <= minSdk) {
    return true;
  }
  if (mySeenTargetApi) {
    int target=getTargetApi(element,myFile);
    if (target != -1) {
      if (api <= target) {
        return true;
      }
    }
  }
  if (mySeenSuppress && (IntellijLintUtils.isSuppressed(element,myFile,UNSUPPORTED) || IntellijLintUtils.isSuppressed(element,myFile,INLINED))) {
    return true;
  }
  if (isWithinVersionCheckConditional(element,api)) {
    return true;
  }
  return false;
}
