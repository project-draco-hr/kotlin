{
  this.progress=progress;
  this.analyzeExhaust=exhaust;
  this.files=files;
  this.classBuilderMode=builderFactory.getClassBuilderMode();
  this.injector=new InjectorForJvmCodegen(analyzeExhaust.getBindingContext(),this.files,builtinToJavaTypesMapping,builderFactory.getClassBuilderMode(),this,builderFactory);
}
