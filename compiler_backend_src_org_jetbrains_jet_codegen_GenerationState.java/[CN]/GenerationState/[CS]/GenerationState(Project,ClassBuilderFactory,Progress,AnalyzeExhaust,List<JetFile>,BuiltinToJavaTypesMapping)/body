{
  this.progress=progress;
  this.files=files;
  this.classBuilderMode=builderFactory.getClassBuilderMode();
  final DelegatingBindingTrace trace=new DelegatingBindingTrace(exhaust.getBindingContext());
  bindingContext=trace.getBindingContext();
  this.injector=new InjectorForJvmCodegen(trace,this.files,builtinToJavaTypesMapping,builderFactory.getClassBuilderMode(),this,builderFactory,project);
}
