{
  if (DEBUG_SEARCH) {
    StringBuilder sb=new StringBuilder(200);
    for (int i=offset; i < mData.length; i++) {
      if (mData[i] == 0) {
        break;
      }
      char c=(char)UnsignedBytes.toInt(mData[i]);
      sb.append(c);
    }
    return sb.toString();
  }
 else {
    return "<disabled>";
  }
}
