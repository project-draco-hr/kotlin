{
  NamespaceDescriptorImpl rootNamespace=new NamespaceDescriptorImpl(module,Collections.<AnnotationDescriptor>emptyList(),DescriptorUtils.ROOT_NAMESPACE_NAME);
  rootNamespace.initialize(new WritableScopeImpl(JetScope.EMPTY,rootNamespace,RedeclarationHandler.DO_NOTHING,"members of root namespace"));
  module.setRootNamespace(rootNamespace);
  rootNamespace.getMemberScope().addNamespace(new BuiltinsNamespaceDescriptorImpl(new LockBasedStorageManager(),rootNamespace));
  rootNamespace.getMemberScope().changeLockLevel(WritableScope.LockLevel.READING);
}
