{
  FqName mainClass=null;
  for (  JetFile file : files) {
    if (JetMainDetector.hasMain(file.getDeclarations())) {
      if (mainClass != null) {
        return null;
      }
      FqName fqName=JetPsiUtil.getFQName(file);
      mainClass=PackageClassUtils.getPackageClassFqName(fqName);
    }
  }
  return mainClass;
}
