{
  Iterator<Expression> iterator=node.astArguments().iterator();
  Expression argument=null;
  for (int i=0; i <= parameter; i++) {
    if (!iterator.hasNext()) {
      return;
    }
    argument=iterator.next();
  }
  if (argument == null) {
    return;
  }
  long value=getLongValue(context,argument);
  if (value < min) {
    String message=String.format("Value will be forced up to %d as of Android 5.1; " + "don't rely on this to be exact",min);
    context.report(ISSUE,argument,context.getLocation(argument),message);
  }
}
