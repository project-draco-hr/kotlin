{
  Project project=externalProject;
  if (project == null) {
    project=getProject(files);
  }
  final List<JetFile> jsLibFiles=getJsSupportStdLib(project);
  List<JetFile> allFiles=allFiles(files,jsLibFiles);
  return AnalyzingUtils.analyzeFiles(project,JsConfiguration.jsLibConfiguration(project),allFiles,notLibFiles(jsLibFiles),JetControlFlowDataTraceFactory.EMPTY);
}
