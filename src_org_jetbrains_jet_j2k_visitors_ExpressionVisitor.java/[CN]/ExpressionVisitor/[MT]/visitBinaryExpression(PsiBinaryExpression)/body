{
  super.visitBinaryExpression(expression);
  if (expression.getOperationSign().getTokenType() == JavaTokenType.GTGTGT) {
    myResult=new DummyMethodCallExpression(getConverter().expressionToExpression(expression.getLOperand()),"ushr",getConverter().expressionToExpression(expression.getROperand()));
  }
 else {
    myResult=new BinaryExpression(getConverter().expressionToExpression(expression.getLOperand(),expression.getType()),getConverter().expressionToExpression(expression.getROperand(),expression.getType()),getOperatorString(expression.getOperationSign().getTokenType()));
  }
}
