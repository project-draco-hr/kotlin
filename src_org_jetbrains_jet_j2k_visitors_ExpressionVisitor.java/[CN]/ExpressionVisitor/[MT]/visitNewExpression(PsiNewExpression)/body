{
  super.visitNewExpression(expression);
  if (expression.getArrayInitializer() != null)   myResult=createNewEmptyArray(expression);
 else   if (expression.getArrayDimensions().length > 0) {
    myResult=createNewNonEmptyArray(expression);
  }
 else {
    myResult=createNewClassExpression(expression);
  }
}
