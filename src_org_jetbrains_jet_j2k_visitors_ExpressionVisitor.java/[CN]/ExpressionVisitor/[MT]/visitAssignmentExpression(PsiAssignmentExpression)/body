{
  super.visitAssignmentExpression(expression);
  final IElementType tokenType=expression.getOperationSign().getTokenType();
  if (tokenType == JavaTokenType.GTGTGTEQ) {
    myResult=new AssignmentExpression(expressionToExpression(expression.getLExpression()),new DummyMethodCallExpression(expressionToExpression(expression.getLExpression()),"cyclicShiftRight",expressionToExpression(expression.getRExpression())),"=");
  }
 else {
    String secondOp="";
    if (tokenType == JavaTokenType.GTGTEQ)     secondOp="shr";
    if (tokenType == JavaTokenType.LTLTEQ)     secondOp="shl";
    if (tokenType == JavaTokenType.XOREQ)     secondOp="xor";
    if (tokenType == JavaTokenType.ANDEQ)     secondOp="and";
    if (tokenType == JavaTokenType.OREQ)     secondOp="or";
    if (!secondOp.equals(""))     myResult=new AssignmentExpression(expressionToExpression(expression.getLExpression()),new BinaryExpression(expressionToExpression(expression.getLExpression()),expressionToExpression(expression.getRExpression()),secondOp),"=");
 else     myResult=new AssignmentExpression(expressionToExpression(expression.getLExpression()),expressionToExpression(expression.getRExpression()),expression.getOperationSign().getText());
  }
}
