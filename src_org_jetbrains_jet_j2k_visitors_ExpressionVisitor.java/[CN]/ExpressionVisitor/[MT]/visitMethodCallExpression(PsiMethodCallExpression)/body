{
  super.visitMethodCallExpression(expression);
  if (!SuperVisitor.isSuper(expression.getMethodExpression()) || !isInsidePrimaryConstructor(expression)) {
    myResult=new MethodCallExpression(getConverter().expressionToExpression(expression.getMethodExpression()),getConverter().argumentsToExpressionList(expression),getConverter().typesToTypeList(expression.getTypeArguments()),getConverter().typeToType(expression.getType()).getNullable());
  }
}
