{
  super.visitMethodCallExpression(expression);
  if (!SuperVisitor.isSuper(expression.getMethodExpression()) || !isInsidePrimaryConstructor(expression)) {
    myResult=new MethodCallExpression(getConverter().expressionToExpression(expression.getMethodExpression()),getConverter().expressionsToExpressionList(expression.getArgumentList().getExpressions()),getConverter().createConversions(expression),getConverter().typeToType(expression.getType()).getNullable(),getConverter().typesToTypeList(expression.getTypeArguments()));
  }
}
