{
  if (stackValue instanceof StackValueWithoutReceiver) {
    return stackValue;
  }
 else {
    if (stackValue instanceof StackValueWithSimpleReceiver) {
      return new Delegated(stackValue.type,(StackValueWithSimpleReceiver)stackValue,new ComplexReceiver((StackValueWithSimpleReceiver)stackValue,operations));
    }
 else {
      throw new UnsupportedOperationException();
    }
  }
}
