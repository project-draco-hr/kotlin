{
  if (getter == null) {
    v.visitFieldInsn(isStatic ? GETSTATIC : GETFIELD,methodOwner.getInternalName(),descriptor.getName().getName(),this.type.getDescriptor());
    genNotNullAssertionForField(v,state,descriptor);
  }
 else {
    v.visitMethodInsn(getterInvokeOpcode,methodOwner.getInternalName(),getter.getName(),getter.getDescriptor());
  }
  coerceTo(type,v);
}
