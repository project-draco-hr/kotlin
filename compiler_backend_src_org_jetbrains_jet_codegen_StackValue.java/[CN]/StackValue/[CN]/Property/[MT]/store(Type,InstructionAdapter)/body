{
  coerceFrom(topOfStackType,v);
  if (setter == null) {
    v.visitFieldInsn(isStatic ? PUTSTATIC : PUTFIELD,methodOwner.getInternalName(),descriptor.getName().getName(),this.type.getDescriptor());
  }
 else {
    v.visitMethodInsn(setterInvokeOpcode,methodOwner.getInternalName(),setter.getName(),setter.getDescriptor());
  }
}
