{
  if (call == null)   return true;
  for (  TypeParameterDescriptor typeParameterDescriptor : call.getResultingDescriptor().getTypeParameters()) {
    if (typeParameterDescriptor.isReified())     return false;
  }
  if (call.getResultingDescriptor().getValueParameters().size() != 1)   return false;
  if (codegen.typeMapper.mapType(call.getResultingDescriptor().getValueParameters().get(0).getOutType()).getSize() != 1)   return false;
  if (call.getThisObject().exists()) {
    if (call.getReceiverArgument().exists())     return false;
  }
 else {
    if (codegen.typeMapper.mapType(call.getResultingDescriptor().getReceiverParameter().getType()).getSize() != 1)     return false;
  }
  return true;
}
