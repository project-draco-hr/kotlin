{
  if (!withReceiver) {
    super.dup(v,withReceiver);
  }
 else {
    int receiverSize=hasReceiver(false) && hasReceiver(true) ? receiverSize() : 0;
switch (receiverSize) {
case 0:
      AsmUtil.dup(v,type);
    break;
case 1:
  if (type.getSize() == 2) {
    v.dup2X1();
  }
 else {
    v.dupX1();
  }
break;
case 2:
if (type.getSize() == 2) {
v.dup2X2();
}
 else {
v.dupX2();
}
break;
case -1:
throw new UnsupportedOperationException();
}
}
}
