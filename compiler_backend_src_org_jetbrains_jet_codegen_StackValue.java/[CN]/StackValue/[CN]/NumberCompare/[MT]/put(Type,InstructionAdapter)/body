{
  if (type == Type.VOID_TYPE) {
    return;
  }
  if (type.equals(JetTypeMapper.TUPLE0_TYPE)) {
    putTuple0Instance(v);
    return;
  }
  if (type != Type.BOOLEAN_TYPE) {
    throw new UnsupportedOperationException("don't know how to put a compare as a non-boolean type " + type);
  }
  putAsBoolean(v);
}
