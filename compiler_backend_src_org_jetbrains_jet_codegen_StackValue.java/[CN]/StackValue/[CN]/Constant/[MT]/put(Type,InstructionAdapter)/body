{
  if (value instanceof Integer) {
    v.iconst((Integer)value);
  }
 else   if (value instanceof Long) {
    v.lconst((Long)value);
  }
 else   if (value instanceof Float) {
    v.fconst((Float)value);
  }
 else   if (value instanceof Double) {
    v.dconst((Double)value);
  }
 else {
    v.aconst(value);
  }
  if (value != null) {
    coerceTo(type,v);
  }
}
