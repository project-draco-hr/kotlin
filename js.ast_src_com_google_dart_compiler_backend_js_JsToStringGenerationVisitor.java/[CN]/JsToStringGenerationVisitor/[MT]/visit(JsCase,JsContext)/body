{
  _case();
  _space();
  accept(x.getCaseExpr());
  _colon();
  _newlineOpt();
  indent();
  for (  Object element : x.getStmts()) {
    JsStatement stmt=(JsStatement)element;
    needSemi=true;
    accept(stmt);
    if (needSemi) {
      _semi();
    }
    _newlineOpt();
  }
  outdent();
  needSemi=false;
  return false;
}
