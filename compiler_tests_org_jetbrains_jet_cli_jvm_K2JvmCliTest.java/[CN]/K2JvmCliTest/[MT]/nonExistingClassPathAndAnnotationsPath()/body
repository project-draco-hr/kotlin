{
  String[] args={"-src","compiler/testData/cli/simple.kt","-classpath","not/existing/path","-annotations","yet/another/not/existing/path","-output",tmpdir.getTmpDir().getPath()};
  executeCompilerCompareOutputJVM(args);
  Assert.assertTrue(new File(tmpdir.getTmpDir(),PackageClassUtils.getPackageClassName(FqName.ROOT) + ".class").isFile());
