{
  if (s.equals("usages")) {
    options.isUsages=true;
    return true;
  }
  if (s.equals("skipImports")) {
    options.isSkipImportStatements=true;
    return true;
  }
 else   if (s.equals("textOccurrences")) {
    options.isSearchForTextOccurrences=true;
    return true;
  }
  return false;
}
