{
  String assertTestsPresentStr=String.format("JetTestUtils.assertAllTestsPresentInSingleGeneratedClass(this.getClass(), \"%s\", new File(\"%s\"), Pattern.compile(\"%s\"));",generatorClassFqName,JetTestUtils.getFilePath(rootFile),StringUtil.escapeStringCharacters(filenamePattern.pattern()));
  p.println(assertTestsPresentStr);
}
