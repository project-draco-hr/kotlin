{
  final AnalyzeExhaust analyzeExhaust=AnalyzerFacadeForJVM.analyzeFilesWithJavaIntegrationAndCheckForErrors(project,files,Collections.<AnalyzerScriptParameter>emptyList(),Predicates.<PsiFile>alwaysTrue(),BuiltinsScopeExtensionMode.ALL);
  return compileFilesGetGenerationState(project,analyzeExhaust,files);
