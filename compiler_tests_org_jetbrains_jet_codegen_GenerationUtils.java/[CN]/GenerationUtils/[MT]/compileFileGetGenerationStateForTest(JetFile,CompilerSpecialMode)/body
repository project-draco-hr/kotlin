{
  final AnalyzeExhaust analyzeExhaust=AnalyzerFacadeForJVM.analyzeOneFileWithJavaIntegrationAndCheckForErrors(psiFile,CompileCompilerDependenciesTest.compilerDependenciesForTests(compilerSpecialMode,true));
  analyzeExhaust.throwIfError();
  GenerationState state=new GenerationState(psiFile.getProject(),ClassBuilderFactories.binaries(false),analyzeExhaust,Collections.singletonList(psiFile));
  state.compileCorrectFiles(CompilationErrorHandler.THROW_EXCEPTION);
  return state;
}
