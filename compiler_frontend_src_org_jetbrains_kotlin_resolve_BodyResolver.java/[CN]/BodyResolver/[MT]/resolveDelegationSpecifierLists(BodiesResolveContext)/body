{
  for (  Map.Entry<JetClassOrObject,ClassDescriptorWithResolutionScopes> entry : c.getDeclaredClasses().entrySet()) {
    JetClassOrObject classOrObject=entry.getKey();
    ClassDescriptorWithResolutionScopes descriptor=entry.getValue();
    resolveDelegationSpecifierList(c.getOuterDataFlowInfo(),classOrObject,descriptor,descriptor.getUnsubstitutedPrimaryConstructor(),asJetScope(descriptor.getScopeForClassHeaderResolution()),asJetScope(descriptor.getScopeForMemberDeclarationResolution()));
  }
}
