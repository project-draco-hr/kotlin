{
  for (  Map.Entry<KtClassOrObject,ClassDescriptorWithResolutionScopes> entry : c.getDeclaredClasses().entrySet()) {
    KtClassOrObject classOrObject=entry.getKey();
    ClassDescriptorWithResolutionScopes descriptor=entry.getValue();
    resolveDelegationSpecifierList(c.getOuterDataFlowInfo(),classOrObject,descriptor,descriptor.getUnsubstitutedPrimaryConstructor(),descriptor.getScopeForConstructorHeaderResolution(),descriptor.getScopeForMemberDeclarationResolution());
  }
}
