{
  WritableScope parameterScope=new WritableScopeImpl(originalScope,unsubstitutedPrimaryConstructor,RedeclarationHandler.DO_NOTHING,"Scope with value parameters of a constructor");
  for (  ValueParameterDescriptor valueParameterDescriptor : unsubstitutedPrimaryConstructor.getValueParameters()) {
    parameterScope.addVariableDescriptor(valueParameterDescriptor);
  }
  parameterScope.changeLockLevel(WritableScope.LockLevel.READING);
  return parameterScope;
}
