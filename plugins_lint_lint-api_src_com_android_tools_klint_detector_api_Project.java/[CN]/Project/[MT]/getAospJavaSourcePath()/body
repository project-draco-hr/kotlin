{
  List<File> sources=new ArrayList<File>(2);
  File src=new File(mDir,"src");
  if (src.exists()) {
    sources.add(src);
  }
  for (  File dir : getIntermediateDirs()) {
    File classes=new File(dir,"src");
    if (classes.exists()) {
      sources.add(classes);
    }
  }
  if (sources.isEmpty()) {
    mClient.log(null,"Warning: Could not find sources or generated sources for project %1$s",getName());
  }
  return sources;
}
