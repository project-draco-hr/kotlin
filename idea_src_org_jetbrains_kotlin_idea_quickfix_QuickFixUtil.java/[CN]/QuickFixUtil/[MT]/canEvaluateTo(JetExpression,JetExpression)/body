{
  if (parent == null || child == null) {
    return false;
  }
  while (parent != child) {
    if (child.getParent() instanceof JetParenthesizedExpression) {
      child=(JetExpression)child.getParent();
      continue;
    }
    child=getParentIfForBranch(child);
    if (child == null)     return false;
  }
  return true;
}
