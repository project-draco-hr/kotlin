{
  String simpleName=name.substring(name.lastIndexOf('/') + 1);
  ClassDescriptorImpl classDescriptor=new ClassDescriptorImpl(correctContainerForLambda(callableDescriptor,element),Name.special("<closure-" + simpleName + ">"),Modality.FINAL,ClassKind.CLASS,supertypes,KotlinSourceElementKt.toSourceElement(element));
  classDescriptor.initialize(MemberScope.Empty.INSTANCE,Collections.<ConstructorDescriptor>emptySet(),null);
  bindingTrace.record(CLASS_FOR_CALLABLE,callableDescriptor,classDescriptor);
  return classDescriptor;
}
