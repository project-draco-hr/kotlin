{
  List<VirtualFile> roots=new ArrayList<VirtualFile>();
  File alts=getAltHeadersPath();
  if (alts != null) {
    for (    File root : alts.listFiles()) {
      VirtualFile jarRoot=VirtualFileManager.getInstance().findFileByUrl("jar://" + root.getPath() + "!/");
      roots.add(jarRoot);
    }
  }
  return roots;
}
