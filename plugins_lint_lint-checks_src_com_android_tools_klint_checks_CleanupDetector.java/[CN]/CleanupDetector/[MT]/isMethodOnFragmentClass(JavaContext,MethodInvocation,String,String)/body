{
  ResolvedNode resolved=context.resolve(call);
  if (resolved instanceof ResolvedMethod) {
    ResolvedClass containingClass=((ResolvedMethod)resolved).getContainingClass();
    return containingClass.isSubclassOf(fragmentClass,false) || containingClass.isSubclassOf(v4FragmentClass,false);
  }
  return false;
}
