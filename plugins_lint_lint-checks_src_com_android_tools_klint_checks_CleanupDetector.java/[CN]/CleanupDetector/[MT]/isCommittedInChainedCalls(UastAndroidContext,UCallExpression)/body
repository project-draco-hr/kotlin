{
  UElement parent=node.getParent();
  while (parent instanceof UCallExpression) {
    UCallExpression methodInvocation=(UCallExpression)parent;
    if (isTransactionCommitMethodCall(context,methodInvocation) || isShowFragmentMethodCall(context,methodInvocation)) {
      return true;
    }
    parent=parent.getParent();
  }
  return false;
}
