{
  PsiBuilder.Marker marker=mark();
  if (at(AT) && !WHITE_SPACE_OR_COMMENT_BIT_SET.contains(myBuilder.rawLookup(1))) {
    advance();
  }
  if (atSet(MODIFIER_KEYWORDS)) {
    IElementType tt=tt();
    if (tokenConsumer != null) {
      tokenConsumer.consume(tt);
    }
    advance();
    marker.collapse(tt);
    return true;
  }
  marker.rollbackTo();
  return false;
}
