{
  PsiBuilder.Marker body=mark();
  myBuilder.enableNewlines();
  if (expect(LBRACE,"Expecting a class body")) {
    while (!eof()) {
      if (at(RBRACE)) {
        break;
      }
      parseMemberDeclaration();
    }
    expect(RBRACE,"Missing '}");
  }
  myBuilder.restoreNewlinesState();
  body.done(CLASS_BODY);
}
