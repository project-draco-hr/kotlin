{
  if (at(LBRACKET)) {
    return parseAnnotationList(mode);
  }
 else   if (mode.allowShortAnnotations && at(IDENTIFIER)) {
    parseAnnotationEntry();
    return true;
  }
 else   if (at(AT)) {
    if (myBuilder.rawLookup(1) == IDENTIFIER) {
      parseAnnotationEntry();
    }
 else {
      errorAndAdvance("Expected annotation identifier after '@'",1);
    }
    return true;
  }
  return false;
}
