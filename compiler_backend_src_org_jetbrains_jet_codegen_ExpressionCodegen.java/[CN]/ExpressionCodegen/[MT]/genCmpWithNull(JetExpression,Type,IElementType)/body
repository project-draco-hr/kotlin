{
  gen(exp,boxType(expType));
  Label trueLabel=new Label();
  Label afterLabel=new Label();
  if (JetTokens.EQEQ == opToken || JetTokens.EQEQEQ == opToken) {
    v.ifnull(trueLabel);
  }
 else {
    v.ifnonnull(trueLabel);
  }
  v.iconst(0);
  v.goTo(afterLabel);
  v.mark(trueLabel);
  v.iconst(1);
  v.mark(afterLabel);
  return StackValue.onStack(Type.BOOLEAN_TYPE);
}
