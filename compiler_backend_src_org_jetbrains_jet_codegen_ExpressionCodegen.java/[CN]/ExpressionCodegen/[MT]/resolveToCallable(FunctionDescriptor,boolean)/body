{
  final IntrinsicMethod intrinsic=state.getIntrinsics().getIntrinsic(fd);
  if (intrinsic != null) {
    return intrinsic;
  }
  CallableMethod callableMethod;
  if (isCallAsFunctionObject(fd)) {
    SimpleFunctionDescriptor invoke=createInvoke(fd);
    callableMethod=typeMapper.asCallableMethod(invoke);
  }
 else {
    callableMethod=typeMapper.mapToCallableMethod(fd,superCall,isCallInsideSameClassAsDeclared(fd,context),isCallInsideSameModuleAsDeclared(fd,context),OwnerKind.IMPLEMENTATION);
  }
  return callableMethod;
}
