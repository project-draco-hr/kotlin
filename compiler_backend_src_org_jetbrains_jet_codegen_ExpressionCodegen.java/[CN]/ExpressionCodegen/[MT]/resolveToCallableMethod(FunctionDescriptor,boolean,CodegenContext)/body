{
  if (isCallAsFunctionObject(fd)) {
    return typeMapper.asCallableMethod(createInvoke(fd));
  }
 else {
    SimpleFunctionDescriptor originalOfSamAdapter=getOriginalIfSamAdapter(fd);
    return typeMapper.mapToCallableMethod(originalOfSamAdapter != null ? originalOfSamAdapter : fd,superCall,isCallInsideSameClassAsDeclared(fd,context),isCallInsideSameModuleAsDeclared(fd,context),OwnerKind.IMPLEMENTATION);
  }
}
