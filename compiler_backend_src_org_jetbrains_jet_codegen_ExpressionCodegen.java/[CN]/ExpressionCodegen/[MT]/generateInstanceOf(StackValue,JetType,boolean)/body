{
  expressionToGen.put(TYPE_OBJECT,v);
  if (leaveExpressionOnStack) {
    v.dup();
  }
  Type type=boxType(asmType(jetType));
  if (jetType.isNullable()) {
    Label nope=new Label();
    Label end=new Label();
    v.dup();
    v.ifnull(nope);
    v.instanceOf(type);
    v.goTo(end);
    v.mark(nope);
    v.pop();
    v.iconst(1);
    v.mark(end);
  }
 else {
    v.instanceOf(type);
  }
}
