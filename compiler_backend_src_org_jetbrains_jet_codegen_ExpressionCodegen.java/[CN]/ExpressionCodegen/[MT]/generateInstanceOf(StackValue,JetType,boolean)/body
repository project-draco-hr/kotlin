{
  expressionToGen.put(OBJECT_TYPE,v);
  if (leaveExpressionOnStack) {
    v.dup();
  }
  if (jetType.isNullable()) {
    Label nope=new Label();
    Label end=new Label();
    v.dup();
    v.ifnull(nope);
    generateInstanceOfInstruction(jetType);
    v.goTo(end);
    v.mark(nope);
    v.pop();
    v.iconst(1);
    v.mark(end);
  }
 else {
    generateInstanceOfInstruction(jetType);
  }
}
