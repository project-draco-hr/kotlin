{
  try {
    return FileUtil.loadFile(new File(sdkHome,"build.txt")).trim();
  }
 catch (  IOException e) {
    try {
      File compilerJar=PathUtil.getCompilerPath(sdkHome);
      return compilerJar == null ? null : getJarImplementationVersion(compilerJar);
    }
 catch (    IOException e1) {
      return null;
    }
  }
}
