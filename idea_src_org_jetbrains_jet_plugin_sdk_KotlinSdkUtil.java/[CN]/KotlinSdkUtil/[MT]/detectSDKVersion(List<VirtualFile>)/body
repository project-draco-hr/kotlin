{
  for (  final VirtualFile jar : jars) {
    if (jar.getName().equals(KOTLIN_COMPILER_JAR)) {
      final VirtualFile libDir=jar.getParent();
      if (libDir != null) {
        final VirtualFile sdkHomeDir=libDir.getParent();
        if (sdkHomeDir != null) {
          return getSDKVersion(sdkHomeDir.getPath());
        }
      }
    }
  }
  return null;
}
