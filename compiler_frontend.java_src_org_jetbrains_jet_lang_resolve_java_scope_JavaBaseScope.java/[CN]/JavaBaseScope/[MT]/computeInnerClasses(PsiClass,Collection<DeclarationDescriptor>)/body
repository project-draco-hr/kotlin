{
  if (descriptor instanceof JavaNamespaceDescriptor) {
    result.addAll(semanticServices.getDescriptorResolver().resolveInnerClasses(descriptor,psiClass,false));
  }
 else {
    result.addAll(semanticServices.getDescriptorResolver().resolveInnerClasses(descriptor,psiClass,((ClassPsiDeclarationProviderBase)resolverScopeData).isStaticMembers()));
  }
}
