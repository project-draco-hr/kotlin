{
  if (getContainingDeclaration() instanceof JavaNamespaceDescriptor) {
    result.addAll(semanticServices.getDescriptorResolver().resolveInnerClasses(resolverScopeData.getClassOrNamespaceDescriptor(),psiClass,false));
  }
 else {
    result.addAll(semanticServices.getDescriptorResolver().resolveInnerClasses(resolverScopeData.getClassOrNamespaceDescriptor(),psiClass,resolverScopeData.isStaticMembers()));
  }
}
