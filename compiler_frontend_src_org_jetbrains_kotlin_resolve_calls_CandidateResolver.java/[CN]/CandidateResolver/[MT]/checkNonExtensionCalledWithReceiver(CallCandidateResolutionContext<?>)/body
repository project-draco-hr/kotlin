{
  MutableResolvedCall<?> candidateCall=context.candidateCall;
  if (TasksPackage.isSynthesizedInvoke(candidateCall.getCandidateDescriptor()) && !KotlinBuiltIns.isExtensionFunctionType(candidateCall.getDispatchReceiver().getType())) {
    context.tracing.freeFunctionCalledAsExtension(context.trace);
    candidateCall.addStatus(OTHER_ERROR);
  }
}
