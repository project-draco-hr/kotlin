{
  List<PsiReference> references=PsiReferenceService.getService().getReferences(element,PsiReferenceService.Hints.NO_HINTS);
  for (  PsiReference ref : references) {
    if (ReferenceRange.containsOffsetInElement(ref,offsetInElement)) {
      processElement(ref,ref.getElement(),consumer);
    }
  }
  return true;
}
