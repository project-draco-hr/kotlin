{
  registerExecutor(new QueryExecutorBase<PsiReference,SearchParameters>(){
    @Override public void processQuery(    @NotNull final SearchParameters p,    @NotNull Processor<PsiReference> consumer){
      ApplicationManager.getApplication().runReadAction(new Runnable(){
        @Override public void run(){
          p.getOptimizer().searchWord(p.getMethod().getName(),p.getSearchScope(),UsageSearchContext.IN_CODE,true,new ExtensionTextOccurenceProcessor(p));
        }
      }
);
    }
  }
);
}
