{
  super.visitBinaryExpression(expression);
  if (expression.getOperationSign().getTokenType() == JavaTokenType.GTGTGT) {
    myResult=new DummyMethodCallExpression(getConverter().expressionToExpression(expression.getLOperand()),"ushr",getConverter().expressionToExpression(expression.getROperand()));
  }
 else {
    myResult=new BinaryExpression(getConverter().expressionToExpression(expression.getLOperand()),getConverter().expressionToExpression(expression.getROperand()),getOperatorString(expression.getOperationSign().getTokenType()),getConverter().createConversions(expression,PsiType.BOOLEAN));
  }
}
