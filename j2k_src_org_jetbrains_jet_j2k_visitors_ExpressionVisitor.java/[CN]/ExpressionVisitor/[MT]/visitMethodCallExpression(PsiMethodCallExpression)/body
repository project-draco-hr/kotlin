{
  super.visitMethodCallExpression(expression);
  if (!SuperVisitor.isSuper(expression.getMethodExpression()) || !isInsidePrimaryConstructor(expression)) {
    myResult=new MethodCallExpression(expressionToExpression(expression.getMethodExpression()),expressionsToExpressionList(expression.getArgumentList().getExpressions()),createConversions(expression),typeToType(expression.getType()).isNullable(),typesToTypeList(expression.getTypeArguments()));
  }
}
