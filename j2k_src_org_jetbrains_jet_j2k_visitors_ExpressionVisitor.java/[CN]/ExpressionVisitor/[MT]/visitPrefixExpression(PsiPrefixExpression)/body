{
  super.visitPrefixExpression(expression);
  if (expression.getOperationTokenType() == JavaTokenType.TILDE) {
    myResult=new DummyMethodCallExpression(new ParenthesizedExpression(expressionToExpression(expression.getOperand())),"inv",Expression.EMPTY_EXPRESSION);
  }
 else {
    myResult=new PrefixOperator(getOperatorString(expression.getOperationSign().getTokenType()),expressionToExpression(expression.getOperand()));
  }
}
