{
  JetType receiverType=functionDescriptor.getExtensionReceiverParameter() != null ? functionDescriptor.getExtensionReceiverParameter().getType() : null;
  JetType returnType=functionDescriptor.getReturnType();
  if (returnType == null) {
    return null;
  }
  List<JetType> parameters=ContainerUtil.map(functionDescriptor.getValueParameters(),new Function<ValueParameterDescriptor,JetType>(){
    @Override public JetType fun(    ValueParameterDescriptor descriptor){
      return descriptor.getType();
    }
  }
);
  return components.builtIns.getFunctionType(Annotations.EMPTY,receiverType,parameters,returnType);
}
