{
  Collection<JetReturnExpression> result=Lists.newArrayList();
  expression.accept(new JetTreeVisitor<Collection<JetReturnExpression>>(){
    @Override public Void visitReturnExpression(    @NotNull JetReturnExpression expression,    Collection<JetReturnExpression> data){
      data.add(expression);
      return null;
    }
  }
,result);
  return result;
}
