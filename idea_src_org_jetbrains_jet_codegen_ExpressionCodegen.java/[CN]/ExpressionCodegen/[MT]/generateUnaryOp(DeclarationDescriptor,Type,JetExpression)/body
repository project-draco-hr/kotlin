{
  if (op.getName().equals("minus")) {
    gen(operand,asmType);
    v.neg(asmType);
    myStack.push(StackValue.onStack(asmType));
    return true;
  }
 else   if (op.getName().equals("inc") || op.getName().equals("dec")) {
    myStack.push(generateIncrement(op,asmType,operand));
    return true;
  }
  return false;
}
