{
  final StackValue value=outerThisExpressions.get(calleeContainingClass);
  if (value != null) {
    value.put(value.type,v);
  }
 else {
    v.load(0,JetTypeMapper.TYPE_OBJECT);
    if (calleeContainingClass != null && contextType instanceof ClassDescriptor && calleeContainingClass == contextType.getContainingDeclaration()) {
      v.getfield(typeMapper.jvmName((ClassDescriptor)contextType,OwnerKind.IMPLEMENTATION),"this$0",typeMapper.jvmType(calleeContainingClass,OwnerKind.IMPLEMENTATION).getDescriptor());
    }
  }
}
