{
  final StackValue value=outerThisExpressions.get(calleeContainingClass);
  if (value != null) {
    return value;
  }
 else {
    if (calleeContainingClass != null && contextType() instanceof ClassDescriptor && calleeContainingClass == contextType().getContainingDeclaration()) {
      v.load(0,JetTypeMapper.TYPE_OBJECT);
      return StackValue.field(typeMapper.jvmType(calleeContainingClass,OwnerKind.IMPLEMENTATION),typeMapper.jvmName((ClassDescriptor)contextType(),OwnerKind.IMPLEMENTATION),"this$0",false);
    }
 else {
      return thisExpression() != null ? thisExpression() : StackValue.local(0,JetTypeMapper.TYPE_OBJECT);
    }
  }
}
