{
  Label condition=new Label();
  myLoopStarts.push(condition);
  v.mark(condition);
  Label end=new Label();
  myLoopEnds.push(end);
  gen(expression.getCondition());
  Label thenLabel=new Label();
  myStack.pop().condJump(thenLabel,end,v);
  v.mark(thenLabel);
  gen(expression.getBody(),Type.VOID_TYPE);
  v.goTo(condition);
  v.mark(end);
  myLoopEnds.pop();
  myLoopStarts.pop();
}
