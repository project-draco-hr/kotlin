{
  Label condition=new Label();
  myLoopStarts.push(condition);
  v.mark(condition);
  Label end=new Label();
  myLoopEnds.push(end);
  gen(expression.getCondition());
  myStack.pop().condJump(end,true,v);
  gen(expression.getBody(),Type.VOID_TYPE);
  v.goTo(condition);
  v.mark(end);
  myLoopEnds.pop();
  myLoopStarts.pop();
}
