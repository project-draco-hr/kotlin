{
  v.iconst(1);
  leftValue.put(Type.INT_TYPE,v);
  v.dup2();
  gen(rangeExpression.getLeft(),Type.INT_TYPE);
  Label lok=new Label();
  v.ificmpge(lok);
  v.pop();
  v.iconst(0);
  v.mark(lok);
  v.dupX2();
  v.pop();
  gen(rangeExpression.getRight(),Type.INT_TYPE);
  Label rok=new Label();
  v.ificmple(rok);
  v.pop();
  v.iconst(0);
  v.mark(rok);
  v.and(Type.INT_TYPE);
  if (inverted) {
    v.iconst(1);
    v.xor(Type.INT_TYPE);
  }
}
