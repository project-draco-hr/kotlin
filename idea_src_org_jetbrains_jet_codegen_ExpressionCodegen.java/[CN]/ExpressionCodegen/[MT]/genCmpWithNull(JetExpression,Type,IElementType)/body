{
  gen(exp,typeMapper.boxType(expType));
  Label fail=new Label(), end=new Label();
  if (JetTokens.EQEQ == opToken || JetTokens.EQEQEQ == opToken) {
    v.ifnonnull(fail);
    v.iconst(1);
    v.goTo(end);
    v.mark(fail);
    v.iconst(0);
    v.mark(end);
  }
 else {
    v.ifnull(fail);
    v.iconst(1);
    v.goTo(end);
    v.mark(fail);
    v.iconst(0);
    v.mark(end);
  }
  return StackValue.onStack(Type.BOOLEAN_TYPE);
}
