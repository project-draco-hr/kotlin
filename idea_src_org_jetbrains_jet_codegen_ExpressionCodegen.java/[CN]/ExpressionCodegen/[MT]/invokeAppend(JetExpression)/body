{
  Type exprType=expressionType(expr);
  gen(expr,exprType);
  Method appendDescriptor=new Method("append",Type.getObjectType(CLASS_STRING_BUILDER),new Type[]{exprType.getSort() == Type.OBJECT ? Type.getObjectType(CLASS_OBJECT) : exprType});
  v.invokevirtual(CLASS_STRING_BUILDER,"append",appendDescriptor.getDescriptor());
}
