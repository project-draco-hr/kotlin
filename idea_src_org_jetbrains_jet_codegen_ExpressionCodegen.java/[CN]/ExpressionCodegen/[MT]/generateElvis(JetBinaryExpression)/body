{
  final Type exprType=expressionType(expression);
  gen(expression.getLeft(),exprType);
  v.dup();
  Label end=new Label();
  Label ifNull=new Label();
  v.ifnull(ifNull);
  v.goTo(end);
  v.mark(ifNull);
  v.pop();
  gen(expression.getRight(),exprType);
  v.mark(end);
  myStack.push(StackValue.onStack(exprType));
}
