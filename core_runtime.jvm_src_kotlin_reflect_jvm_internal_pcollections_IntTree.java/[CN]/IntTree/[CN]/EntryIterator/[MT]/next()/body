{
  IntTree<V> node=stack.get(0);
  final Entry<Integer,V> result=new SimpleImmutableEntry<Integer,V>(key,node.value);
  if (node.right.size > 0)   gotoMinOf(node.right);
 else   while (true) {
    key-=node.key;
    stack=stack.subList(1);
    if (node.key < 0 || stack.size() == 0)     break;
    node=stack.get(0);
  }
  return result;
}
