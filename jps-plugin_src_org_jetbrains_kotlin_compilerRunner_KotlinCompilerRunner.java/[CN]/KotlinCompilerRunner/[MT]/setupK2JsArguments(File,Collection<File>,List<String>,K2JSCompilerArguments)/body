{
  settings.noStdlib=true;
  settings.freeArgs=ContainerUtil.map(sourceFiles,new Function<File,String>(){
    @Override public String fun(    File file){
      return file.getPath();
    }
  }
);
  settings.outputFile=outputFile.getPath();
