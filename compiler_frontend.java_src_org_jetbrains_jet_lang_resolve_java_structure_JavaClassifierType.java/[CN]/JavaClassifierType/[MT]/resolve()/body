{
  if (resolutionResult == null) {
    PsiClassType.ClassResolveResult result=getPsi().resolveGenerics();
    PsiClass psiClass=result.getElement();
    resolutionResult=new ResolutionResult(psiClass == null ? null : JavaClassifierImpl.create(psiClass),new JavaTypeSubstitutor(result.getSubstitutor()));
  }
}
