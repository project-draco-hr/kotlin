{
  if (checkBounds) {
    return typeParameterDescriptor.getBoundsAsType().getMemberScope();
  }
 else {
    return new LazyScopeAdapter(new LazyValue<JetScope>(){
      @Override protected JetScope compute(){
        return typeParameterDescriptor.getBoundsAsType().getMemberScope();
      }
    }
);
  }
}
