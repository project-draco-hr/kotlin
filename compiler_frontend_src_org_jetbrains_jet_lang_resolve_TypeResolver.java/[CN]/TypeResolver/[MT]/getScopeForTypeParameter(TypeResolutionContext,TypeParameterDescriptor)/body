{
  if (c.checkBounds) {
    return typeParameterDescriptor.getUpperBoundsAsType().getMemberScope();
  }
 else {
    return new LazyScopeAdapter(new NotNullLazyValueImpl<JetScope>(){
      @NotNull @Override protected JetScope doCompute(){
        return typeParameterDescriptor.getUpperBoundsAsType().getMemberScope();
      }
    }
);
  }
}
