{
  if (c.checkBounds) {
    return typeParameterDescriptor.getUpperBoundsAsType().getMemberScope();
  }
 else {
    return new LazyScopeAdapter(NO_LOCKS.createLazyValue(new Computable<JetScope>(){
      @Override public JetScope compute(){
        return typeParameterDescriptor.getUpperBoundsAsType().getMemberScope();
      }
    }
));
  }
}
