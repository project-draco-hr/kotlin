{
  if (c.checkBounds) {
    return typeParameterDescriptor.getUpperBoundsAsType().getMemberScope();
  }
 else {
    return new LazyScopeAdapter(LockBasedStorageManager.NO_LOCKS.createLazyValue(new Function0<JetScope>(){
      @Override public JetScope invoke(){
        return typeParameterDescriptor.getUpperBoundsAsType().getMemberScope();
      }
    }
));
  }
}
