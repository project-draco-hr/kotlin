{
  if (checkBounds) {
    return typeParameterDescriptor.getUpperBoundsAsType().getMemberScope();
  }
 else {
    return new LazyScopeAdapter(new RecursionIntolerantLazyValue<JetScope>(){
      @Override protected JetScope compute(){
        return typeParameterDescriptor.getUpperBoundsAsType().getMemberScope();
      }
    }
);
  }
}
