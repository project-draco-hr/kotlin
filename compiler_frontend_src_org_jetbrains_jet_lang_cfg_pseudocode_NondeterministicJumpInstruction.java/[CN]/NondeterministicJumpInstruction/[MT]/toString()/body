{
  StringBuilder sb=new StringBuilder();
  sb.append("jmp?(");
  for (Iterator<Label> iterator=targetLabels.iterator(); iterator.hasNext(); ) {
    Label targetLabel=iterator.next();
    sb.append(targetLabel.getName());
    if (iterator.hasNext()) {
      sb.append(", ");
    }
  }
  if (inputValue != null) {
    sb.append("|").append(inputValue);
  }
  sb.append(")");
  return sb.toString();
}
