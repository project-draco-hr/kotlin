{
  ApplicationManager.getApplication().assertReadAccessAllowed();
synchronized (declarationAnalysisLock) {
    if (allowIncomplete) {
      if (incompleteJvmTrace != null) {
        return new KotlinDeclarationsCache(){
          @NotNull @Override public BindingContext getBindingContext(){
            return incompleteJvmTrace.getBindingContext();
          }
        }
;
      }
    }
    return CachedValuesManager.getManager(project).getCachedValue(project,KOTLIN_DECLARATIONS_CACHE,new KotlinDeclarationsJvmCacheProvider(),false);
  }
}
