{
  StringBuilder sb=new StringBuilder();
  boolean needsParentheses=left instanceof Complex && ((Complex)left).operator != BinaryOperator.LOGICAL_AND;
  if (needsParentheses) {
    sb.append('(');
  }
  sb.append(left.toString());
  if (needsParentheses) {
    sb.append(')');
  }
  appendOperator(sb,operator);
  needsParentheses=right instanceof Complex && ((Complex)right).operator != BinaryOperator.LOGICAL_AND;
  if (needsParentheses) {
    sb.append('(');
  }
  sb.append(right.toString());
  if (needsParentheses) {
    sb.append(')');
  }
  return sb.toString();
}
