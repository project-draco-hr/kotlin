{
  Type classType=typeMapper.mapClass(context.getContextDescriptor());
  v.defineClass(scriptDeclaration,V1_6,ACC_PUBLIC,classType.getInternalName(),null,"java/lang/Object",ArrayUtil.EMPTY_STRING_ARRAY);
  generateReflectionObjectField(state,classType,v,method("createKotlinClass",K_CLASS_TYPE,getType(Class.class)),JvmAbi.KOTLIN_CLASS_FIELD_NAME,createOrGetClInitCodegen().v);
}
