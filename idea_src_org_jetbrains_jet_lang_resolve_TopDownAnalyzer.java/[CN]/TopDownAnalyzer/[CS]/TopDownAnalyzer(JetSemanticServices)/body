{
  this.semanticServices=semanticServices;
  this.classDescriptorResolver=new ClassDescriptorResolver(semanticServices,new BindingTrace(){
    @Override public void recordExpressionType(    JetExpression expression,    Type type){
      throw new UnsupportedOperationException();
    }
    @Override public void recordResolutionResult(    JetReferenceExpression expression,    DeclarationDescriptor descriptor){
      throw new UnsupportedOperationException();
    }
    @Override public void recordTypeResoltion(    JetTypeReference typeReference,    Type type){
      types.put(typeReference,type);
    }
  }
);
}
