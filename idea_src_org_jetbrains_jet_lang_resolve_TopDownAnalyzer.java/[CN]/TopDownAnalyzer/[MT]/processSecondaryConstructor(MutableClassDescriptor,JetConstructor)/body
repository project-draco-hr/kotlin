{
  ConstructorDescriptor constructorDescriptor=classDescriptorResolver.resolveSecondaryConstructorDescriptor(classDescriptor.getWritableUnsubstitutedMemberScope(),classDescriptor,constructor);
  classDescriptor.addConstructor(constructorDescriptor);
  constructors.put(constructor,constructorDescriptor);
  declaringScopes.put(constructor,classDescriptor.getWritableUnsubstitutedMemberScope());
}
