{
  ConstructorDescriptor constructorDescriptor=classDescriptorResolver.resolveSecondaryConstructorDescriptor(classDescriptor.getScopeForMemberLookup(),classDescriptor,constructor);
  classDescriptor.addConstructor(constructorDescriptor);
  constructors.put(constructor,constructorDescriptor);
  declaringScopes.put(constructor,classDescriptor.getScopeForMemberLookup());
}
