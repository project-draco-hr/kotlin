{
  for (  DeclarationDescriptor member : DescriptorUtils.getAllDescriptors(classDescriptor.getDefaultType().getMemberScope())) {
    if (member instanceof CallableMemberDescriptor) {
      collectMissingImplementations((CallableMemberDescriptor)member,abstractNoImpl,manyImpl,abstractInBaseClassNoImpl,conflictingInterfaceOverrides);
    }
  }
}
