{
  if (returnType.isNullable()) {
    return "null";
  }
 else   if (returnType.equals(builtIns.getIntType()) || returnType.equals(builtIns.getLongType()) || returnType.equals(builtIns.getShortType())|| returnType.equals(builtIns.getByteType())|| returnType.equals(builtIns.getFloatType())|| returnType.equals(builtIns.getDoubleType())) {
    return "0";
  }
 else   if (returnType.equals(builtIns.getBooleanType())) {
    return "false";
  }
  return null;
}
