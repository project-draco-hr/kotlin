{
  if (insn.getOpcode() != Opcodes.INVOKESTATIC)   return false;
  MethodInsnNode node=(MethodInsnNode)insn;
  return isWrapperClassName(node.owner) && "valueOf".equals(node.name) && Type.getMethodDescriptor(Type.getObjectType(node.owner),AsmUtil.unboxType(Type.getObjectType(node.owner))).equals(node.desc);
}
