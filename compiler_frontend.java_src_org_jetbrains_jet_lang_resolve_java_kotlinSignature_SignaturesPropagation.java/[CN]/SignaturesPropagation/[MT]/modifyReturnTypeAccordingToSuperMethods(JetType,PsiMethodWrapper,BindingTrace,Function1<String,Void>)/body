{
  List<JetType> typesFromSuperMethods=ContainerUtil.map(getSuperFunctionsForMethod(method,trace),new Function<FunctionDescriptor,JetType>(){
    @Override public JetType fun(    FunctionDescriptor superFunction){
      return superFunction.getReturnType();
    }
  }
);
  return modifyReturnTypeAccordingToSuperMethods(autoType,typesFromSuperMethods,true,reportError);
}
