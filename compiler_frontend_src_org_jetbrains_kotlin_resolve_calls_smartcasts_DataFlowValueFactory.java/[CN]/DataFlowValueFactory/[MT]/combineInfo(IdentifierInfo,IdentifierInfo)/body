{
  if (selectorInfo.id == null || receiverInfo == NO_IDENTIFIER_INFO) {
    return NO_IDENTIFIER_INFO;
  }
  if (receiverInfo == null || receiverInfo.isPackage) {
    return selectorInfo;
  }
  return createInfo(Pair.create(receiverInfo.id,selectorInfo.id),receiverInfo.kind.isStable() && selectorInfo.kind.isStable() ? DataFlowValue.Kind.STABLE_VALUE : DataFlowValue.Kind.OTHER);
}
