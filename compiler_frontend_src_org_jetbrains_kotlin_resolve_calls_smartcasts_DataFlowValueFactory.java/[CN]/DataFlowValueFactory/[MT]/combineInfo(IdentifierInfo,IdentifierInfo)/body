{
  if (selectorInfo.id == null) {
    return NO_IDENTIFIER_INFO;
  }
  if (receiverInfo == null || receiverInfo == NO_IDENTIFIER_INFO || receiverInfo.isPackage) {
    return selectorInfo;
  }
  return createInfo(Pair.create(receiverInfo.id,selectorInfo.id),receiverInfo.isStable && selectorInfo.isStable,false);
}
