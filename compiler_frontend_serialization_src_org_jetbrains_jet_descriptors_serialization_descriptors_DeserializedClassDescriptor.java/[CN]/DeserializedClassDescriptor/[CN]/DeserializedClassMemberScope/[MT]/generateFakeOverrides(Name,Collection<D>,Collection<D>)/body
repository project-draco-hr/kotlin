{
  OverrideResolver.generateOverridesInFunctionGroup(name,fromSupertypes,new ArrayList<CallableMemberDescriptor>(fromCurrent),classDescriptor,new OverrideResolver.DescriptorSink(){
    @Override public void addToScope(    @NotNull CallableMemberDescriptor fakeOverride){
      fromCurrent.add((D)fakeOverride);
    }
    @Override public void conflict(    @NotNull CallableMemberDescriptor fromSuper,    @NotNull CallableMemberDescriptor fromCurrent){
    }
  }
);
}
