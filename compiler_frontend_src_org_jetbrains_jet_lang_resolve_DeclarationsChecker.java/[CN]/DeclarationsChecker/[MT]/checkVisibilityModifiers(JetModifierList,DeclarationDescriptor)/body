{
  if (modifierList == null)   return;
  DeclarationDescriptor containingDeclaration=descriptor.getContainingDeclaration();
  if (containingDeclaration instanceof NamespaceDescriptor) {
    if (modifierList.hasModifier(JetTokens.PROTECTED_KEYWORD)) {
      trace.report(Errors.PACKAGE_MEMBER_CANNOT_BE_PROTECTED.on(modifierList.getModifierNode(JetTokens.PROTECTED_KEYWORD).getPsi()));
    }
  }
  checkCompatibility(modifierList,Lists.newArrayList(JetTokens.PRIVATE_KEYWORD,JetTokens.PROTECTED_KEYWORD,JetTokens.PUBLIC_KEYWORD,JetTokens.INTERNAL_KEYWORD),Lists.<JetToken>newArrayList(JetTokens.PROTECTED_KEYWORD,JetTokens.INTERNAL_KEYWORD));
}
