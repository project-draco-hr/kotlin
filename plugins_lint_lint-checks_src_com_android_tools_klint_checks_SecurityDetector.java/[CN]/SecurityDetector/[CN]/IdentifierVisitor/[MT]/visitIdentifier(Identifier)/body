{
  if ("MODE_WORLD_WRITEABLE".equals(node.astValue())) {
    Location location=mContext.getLocation(node);
    mContext.report(WORLD_WRITEABLE,node,location,"Using `MODE_WORLD_WRITEABLE` when creating files can be " + "risky, review carefully");
  }
 else   if ("MODE_WORLD_READABLE".equals(node.astValue())) {
    Location location=mContext.getLocation(node);
    mContext.report(WORLD_READABLE,node,location,"Using `MODE_WORLD_READABLE` when creating files can be " + "risky, review carefully");
  }
  return false;
}
