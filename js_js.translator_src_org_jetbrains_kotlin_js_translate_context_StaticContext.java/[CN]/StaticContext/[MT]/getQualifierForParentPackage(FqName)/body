{
  JsNameRef result=null;
  JsNameRef qualifier=null;
  FqName fqName=packageFqName;
  while (true) {
    JsNameRef ref=getNameForPackage(fqName).makeRef();
    if (qualifier == null) {
      result=ref;
    }
 else {
      qualifier.setQualifier(ref);
    }
    qualifier=ref;
    if (fqName.isRoot())     break;
    fqName=fqName.parent();
  }
  return result;
}
