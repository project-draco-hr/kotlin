{
  if (descriptor != null) {
    return ONLY_NAMES_WITH_SHORT_TYPES.render(descriptor);
  }
  String text=navigatablePsiElement.getName();
  if (!StringUtil.isEmpty(text)) {
    return text;
  }
  if (navigatablePsiElement instanceof KtClassInitializer) {
    return "<class initializer>";
  }
  return null;
}
