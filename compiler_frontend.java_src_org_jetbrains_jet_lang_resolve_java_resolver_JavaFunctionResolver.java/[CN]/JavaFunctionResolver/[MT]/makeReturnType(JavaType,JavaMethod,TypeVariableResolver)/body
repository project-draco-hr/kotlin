{
  TypeUsage typeUsage=JavaAnnotationResolver.hasReadonlyAnnotation(method) && !JavaAnnotationResolver.hasMutableAnnotation(method) ? TypeUsage.MEMBER_SIGNATURE_CONTRAVARIANT : TypeUsage.MEMBER_SIGNATURE_COVARIANT;
  JetType transformedType=typeTransformer.transformToType(returnType,typeUsage,typeVariableResolver);
  if (JavaAnnotationResolver.hasNotNullAnnotation(method)) {
    return TypeUtils.makeNotNullable(transformedType);
  }
 else {
    return transformedType;
  }
}
