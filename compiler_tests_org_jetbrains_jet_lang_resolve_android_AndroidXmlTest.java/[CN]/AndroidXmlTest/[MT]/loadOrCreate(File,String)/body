{
  try {
    return new Scanner(file,"UTF-8").useDelimiter("\\A").next();
  }
 catch (  IOException e) {
    file.createNewFile();
    FileWriter fileWriter=new FileWriter(file);
    fileWriter.write(data);
    fileWriter.close();
    fail("Empty expected data, creating from actual");
    return data;
  }
}
