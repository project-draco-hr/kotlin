{
  StringBuilder sb=new StringBuilder("#(");
  for (Iterator<TypeProjection> iterator=type.getArguments().iterator(); iterator.hasNext(); ) {
    TypeProjection argument=iterator.next();
    sb.append(renderType(argument.getType()));
    if (iterator.hasNext()) {
      sb.append(", ");
    }
  }
  sb.append(")");
  if (type.isNullable()) {
    sb.append("?");
  }
  return sb.toString();
}
