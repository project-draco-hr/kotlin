{
  if (clsClass.getUserData(ClsClassImpl.DELEGATE_KEY) != null) {
    return null;
  }
  JetClass jetClass=(JetClass)getJetDeclarationByClsElement(clsClass);
  if (jetClass != null) {
    JetClass sourceClass=JetSourceNavigationHelper.getSourceClass(jetClass);
    if (sourceClass != null) {
      return sourceClass;
    }
  }
  return jetClass;
}
