{
  if (index < paramSize) {
    ParameterInfo info=params.get(index);
    if (info.isSkipped) {
      throw new RuntimeException("Trying to access skipped parameter: " + info.type + " at "+ index);
    }
    return info.getInlinedIndex();
  }
 else {
    return paramShift + actualParams + index;
  }
}
