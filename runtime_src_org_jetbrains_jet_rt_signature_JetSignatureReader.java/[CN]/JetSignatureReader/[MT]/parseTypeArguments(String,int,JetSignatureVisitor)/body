{
  char c;
  while (true) {
switch (c=signature.charAt(pos)) {
case '>':
      return pos;
case '*':
    ++pos;
  v.visitTypeArgument();
break;
case '+':
case '-':
pos=parseType(signature,pos + 1,v.visitTypeArgument(JetSignatureVariance.parseVariance(c)));
break;
default :
pos=parseType(signature,pos,v.visitTypeArgument(JetSignatureVariance.INVARIANT));
break;
}
}
}
