{
  return new ValueParameterDescriptorImpl(containingDeclaration,index,getAnnotations(proto),nameResolver.getName(proto.getName()),typeDeserializer.type(proto.getType()),Flags.DECLARES_DEFAULT_VALUE.get(proto.getFlags()),typeDeserializer.typeOrNull(proto.hasVarargElementType() ? proto.getVarargElementType() : null));
}
