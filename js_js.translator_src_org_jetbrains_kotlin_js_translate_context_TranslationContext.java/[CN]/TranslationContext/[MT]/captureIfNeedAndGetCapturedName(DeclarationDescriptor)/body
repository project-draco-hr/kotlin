{
  if (usageTracker != null) {
    usageTracker.used(descriptor);
    JsName name=getNameForCapturedDescriptor(usageTracker,descriptor);
    if (name != null) {
      JsNameRef result=name.makeRef();
      if (shouldCaptureViaThis()) {
        result.setQualifier(JsLiteral.THIS);
      }
      return result;
    }
  }
  return null;
}
