{
  super.addUsagesOptions(optionsPanel);
  if (!renameCheckbox(optionsPanel,FindBundle.message("find.options.include.overloaded.methods.checkbox"),JetBundle.message("find.options.include.overloaded.methods.checkbox"))) {
    addCheckboxToPanel(JetBundle.message("find.options.include.overloaded.methods.checkbox"),FindSettings.getInstance().isSearchOverloadedMethods(),optionsPanel,false);
  }
}
