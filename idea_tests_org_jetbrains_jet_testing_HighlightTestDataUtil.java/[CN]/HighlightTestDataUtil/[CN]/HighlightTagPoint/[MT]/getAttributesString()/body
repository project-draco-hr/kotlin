{
  if (isStart) {
    if (highlightInfo.getDescription() != null) {
      return String.format("textAttributesKey=\"%s\" descr=%s",highlightInfo.forcedTextAttributesKey,highlightInfo.getDescription());
    }
 else {
      return String.format("textAttributesKey=\"%s\"",getName());
    }
  }
 else {
    return "";
  }
}
