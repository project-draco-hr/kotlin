{
  File javaFilesDir=createJavaFilesDir();
  return KotlinCoreEnvironment.createForTests(getTestRootDisposable(),JetTestUtils.compilerConfigurationForTests(ConfigurationKind.ALL,TestJdkKind.FULL_JDK,Arrays.asList(JetTestUtils.getAnnotationsJar()),Arrays.asList(javaFilesDir)),EnvironmentConfigFiles.JVM_CONFIG_FILES);
