{
  FunctionCodegen codegen=new FunctionCodegen(CodegenContext.STATIC.intoClass((ClassDescriptor)erasedInterfaceFunction.getContainingDeclaration(),OwnerKind.IMPLEMENTATION,state),cv,state,parentCodegen);
  FunctionDescriptor invokeFunction=functionJetType.getMemberScope().getFunctions(Name.identifier("invoke")).iterator().next().getOriginal();
  StackValue functionField=StackValue.field(functionType,ownerType,FUNCTION_FIELD_NAME,false);
  codegen.genDelegate(erasedInterfaceFunction,invokeFunction,functionField);
}
