{
  if (!IncrementalCompilation.isEnabled()) {
    return null;
  }
  for (  PackageFragmentDescriptor fragment : state.getModule().getPackage(fqName).getFragments()) {
    if (fragment instanceof IncrementalPackageFragmentProvider.IncrementalPackageFragment && ((IncrementalPackageFragmentProvider.IncrementalPackageFragment)fragment).getTarget().equals(state.getTargetId())) {
      return (IncrementalPackageFragmentProvider.IncrementalPackageFragment)fragment;
    }
  }
  return null;
}
