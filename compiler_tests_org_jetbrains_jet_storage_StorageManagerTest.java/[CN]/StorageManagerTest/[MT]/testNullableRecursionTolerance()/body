{
class C {
    NullableLazyValue<String> rec=m.createRecursionTolerantNullableLazyValue(new Computable<String>(){
      @Override public String compute(){
        assertEquals(null,rec.invoke());
        return "tolerant!";
      }
    }
,null);
  }
  assertEquals("tolerant!",new C().rec.invoke());
}
