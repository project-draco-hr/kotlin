{
  return TranslatorTestCaseBuilder.suiteForDirectory(TEST_DATA_DIR_PATH + "outputPrefixPostfix/cases/",new TranslatorTestCaseBuilder.NamedTestFactory(){
    @NotNull @Override public Test createTest(    @NotNull String filename){
      OutputPrefixPostfixTest examplesTest=new OutputPrefixPostfixTest(filename);
      examplesTest.setName(filename);
      return examplesTest;
    }
  }
);
}
