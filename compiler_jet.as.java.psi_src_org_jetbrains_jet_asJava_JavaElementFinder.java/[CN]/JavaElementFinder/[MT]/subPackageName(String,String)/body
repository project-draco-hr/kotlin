{
  if (!isInPackage(packageName,packageFQN)) {
    return null;
  }
  int nextDotIndex=packageFQN.indexOf('.',(packageName + ".").length());
  if (nextDotIndex != -1) {
    return packageFQN.substring((packageName + ".").length(),nextDotIndex);
  }
  return packageFQN.substring((packageName + ".").length());
}
