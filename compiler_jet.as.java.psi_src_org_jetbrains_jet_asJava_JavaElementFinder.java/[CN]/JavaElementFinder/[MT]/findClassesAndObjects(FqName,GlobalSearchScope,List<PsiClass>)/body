{
  Collection<JetClassOrObject> classOrObjectDeclarations=lightClassGenerationSupport.findClassOrObjectDeclarations(qualifiedName,scope);
  for (  JetClassOrObject declaration : classOrObjectDeclarations) {
    if (!(declaration instanceof JetEnumEntry)) {
      KotlinLightClass lightClass=KotlinLightClass.create(psiManager,qualifiedName,declaration);
      if (lightClass != null) {
        answer.add(lightClass);
      }
    }
  }
}
