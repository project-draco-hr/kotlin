{
  JetPsiChecker.highlightName(holder,elementToHighlight,namespace ? JetHighlightingColors.NAMESPACE_PROPERTY : JetHighlightingColors.INSTANCE_PROPERTY);
  if (withBackingField) {
    holder.createInfoAnnotation(elementToHighlight,"This property has a backing field").setTextAttributes(JetHighlightingColors.PROPERTY_WITH_BACKING_FIELD);
  }
}
