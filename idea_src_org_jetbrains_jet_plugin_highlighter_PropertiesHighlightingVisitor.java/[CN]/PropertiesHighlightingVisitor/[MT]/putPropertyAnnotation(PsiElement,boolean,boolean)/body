{
  holder.createInfoAnnotation(elementToHighlight,"This property has a backing field").setTextAttributes(withBackingField ? (namespace ? JetHighlightingColors.NAMESPACE_PROPERTY_WITH_BACKING_FIELD : JetHighlightingColors.INSTANCE_PROPERTY_WITH_BACKING_FIELD) : (namespace ? JetHighlightingColors.NAMESPACE_PROPERTY : JetHighlightingColors.INSTANCE_PROPERTY));
}
