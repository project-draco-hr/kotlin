{
  FakeMemberCodegen parentCodegen=new FakeMemberCodegen(codegen.getParentCodegen(),declaration,(FieldOwnerContext)context.getParentContext(),isLambda ? codegen.getParentCodegen().getClassName() : typeMapper.mapOwner(descriptor,false).getInternalName());
  FunctionCodegen.generateMethodBody(adapter,descriptor,context,jvmMethodSignature,new FunctionGenerationStrategy.FunctionDefault(state,descriptor,declaration),parentCodegen);
  return createSMAPWithDefaultMapping(declaration,parentCodegen.getOrCreateSourceMapper().getResultMappings());
}
