{
  Set<Module> modules=Sets.newHashSet();
  for (  KotlinProjectConfigurator configurator : getApplicableConfigurators(project)) {
    for (    Module module : getModulesWithKotlinFiles(project)) {
      if (!configurator.isConfigured(module)) {
        modules.add(module);
      }
    }
  }
  return modules;
}
