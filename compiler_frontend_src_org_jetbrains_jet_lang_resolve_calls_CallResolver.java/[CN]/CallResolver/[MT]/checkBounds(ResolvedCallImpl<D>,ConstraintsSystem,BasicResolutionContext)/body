{
  for (  TypeParameterDescriptor typeParameter : call.getCandidateDescriptor().getTypeParameters()) {
    if (!ConstraintsUtil.checkUpperBoundIsSatisfied(constraintsSystem,typeParameter)) {
      context.trace.report(Errors.TYPE_INFERENCE_UPPER_BOUND_VIOLATED.on(context.call.getCallElement(),InferenceErrorData.create(call.getCandidateDescriptor(),constraintsSystem)));
    }
  }
}
