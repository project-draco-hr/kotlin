{
  String path=virtualFile == null ? "<null>" : virtualFile.getPath();
  LOG.error("Could not generate LightClass for " + fqName + " declared in "+ path+ "\n"+ "built-ins dir URL is "+ KotlinBuiltIns.getBuiltInsDirUrl()+ "\n"+ "System: "+ SystemInfo.OS_NAME+ " "+ SystemInfo.OS_VERSION+ " Java Runtime: "+ SystemInfo.JAVA_RUNTIME_VERSION,cause);
}
