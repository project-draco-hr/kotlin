{
  if (getterAccessorRequired && setterAccessorRequired) {
    return getOrCreateAccessorWithSyntheticGetterAndSetter();
  }
 else   if (getterAccessorRequired && !setterAccessorRequired) {
    if (withSyntheticGetter == null) {
      withSyntheticGetter=new AccessorForPropertyDescriptor(property,containingDeclaration,superCallExpression,nameSuffix,true,false);
    }
    return withSyntheticGetter;
  }
 else   if (!getterAccessorRequired && setterAccessorRequired) {
    if (withSyntheticSetter == null) {
      withSyntheticSetter=new AccessorForPropertyDescriptor(property,containingDeclaration,superCallExpression,nameSuffix,false,true);
    }
    return withSyntheticSetter;
  }
 else {
    return property;
  }
}
