{
  ensureInitialized();
  String path=mProject != null ? mProject.getRelativePath(file) : file.getPath();
  List<String> paths=mSuppressed.get(issue.getId());
  if (paths == null) {
    paths=new ArrayList<String>();
    mSuppressed.put(issue.getId(),paths);
  }
  paths.add(path);
  Collections.sort(paths);
  if (!mBulkEditing) {
    writeConfig();
  }
}
