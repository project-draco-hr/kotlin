{
  ProgressIndicatorProvider.checkCanceled();
  PsiClass aClass=javaFileManager.findClass(qualifiedName,scope);
  if (aClass != null) {
    return aClass;
  }
  for (  PsiElementFinder finder : extensionPsiElementFinders) {
    aClass=finder.findClass(qualifiedName,scope);
    if (aClass != null) {
      return aClass;
    }
  }
  return null;
}
