{
  PsiPackage psiPackage=javaFileManager.findPackage(qualifiedName);
  if (psiPackage != null) {
    return psiPackage;
  }
  for (  PsiElementFinder finder : extensionPsiElementFinders) {
    psiPackage=finder.findPackage(qualifiedName);
    if (psiPackage != null) {
      return psiPackage;
    }
  }
  return psiPackage;
}
