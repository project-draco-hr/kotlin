{
  WritableScopeImpl scope=new WritableScopeImpl(JetScope.EMPTY,this,RedeclarationHandler.DO_NOTHING,"Member Declaration Resolution");
  scope.addLabeledDeclaration(this);
  scope.changeLockLevel(WritableScope.LockLevel.READING);
  return new ChainedScope(this,"ScopeForMemberDeclarationResolution: " + getName(),scope,getScopeForMemberLookup(),getScopeForClassHeaderResolution());
}
