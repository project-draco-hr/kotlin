{
  if (!CodeFragmentUtilPackage.skipVisibilityCheck(referenceExpression.getContainingJetFile()) && !Visibilities.isVisible(descriptor,scopeToCheckVisibility.getContainingDeclaration())) {
    trace.report(INVISIBLE_REFERENCE.on(referenceExpression,descriptor,descriptor.getVisibility(),descriptor.getContainingDeclaration()));
  }
}
