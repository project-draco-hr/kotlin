{
  return CollectionsKt.map(ArraysKt.filterIsInstance(getOccurrences(),JetSimpleNameExpression.class),new Function1<JetSimpleNameExpression,PsiReference>(){
    @Override public PsiReference invoke(    JetSimpleNameExpression expression){
      return ReferenceUtilKt.getMainReference(expression);
    }
  }
);
}
