{
  return CollectionsKt.map(ArraysKt.filterIsInstance(getOccurrences(),KtSimpleNameExpression.class),new Function1<KtSimpleNameExpression,PsiReference>(){
    @Override public PsiReference invoke(    KtSimpleNameExpression expression){
      return ReferenceUtilKt.getMainReference(expression);
    }
  }
);
}
