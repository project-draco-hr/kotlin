{
  return KotlinPackage.map(KotlinPackage.filterIsInstance(getOccurrences(),JetSimpleNameExpression.class),new Function1<JetSimpleNameExpression,PsiReference>(){
    @Override public PsiReference invoke(    JetSimpleNameExpression expression){
      return ReferencesPackage.getMainReference(expression);
    }
  }
);
}
